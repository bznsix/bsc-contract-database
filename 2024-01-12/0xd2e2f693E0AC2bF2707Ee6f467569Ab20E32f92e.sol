pragma solidity ^0.8.19;

import "./libraries/ERC20Burnable.sol";
import "./libraries/Ownable.sol";
import "./libraries/SafeMath.sol";

import "./interfaces/IUniswapV2Factory.sol";
import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IUniswapV2Router02.sol";

interface IDinoBuy {
    function dinoBuyBack() external;
}

contract TokenTracker{
    constructor (address token, uint256 amount) {
        IERC20(token).approve(msg.sender, amount);
    }
}

contract DinoToken is ERC20Burnable, Ownable {
    using SafeMath for uint256;

    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;

    uint256 public buyFeeDao = 2;
    uint256 public buyFeeLp = 3;

    uint256 public sellFeeDao = 3;
    uint256 public sellFeeLp = 5;

    uint256 public sellPreFeeDao = 12;
    uint256 public sellPreFeeLp = 18;

    uint256 public feeDaoCalc;
    uint256 public feeLpCalc;

    uint256 public delLPFee = 50;

    uint256 public commonDiv = 100;
    mapping(address => bool) _excludedFees;
    bool swapIng;

    uint256 public perMintPrice = 5*10**17;// 0.5 per mint
    uint256 public lpRate = 60;
    uint256 public buyRate = 40;

    uint256 public perMintAmount = 120000e18;// per mint amount
    uint256 public maxSupply = 160000000e18; // max supply 160000000 
    uint256 public minted;
    uint256 public mintableRate = 75;

    uint256 public maxTxAmount = 10000e18;// 10000

    address public adminHolder;

    uint256 public totalDestroy;

    IDinoBuy public dinoBuy;

    address public tokenReciever;

    mapping(address => bool) public holder;
    uint256 public totalHolder;

    mapping(address => bool) public automatedMarketMakerPairs;

    bool public enableSwitch = true;

    uint256 public startTime;

    uint256 public aDay = 1 days;// 1 day

    address public dividendPool;
    address public marketPool;

    uint256 private constant MAX = type(uint256).max;

    constructor(address _newOwner) ERC20("Dinosaur Token", "DINO") {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x10ED43C718714eb63d5aA57B78B54704E256024E //bsc network
            //0xD99D1c33F9fC3444f8101754aBC46c52416550D1 //test bsc network
        );
        _transferOwnership(_newOwner);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(_uniswapV2Router.WETH(), address(this));
        _excludedFees[_newOwner] = true;
        _excludedFees[address(this)] = true;
        _setAutomatedMarketMakerPair(address(uniswapV2Pair), true);
        uniswapV2Router = _uniswapV2Router;

        tokenReciever = address(new TokenTracker(_uniswapV2Router.WETH(), MAX));
    }

    bool public minting;
    modifier lockMint() {
        minting = true;
        _;
        minting = false;
    }

    modifier lockTheSwap() {
        swapIng = true;
        _;
        swapIng = false;
    }

    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);

    function isExcludedFromFees(address account) external view returns (bool) {
        return _excludedFees[account];
    }

    function excludedFromFees(address account, bool excluded) external onlyOwner {
        _excludedFees[account] = excluded;
    }

    function setAdminHolder(address _addr) external onlyOwner {
        adminHolder = _addr;
    }

    function setPerMintAmount(uint256 _amount) external onlyOwner {
        perMintAmount = _amount;
    }

    function setDinoBuy(address _addr) external onlyOwner {
        dinoBuy = IDinoBuy(_addr);
        _excludedFees[_addr] = true;
    }

    function setEnableSwitch(bool _flag) external onlyOwner {
        enableSwitch = _flag;
    }

    function setCommonDiv(uint256 _commonDiv) external onlyOwner {
        commonDiv = _commonDiv;
    }

    function setMaxTxAmount(uint256 _amount) external onlyOwner {
        maxTxAmount = _amount;
    }

    function setFee(uint256 _bflp, uint256 _bfdao, uint256 _sflp, uint256 _sfdao) external onlyOwner {
        buyFeeDao = _bfdao;
        buyFeeLp = _bflp;
        sellFeeDao = _sfdao;
        sellFeeLp = _sflp;
    }

    function setDelLpFee(uint256 _delLp) external onlyOwner {
        delLPFee = _delLp;
    }

    function setDividend(address _addr, address _market) external onlyOwner {
        dividendPool = _addr;
        marketPool = _market;
    }

    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        automatedMarketMakerPairs[pair] = value;
        emit SetAutomatedMarketMakerPair(pair, value);
    }

    // burn token amount
    function recordBurn(address _who, uint256 _amount) internal {
        super._burn(_who, _amount);
        totalDestroy += _amount;
    }

    function burn(uint256 amount) public virtual override {
        recordBurn(_msgSender(), amount);
    }

    function burnFrom(address account, uint256 amount) public virtual override {
        super._spendAllowance(account, _msgSender(), amount);
        recordBurn(account, amount);
    }

    function getDay() public view returns (uint) {
        if (startTime > 0 && block.timestamp > startTime){
            return (block.timestamp - startTime) / aDay;
        }else{
            return 0;
        }
    }

    function setAutoLPBurnSettings(uint256 _frequencyInSeconds,uint256 _percent,bool _enabled) external onlyOwner {
        lpBurnFrequency = _frequencyInSeconds;
        percentForLPBurn = _percent;
        lpBurnEnabled = _enabled;
    }

    bool public lpBurnEnabled = false;

    function _isLiquidity(address from) internal view returns(bool isDel){
        if( uniswapV2Pair == address(0)) return(false);
        address token0 = IUniswapV2Pair(uniswapV2Pair).token0();
        (uint r0,,) = IUniswapV2Pair(uniswapV2Pair).getReserves();
        uint bal0 = IERC20(token0).balanceOf(address(uniswapV2Pair));
        if( from == uniswapV2Pair ) {
            if( token0 != address(this) && bal0 < r0 ){
                isDel = r0 - bal0 > 0; 
            }
        }
    }

    event DelLpEvent(address indexed who, uint256 tokenAmount);
    error ErrUnableSwap();
    error ErrLpCannotRemoveNow();

    function _dividendFunds() internal {
        uint256 _bal = IERC20(uniswapV2Router.WETH()).balanceOf(tokenReciever);
        uint256 _total = feeDaoCalc + feeLpCalc;
        if (_bal > 0 && _total > 0) {
            uint256 _fund1 = _bal * feeDaoCalc / _total;
            uint256 _fund2 = _bal - _fund1;
            IERC20(uniswapV2Router.WETH()).transferFrom(tokenReciever, marketPool, _fund1);
            IERC20(uniswapV2Router.WETH()).transferFrom(tokenReciever, dividendPool, _fund2);
            feeDaoCalc = 0;
            feeLpCalc = 0;
        }
    }

    function _transfer(address from, address to, uint256 amount) internal virtual override {
        if (enableSwitch) {
            if (!_excludedFees[from] && !_excludedFees[to]) {
                if (maxSupply > minted) {
                    revert ErrUnableSwap();
                }
                if (startTime == 0 || block.timestamp < startTime){
                    revert ErrUnableSwap();
                }

                uint256 _tokenBal = balanceOf(address(this));
                if( _tokenBal >= maxTxAmount && feeDaoCalc > 0 && feeLpCalc > 0
                    && !swapIng && msg.sender != uniswapV2Pair) {
                    _processSwap(_tokenBal);

                    _dividendFunds();
                }

                uint256 _txFeeDao;
                uint256 _txFeeLp;
                if (to == uniswapV2Pair) {
                    // sell
                    if (block.timestamp < startTime + 30 minutes) {
                        unchecked {
                            _txFeeDao = amount * sellPreFeeDao / commonDiv;
                            _txFeeLp = amount * sellPreFeeLp / commonDiv;
                        }
                    } else {
                        unchecked {
                            _txFeeDao = amount * sellFeeDao / commonDiv;
                            _txFeeLp = amount * sellFeeLp / commonDiv;
                        }
                    }
                    amount -= _txFeeDao;
                    amount -= _txFeeLp;
                } else if (from == uniswapV2Pair) {
                    // buy
                    unchecked {
                        _txFeeDao = amount * buyFeeDao / commonDiv;
                        _txFeeLp = amount * buyFeeLp / commonDiv;
                        amount -= _txFeeDao;
                        amount -= _txFeeLp;
                    }
                }

                if (_txFeeDao > 0 || _txFeeLp > 0) {
                    super._transfer(from, address(this), _txFeeDao + _txFeeLp);
                    feeDaoCalc += _txFeeDao;
                    feeLpCalc += _txFeeLp;
                }

                if (to == uniswapV2Pair) {
                    if (!swapIng && automatedMarketMakerPairs[to] && maxSupply <= minted 
                        && block.timestamp >= lastLpBurnTime + lpBurnFrequency 
                        && !_excludedFees[from]) {
                        autoLiquidityPairTokens();
                    }
                }

                bool _isDelLp = _isLiquidity(from);
                if (_isDelLp) {
                    if (block.timestamp > startTime + 30 minutes) {
                        uint256 costAmount = amount.mul(delLPFee).div(commonDiv);
                        super._transfer(from, marketPool, costAmount);
                        amount -= costAmount;
                        emit DelLpEvent(to, costAmount);
                    } else {
                        revert ErrLpCannotRemoveNow();
                    }
                }

                if (maxSupply <= minted && address(dinoBuy) != address(0) && 
                    from != uniswapV2Pair && to != uniswapV2Pair) {
                    try dinoBuy.dinoBuyBack() {} catch {}
                }
            }
        }
        super._transfer(from, to, amount);
    }

    event AutoNukeLP();

    function _processSwap(uint256 tokenBal) internal lockTheSwap {
        // to save gas fee, swap bnb at once, sub the amount of swap to mbank 
        _swapTokensForEth(tokenBal, tokenReciever); // swap coin to at once save gas fee
    }

    function dexPrice(uint256 _amount) public view returns (uint256){
        address[] memory _path = new address[](2);
        _path[0] = address(this);
        _path[1] = uniswapV2Router.WETH();
        uint256[] memory amounts = uniswapV2Router.getAmountsOut(_amount, _path);
        return amounts[1];
    }

    uint256 public lpBurnFrequency = 3600 seconds;
    uint256 public lastLpBurnTime;
    uint256 public percentForLPBurn = 50; // 50 = .50%
    
    function autoLiquidityPairTokens() internal lockTheSwap returns (bool) {
        lastLpBurnTime = block.timestamp;
        // get balance of liquidity pair
        uint256 liquidityPairBalance = balanceOf(uniswapV2Pair);
        // calculate amount to burn
        uint256 amountToBurn = liquidityPairBalance * percentForLPBurn / 10000;
        // pull tokens from pancakePair liquidity and move to dead address permanently
        if (amountToBurn > 0) {
            recordBurn(uniswapV2Pair, amountToBurn);
        }
        //sync price since this is not in a swap transaction!
        IUniswapV2Pair pair = IUniswapV2Pair(uniswapV2Pair);
        pair.sync();
        emit AutoNukeLP();
        return true;
    }

    function _swapTokensForEth(uint256 tokenAmount,address to) internal {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        super._approve(address(this), address(uniswapV2Router), tokenAmount);
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            to,
            block.timestamp
        );
    }

    //to recieve ETH from uniswapV2Router when swaping
    error ErrorMintFailed();

    function _addLiquidity(uint256 _bnbAmount, uint256 tokenAmount, address _to) internal {
        super._approve(address(this), address(uniswapV2Router), tokenAmount);
        uniswapV2Router.addLiquidityETH{value: _bnbAmount} (
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            _to,
            block.timestamp
        );
    }

    function buildLpToken(uint256 amount, address to) internal lockMint {
        uint256 _tokenAmount = perMintAmount;//save gas
        super._mint(address(this), _tokenAmount);
        minted += _tokenAmount;
        
        _addLiquidity(amount, _tokenAmount, address(this));
        IUniswapV2Pair(uniswapV2Pair).sync();
        emit AutoNukeLP();

        uint256 _lpBal = IERC20(uniswapV2Pair).balanceOf(address(this));
        uint256 _userAmt = _lpBal.mul(84).div(100);
        IERC20(uniswapV2Pair).transfer(to, _userAmt);
        IERC20(uniswapV2Pair).transfer(adminHolder, _lpBal - _userAmt);

        uint256 _mintableRate = mintableRate;
        uint256 _mintableAmount = maxSupply.mul(_mintableRate).div(100);
        if (minted >= _mintableAmount) {
            super._mint(adminHolder, maxSupply - _mintableAmount);
            minted += maxSupply - _mintableAmount;

            startTime = block.timestamp + aDay;
        }
    }

    function _mintToken() internal {
        require(msg.sender == tx.origin, "Only EOA");
        require(msg.value == perMintPrice, "Mint err amount");
        require(minted < maxSupply.mul(mintableRate).div(100), "Mint over");
        require(holder[msg.sender] == false, "user minted");
        require(!minting, "Minting");
        uint256 buildLpAmount = msg.value.mul(lpRate).div(100); // build LP
        uint256 buyBackAmount = msg.value.mul(buyRate).div(100); // buy back
        
        buildLpToken(buildLpAmount, msg.sender);

        if (address(dinoBuy) != address(0) ) {
            payable(address(dinoBuy)).transfer(buyBackAmount);
        }

        holder[msg.sender] = true;
        totalHolder += 1;
    }

    receive() external payable {
        _mintToken();
    }
}// SPDX-License-Identifier: MIT
