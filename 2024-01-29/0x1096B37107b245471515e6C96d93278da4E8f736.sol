// File: library/PriceHelper.sol


pragma solidity >=0.8.0;
// 价格计算辅助函数
library PriceHelper {
    function priceHelper(
        uint256 price,
        uint256 feeRatePriceLimit,
        uint256 feeRate,
        uint256 feeRateTwo
    ) internal pure returns (uint256) {
        uint256 newPrice;
        if (price < feeRatePriceLimit) {
            newPrice = price + (price * feeRate) / 100;
        }
        if (price >= feeRatePriceLimit) {
            newPrice = price + (price * feeRateTwo) / 100;
        }
        return newPrice;
    }
}

