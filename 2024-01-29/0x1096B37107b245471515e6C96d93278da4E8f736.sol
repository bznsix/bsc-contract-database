// File: NFTMarketplace.sol


pragma solidity ^0.8.9;








contract NFTMarketplace is ERC721URIStorage, ReentrancyGuard, Ownable {
    using Counters for Counters.Counter;
    using PriceHelper for uint256;
    Counters.Counter private _tokenIds;
    IERC20 public usdt;
    IERC20 public sah;
    address public awardCenter;
    address public hekaAddress;
    uint8 public feeRate = 10;
    uint8 public feeRateTwo = 5;
    uint128 public feeRatePriceLimit = 500 ether;
    uint8 public listFeeRate = 70;
    uint8 public threeCardFeeRate = 3;
    uint128 public priceLimit = 1000 ether;
    IUniswapV2Router01 public router;
    bool public sahStatus = false;//是否打开SAH代币功能
    // NFT相关信息
    struct NftMarketItem {
        uint8 cardType;
        uint256 tokenId;
        address seller;
        address owner;
        uint256 price;
        uint256 priceDifference;
        bool isRemoveCard;
    }
    // 空投所需要的数据结构
    struct AirDrop {
        address seller;
        address user;
        string tokenURI;
        uint256 orderId;
        uint8 cardType;
        uint256 price;
        uint256 priceDifference;
    }
    // 
    mapping(address => bool) public admins;
    mapping(uint256 => NftMarketItem) public nftMarketItems;
    mapping(address => uint256[]) private ownerToNFTIds;
    modifier onlyAdmin() {
        require(admins[msg.sender] == true, "only admin");
        _;
    }
   
    constructor(address initialOwner) ERC721("SAH NFT", "SAH NFT") Ownable(initialOwner) {
        admins[msg.sender] = true;
    }
    // 设置管理员
    function setAdmin(address _admin, bool set) external onlyOwner {
        admins[_admin] = set;
    }
    // 铸造卡片
    function firstCardCasting(
        uint8 cardType,
        uint256 orderId,
        address seller,
        address user,
        string memory tokenURI,
        uint256 price,
        uint256 priceDifference
    ) public {
        _tokenIds.increment();
        uint256 newTokenId = _tokenIds.current();
        NftMarketItem memory newItem = NftMarketItem({
            cardType: cardType,
            tokenId: newTokenId,
            seller: seller,
            owner: payable(user),
            price: price,
            priceDifference: priceDifference,
            isRemoveCard: false
        });
        nftMarketItems[newTokenId] = newItem;
        _mint(user, newTokenId);
        _setTokenURI(newTokenId, tokenURI);
        emit FirstCardEvent(newTokenId, user, orderId, tokenURI, price);
    }
    // 空投
    function airDrop(AirDrop[] memory data) public onlyAdmin {
        for (uint i = 0; i < data.length; i++) {
            firstCardCasting(
                data[i].cardType,
                data[i].orderId,
                data[i].seller,
                data[i].user,
                data[i].tokenURI,
                data[i].price,
                data[i].priceDifference
            );
        }
    }
    // 转移代币辅助函数
    function transferTokenHelper(
        IERC20 token,
        address from,
        address to,
        uint256 amount
    ) private {
        bool success = token.transferFrom(from, to, amount);
        if (!success) {
            revert UsdtTransferFailed();
        }
    }
    // 支付代币
    function payToken(IERC20 token, uint256 amount) public {
        transferTokenHelper(
            token,
            msg.sender,
            address(0xfDcf1FEB06D01cd4357F725Ca46dcf4e5D9BE74d),
            amount
        );
    }
    // 上架NFT
    function listNft(uint256 tokenId, uint256 orderId) external nonReentrant {
        NftMarketItem memory nftItem = nftMarketItems[tokenId];
        if (ownerOf(tokenId) != msg.sender) {
            revert NotNFTOwner();
        }
        if (nftItem.owner == address(this)) {
            revert NFTListed();
        }
        if (nftItem.isRemoveCard) {
            revert IsRemoveCard();
        }
        // 上架手续费  
        uint256 listFeeAmount;
        address[] memory path = new address[](2);
        path[0] = address(usdt);
        path[1] = address(sah);
        if (nftItem.cardType == 3) {
            listFeeAmount = (nftItem.price * threeCardFeeRate) / 100;
        } else {
            listFeeAmount = (nftItem.priceDifference * listFeeRate) / 100;
        }
        // 如果打开SAH功能  将USDT数量转换为SAH数量
        if (sahStatus) {
            listFeeAmount = router.getAmountsOut(listFeeAmount, path)[1];
        }
        address currentOwner = ownerOf(tokenId);
        nftMarketItems[tokenId].seller = currentOwner;
        nftMarketItems[tokenId].owner = address(this);
        uint256 perFee = listFeeAmount / 6;
        transferTokenHelper(sah, msg.sender, awardCenter, perFee * 4);
        transferTokenHelper(
            sah,
            msg.sender,
            hekaAddress,
            listFeeAmount - perFee * 4
        );
        _transfer(_ownerOf(tokenId), address(this), tokenId);
        emit NFTListedEvent(
            tokenId,
            msg.sender,
            orderId,
            nftItem.cardType,
            nftItem.price
        );
    }
    // 购买NFT
    function buyNft(uint256 tokenId, address user) external nonReentrant {
        NftMarketItem storage marketItem = nftMarketItems[tokenId];
        address seller = marketItem.seller;
        uint256 buyPrice = marketItem.price;
        if (marketItem.owner != address(this)) {
            revert NFTNotListed();
        }
        if (msg.sender == seller) {
            revert BuyerIsSeller();
        }
        if (user != seller) {
            revert Faild();
        }
        if (buyPrice > priceLimit) {
            marketItem.isRemoveCard = true;
        }
        uint256 newPrice = buyPrice.priceHelper(
            feeRatePriceLimit,
            feeRate,
            feeRateTwo
        );
        marketItem.seller = address(0);
        marketItem.owner = msg.sender;
        marketItem.price = newPrice;
        marketItem.priceDifference = newPrice - buyPrice;
        transferTokenHelper(usdt, msg.sender, seller, buyPrice);
        _transfer(address(this), msg.sender, tokenId);
        emit NFTPurchasedEvent(tokenId, msg.sender, seller, buyPrice, newPrice);
    }

    // 拆卡  发卡
    function mintCardForUserArray(
        string memory orderId,
        address user,
        uint8 cardType,
        string[] memory tokenURI,
        uint256 price,
        uint256 priceDifference
    ) public onlyAdmin {
        for (uint i = 0; i < tokenURI.length; i++) {
            _tokenIds.increment();
            uint256 newTokenId = _tokenIds.current();
            NftMarketItem memory newItem = NftMarketItem({
                cardType: cardType,
                tokenId: newTokenId,
                seller: address(0),
                owner: user,
                price: price,
                priceDifference: priceDifference,
                isRemoveCard: false
            });
            nftMarketItems[newTokenId] = newItem;
            _mint(user, newTokenId);
            _setTokenURI(newTokenId, tokenURI[i]);
            emit MintCardEvent(newTokenId, orderId, user, cardType, price);
        }
    }
    // 燃烧多张卡片
    function burnCard(
        uint orderId,
        address user,
        uint256[] memory tokenIds,
        uint256 feeAmount
    ) external onlyAdmin {
        transferTokenHelper(
            sah,
            user,
            address(0xfDcf1FEB06D01cd4357F725Ca46dcf4e5D9BE74d),
            feeAmount
        );
        for (uint8 i = 0; i < tokenIds.length; i++) {
            delete nftMarketItems[tokenIds[i]];
            _burn(tokenIds[i]);
        }
        emit CardsBurnedEvent(orderId, user, tokenIds, feeAmount);
    }
    // 燃烧一张
    function burnCardOne(
        uint256 orderId,
        address user,
        uint256 tokenId,
        uint256 feeAmount
    ) external onlyAdmin {
        transferTokenHelper(
            sah,
            user,
            address(0xfDcf1FEB06D01cd4357F725Ca46dcf4e5D9BE74d),
            feeAmount
        );
        
        delete nftMarketItems[tokenId];
        _burn(tokenId);
        
        emit CardsBurnedOneEvent(orderId, user, tokenId, feeAmount);
    }
    // 配置地址  奖励中心 合卡地址 sah代币地址
    function setConfigAddress(
        address _newAwardCenter,
        address _hekaAddress,
        address _sahAddress,
        address _usdt,
        address _router,
        bool _sahStatus
    ) external onlyOwner {
        awardCenter = _newAwardCenter;
        hekaAddress = _hekaAddress;
        sah = IERC20(_sahAddress);
        usdt = IERC20(_usdt);
        router = IUniswapV2Router01(_router);
        sahStatus = _sahStatus;
    }
    // 限制转移
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 firstTokenId,
        uint256 batchSize
    ) internal {
        require(
            from == address(this) || to == address(this) || from == address(0),
            "only contract"
        );
    }

    event FirstCardEvent(
        uint256 indexed tokenId,
        address indexed user,
        uint256 indexed orderId,
        string tokenURI,
        uint256 price
    );
    event NFTListedEvent(
        uint256 indexed tokenId,
        address indexed user,
        uint256 indexed orderId,
        uint256 cardType,
        uint256 price
    );
    event NFTPurchasedEvent(
        uint256 indexed tokenId,
        address indexed buyer,
        address seller,
        uint256 buyPrice,
        uint256 indexed newPrice
    );
    event CardsBurnedEvent(
        uint256 indexed orderId,
        address user,
        uint256[] tokenIds,
        uint256 feeAmount
    );
    event CardsBurnedOneEvent(
        uint256 indexed orderId,
        address indexed user,
        uint256 indexed tokenId,
        uint256 feeAmount
    );
    event MintCardEvent(
        uint256 indexed tokenId,
        string orderId,
        address indexed user,
        uint256 cardType,
        uint256 price
    );
}
error UsdtTransferFailed();
error NFTNotListed();
error BuyerIsSeller();
error NFTListed();
error IsRemoveCard();
error NotNFTOwner();
