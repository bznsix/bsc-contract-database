pragma solidity ^0.8.19;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

interface IERC1155 {
    function mint(address account, uint256 id, uint256 amount, bytes memory data) external;

    function mintBatch(address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data) external;
}

interface IERC721Mint {
    function ownerOf(uint256 tokenId) external view returns (address);

    function mint(address receiver) external returns (uint256);
}

contract QuestWizardEnhancementReward is AccessControl, Pausable {
    bytes32 public constant ADD_WINNER_ROLE = keccak256("ADD_WINNER_ROLE");

    IERC1155 public ENHANCEMENT_ADDRESS;
    uint256[] public REWARD_IDS;
    uint256[] public REWARD_AMOUNTS;

    IERC721Mint public NFT_CONTRACT_ADDRESS;

    struct RewardStatus {
        bool exists;
        bool claimed;
    }

    mapping(address => RewardStatus) public winnersMap;

    event Claimed(address receiver, uint256 nft1, uint256 nft2, uint256 nft3);

    constructor (
        IERC1155 _enhancementAddress, uint256[] memory _ids, uint256[] memory _amounts,
        IERC721Mint _nftContractAddress,
        address _addWinnerRoleAddress
    ) {
        ENHANCEMENT_ADDRESS = _enhancementAddress;
        REWARD_IDS = _ids;
        REWARD_AMOUNTS = _amounts;
        NFT_CONTRACT_ADDRESS = _nftContractAddress;
        _setupRole(ADD_WINNER_ROLE, _addWinnerRoleAddress);

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(ADD_WINNER_ROLE, _msgSender());
    }

    function setEnhancementContractAddress(IERC1155 _enhancementAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        ENHANCEMENT_ADDRESS = _enhancementAddress;
    }

    function seNftContractAddress(IERC721Mint _nftAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        NFT_CONTRACT_ADDRESS = _nftAddress;
    }

    function addWinners(address[] calldata _addresses) external onlyRole(ADD_WINNER_ROLE) {
        for (uint256 _i = 0; _i < _addresses.length; _i++) {
            if (!winnersMap[_addresses[_i]].exists) {
                winnersMap[_addresses[_i]] = RewardStatus({exists : true, claimed : false});
            }
        }
    }

    function claim() external whenNotPaused {
        require(winnersMap[_msgSender()].exists, 'NO_REWARDS_AVAILABLE');
        require(winnersMap[_msgSender()].claimed == false, 'REWARD_ALREADY_CLAIMED');

        // Mint Enhancements
        winnersMap[_msgSender()].claimed = true;
        ENHANCEMENT_ADDRESS.mintBatch(_msgSender(), REWARD_IDS, REWARD_AMOUNTS, '');

        // Mint 3x NFTs and emit event
        emit Claimed(
            _msgSender(),
            NFT_CONTRACT_ADDRESS.mint(_msgSender()),
            NFT_CONTRACT_ADDRESS.mint(_msgSender()),
            NFT_CONTRACT_ADDRESS.mint(_msgSender())
        );
    }

    function claimStatus(address _address) external view returns (RewardStatus memory){
        return winnersMap[_address];
    }

    function pause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }
}
