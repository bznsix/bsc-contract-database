        uint256 rng = block.prevrandao % tokenIdList.length;
        uint256 tokenId = tokenIdList[rng];
        tokenIdList[rng] = tokenIdList[tokenIdList.length - 1];
        tokenIdList.pop();
        return tokenId;
    }
}