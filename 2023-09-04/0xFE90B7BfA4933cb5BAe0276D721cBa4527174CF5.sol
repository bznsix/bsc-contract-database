contract LTX_Rewarder is Ownable {
    using SafeERC20 for IERC20;

    struct Event {
        uint8 eventType; // 1: ERC20, 2: ERC721, 3: ERC1155(as NFT), 4: ERC1155 (as FT), 5: custom
        address token;
        address source;
        bytes32 merkleRoot;
        uint256 startTimestamp;
        uint256 expireTimestamp;
        mapping(uint256 => uint256) claimedBitMap;
        uint256[] tokenIdList;
    }

    event CreateAirdrop(uint256 indexed eventId, uint8 eventType, address token, address source, bytes32 merkleRoot, uint256 startTimestamp, uint256 expireTimestamp, uint256[] tokenIdList);
    event ClaimedERC20(uint256 indexed eventId, uint256 index, address account, uint256 amount);
    event ClaimedERC721(uint256 indexed eventId, uint256 index, address account, uint256 tokenId);
    event ClaimedERC1155(uint256 indexed eventId, uint256 index, address account, uint256 tokenId, uint256 amount);
    event ClaimedCustom(uint256 indexed eventId, uint256 index, bytes data);

    uint256 public eventCount;

    mapping(address => bool) public isAdmin;
    mapping(uint256 => Event) public events;

    function addAdmin(address admin) external onlyOwner {
        isAdmin[admin] = true;
    }

    function removeAdmin(address admin) external onlyOwner {
        isAdmin[admin] = false;
    }

    modifier onlyAdmin() {
        require(isAdmin[msg.sender], "Airdrop: caller is not the admin");
        _;
    }

    /** 
      * @notice Create airdrop event.
      * @dev 
      * Only ERC721 and ERC1155NFT airdrop event have tokenIdList.
      * Only Custom airdrop event has no source.
     */
    function createAirdrop(uint8 eventType, address token, address source, bytes32 merkleRoot, uint256 startTimestamp, uint256 expireTimestamp, uint256[] calldata tokenIdList) external onlyAdmin {
        Event storage e = events[eventCount];
        e.eventType = eventType;
        e.token = token;
        e.merkleRoot = merkleRoot;
        e.startTimestamp = startTimestamp;
        e.expireTimestamp = expireTimestamp;
        
        if (eventType == 2 || eventType == 3) {
            e.tokenIdList = tokenIdList;
        }
        
        if(eventType != 5) {
            e.source = source;
        }

        emit CreateAirdrop(eventCount, eventType, token, source, merkleRoot, startTimestamp, expireTimestamp, tokenIdList);
        eventCount++;
    }

    function updateAirdropTime (uint256 eventId, uint256 startTimestamp, uint256 expireTimestamp) external onlyAdmin {
        Event storage e = events[eventId];
        require(e.eventType != 0, "Airdrop: Event not found");
        e.startTimestamp = startTimestamp;
        e.expireTimestamp = expireTimestamp;
    }

    function isClaimed(uint256 eventId, uint256 index) public view returns (bool) {
        Event storage e = events[eventId];
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        uint256 claimedWord = e.claimedBitMap[claimedWordIndex];
        uint256 mask = (1 << claimedBitIndex);
        return claimedWord & mask == mask;
    }

    function _setClaimed(uint256 eventId, uint256 index) private {
        Event storage e = events[eventId];
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        e.claimedBitMap[claimedWordIndex] = e.claimedBitMap[claimedWordIndex] | (1 << claimedBitIndex);
    }

    /**
     * @notice Claim ERC20 dirdrop.
     */
    function claimERC20(uint256 eventId, uint256 index, address account, uint256 amount, bytes32[] calldata merkleProof) external {
        Event storage e = getEventWithCheckClaimAvailable(eventId, index, 1);
    
        bytes32 node = keccak256(abi.encodePacked(index, account, amount));
        require(MerkleProof.verify(merkleProof, e.merkleRoot, node), 'Airdrop: Invalid proof');
        
        _setClaimed(eventId, index);
        IERC20(e.token).safeTransferFrom(e.source, account, amount);

        emit ClaimedERC20(eventId, index, account, amount);
    }

    /**
     * @notice Claim ERC721 dirdrop.
     */
    function claimERC721(uint256 eventId, uint256 index, address account, bytes32[] calldata merkleProof) external {
        Event storage e = getEventWithCheckClaimAvailable(eventId, index, 2);
        
        bytes32 node = keccak256(abi.encodePacked(index, account));
        require(MerkleProof.verify(merkleProof, e.merkleRoot, node), 'Airdrop: Invalid proof');
        
        _setClaimed(eventId, index);
        uint tokenId = randomSelect(e.tokenIdList);

        IERC721(e.token).safeTransferFrom(e.source, account, tokenId);

        emit ClaimedERC721(eventId, index, account, tokenId);
    }

    /**
     * @notice Claim ERC1155 NFT dirdrop.
     */
    function claimERC1155NFT(uint256 eventId, uint256 index, address account, bytes32[] calldata merkleProof) external {
        Event storage e = getEventWithCheckClaimAvailable(eventId, index, 3);
        
        bytes32 node = keccak256(abi.encodePacked(index, account));
        require(MerkleProof.verify(merkleProof, e.merkleRoot, node), 'Airdrop: Invalid proof');
        
        _setClaimed(eventId, index);
        uint tokenId = randomSelect(e.tokenIdList);

        IERC1155(e.token).safeTransferFrom(e.source, account, tokenId, 1, "");

        emit ClaimedERC1155(eventId, index, account, tokenId, 1);
    }

    /**
     * @notice Claim ERC1155 FT dirdrop.
     */
    function claimERC1155FT(uint256 eventId, uint256 index, address account, uint256 tokenId, uint256 amount, bytes32[] calldata merkleProof) external {
        Event storage e = getEventWithCheckClaimAvailable(eventId, index, 4);
        
        bytes32 node = keccak256(abi.encodePacked(index, account, tokenId, amount));
        require(MerkleProof.verify(merkleProof, e.merkleRoot, node), 'Airdrop: Invalid proof');
        
        _setClaimed(eventId, index);
        IERC1155(e.token).safeTransferFrom(e.source, account, tokenId, amount, "");

        emit ClaimedERC1155(eventId, index, account, tokenId, amount);
    }

    /**
     * @notice Claim custom dirdrop.
     */
    function claimCustom(uint256 eventId, uint256 index, bytes calldata data, bytes32[] calldata merkleProof) external {
        Event storage e = getEventWithCheckClaimAvailable(eventId, index, 5);
        
