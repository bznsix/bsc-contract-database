import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./pancake/IPancakeFactory.sol";
import "./pancake/IPancakeRouter02.sol";
import "./pancake/IPancakePair.sol";

contract At is Ownable {
    IPancakePair public V2Pair;
    IPancakeRouter02 public router;
    address public qjkToken;
    address public usdtToken;

    uint256 public rate;
    uint256 public slippageTolerance = 50; // 0.5% default
    uint256 public deadline = 1200; // 20 minutes default

    event TokensSwapped(address tokenIn, address tokenOut, uint256 amountIn, uint256 amountOut);

    function setV2PairAndRouterAddresses(address _V2PairAddress, address _routerAddress) public onlyOwner {
        V2Pair = IPancakePair(_V2PairAddress);
        router = IPancakeRouter02(_routerAddress);
        qjkToken = V2Pair.token0();
        usdtToken = V2Pair.token1();
        iniRate();
    }

    function resonance() external {
        (uint256 reservesQJK, uint256 reservesUSDT) = getReserves();
        uint256 currentRate = calculateRate(reservesQJK, reservesUSDT);

        if (currentRate > rate * 130 / 100) {
            // Price increased by 30%, sell 8% of qjkToken
            swapToken(qjkToken, usdtToken, 8);
        } else if (currentRate < rate * 80 / 100) {
            // Price decreased by 20%, buy 8% of qjkToken using usdtToken
            swapToken(usdtToken, qjkToken, 8);
        }
        rate = currentRate; // Update the stored rate
    }

    function swapToken(address tokenIn, address tokenOut, uint256 percentage) internal {
        IERC20 token = IERC20(tokenIn);
        uint256 balance = token.balanceOf(address(this));
        uint256 amountToSwap = balance * percentage / 100;

        require(token.approve(address(router), amountToSwap), "Approval failed");

        address[] memory path = new address[](2);
        path[0] = tokenIn;
        path[1] = tokenOut;

        uint256 amountOutMin = router.getAmountsOut(amountToSwap, path)[1] * (1000 - slippageTolerance) / 1000;

        uint256[] memory amounts = router.swapExactTokensForTokens(
            amountToSwap,
            amountOutMin,
            path,
            address(this),
            block.timestamp + deadline
        );

        emit TokensSwapped(tokenIn, tokenOut, amounts[0], amounts[1]);
    }

    function calculateRate(uint256 reservesQJK, uint256 reservesUSDT) public pure returns (uint256) {
        return reservesQJK * 1000 / reservesUSDT;
    }

    function getReserves() public view returns (uint256, uint256) {
        (uint256 Reserves0, uint256 Reserves1, ) = V2Pair.getReserves();
        return V2Pair.token0() == qjkToken ? (Reserves0, Reserves1) : (Reserves1, Reserves0);
    }

    function iniRate() public onlyOwner {
        (uint256 reservesQJK, uint256 reservesUSDT) = getReserves();
        rate = calculateRate(reservesQJK, reservesUSDT);
    }

    function setSlippageTolerance(uint256 _slippageTolerance) external onlyOwner {
        require(_slippageTolerance <= 1000, "Slippage tolerance out of range");
        slippageTolerance = _slippageTolerance;
    }

    function setDeadline(uint256 _deadline) external onlyOwner {
        deadline = _deadline;
    }

    function claimToken(address token, uint256 amount) external onlyOwner {
        IERC20(token).transfer(msg.sender, amount);
    }
}
