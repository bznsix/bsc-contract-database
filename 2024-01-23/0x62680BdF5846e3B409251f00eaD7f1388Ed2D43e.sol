// helper methods for interacting with ERC20 tokens and sending ETH that do not consistently return true/false
library TransferHelper {
    function safeApprove(address token, address to, uint value) internal {
        // bytes4(keccak256(bytes('approve(address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x095ea7b3, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: APPROVE_FAILED');
    }

    function safeTransfer(address token, address to, uint value) internal {
        // bytes4(keccak256(bytes('transfer(address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0xa9059cbb, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: TRANSFER_FAILED');
    }

    function safeTransferFrom(address token, address from, address to, uint value) internal {
        // bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x23b872dd, from, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: TRANSFER_FROM_FAILED');
    }

    function safeTransferETH(address to, uint value) internal {
        (bool success,) = to.call{value:value}(new bytes(0));
        require(success, 'TransferHelper: ETH_TRANSFER_FAILED');
    }
}

pragma solidity 0.6.12;


contract Lottery  {
  using SafeMathChainlink for uint256;

  mapping(uint256 => address[]) public gamePlayers;
  mapping(uint256 => address) public gameWinners;
  mapping(uint256 => mapping(address =>IndependentContribution)) public independentData;

  uint256 public minBnbContribution = 10000000000000000; // 0.01 bnb

  mapping(address => bool) public delegator;

  function changeMinContribution(uint256 newContr) public {
    require(msg.sender == owner, 'Ownable: you may not');
    minBnbContribution = newContr;
  }
  
  struct IndependentContribution{
      uint256 bnbBet;
      uint256 totalEntries;
  }
    

  uint256 public roundN = 0;
  uint256 public fee = 10;
  address public owner;
  bool public pause = false;
  IRouter02 public router =  IRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
  IBEP20 public RITZ = IBEP20(0x4e89D67D88dBA3c42a8c37E1991c192384C75625);

        
    constructor()  public 
    {
        owner = msg.sender;
        delegator[msg.sender] = true; // external oracle
    }
  

  
  function participateInGame() payable public returns(bool){
      require(msg.value >= minBnbContribution);
      require(pause == false);
      uint256 entries = msg.value.div(10000000000000000);

      // swap
      address[] memory path = new address[](2);
      path[0] = router.WETH();
      path[1] = address(RITZ);
      uint balanceBefore = RITZ.balanceOf(address(this));
      router.swapExactETHForTokensSupportingFeeOnTransferTokens{value:msg.value}(0,path, address(this), block.timestamp);
      uint balanceAfter = RITZ.balanceOf(address(this));
      uint256 difference = balanceAfter - balanceBefore; // amount of added to jackpot
      require(difference > 0, 'bad swap, try higher amounts');

      for(uint256 i=0; i< entries; i++){
          gamePlayers[roundN].push(msg.sender);
          gamePlayers[roundN].push(address(0x0)); // we add 0x0 for rollover!
      }
      independentData[roundN][msg.sender].bnbBet = independentData[roundN][msg.sender].bnbBet.add(msg.value);
      independentData[roundN][msg.sender].totalEntries = independentData[roundN][msg.sender].totalEntries.add(entries);
  }
  

    function returnPlayers(uint256 round) public view returns(address[] memory){
        return gamePlayers[round];
    }
  
    function returnPlayersLenght(uint256 round)public view returns(uint256){
        return gamePlayers[round].length;
    }
    
    function returnIndividualData(address _bettor, uint256 _round) public view returns(uint256, uint256){
        return (independentData[_round][_bettor].bnbBet, independentData[_round][_bettor].totalEntries);
    }
    
    
  
  function extractWinner(uint256 _rand) public {
      require(delegator[msg.sender] == true, 'Not a delegator');
      require(pause == true);
      
      // get round players lenght
      uint256 playerLen = gamePlayers[roundN].length;
      
      // get a number between 0 and player length (selecting winner index)
      uint randomnumber = uint(keccak256(abi.encodePacked(_rand, msg.sender, roundN))) % playerLen;
      
      gameWinners[roundN] = gamePlayers[roundN][randomnumber];
      uint256 theBalance = RITZ.balanceOf(address(this)); 
      uint256 toowner = theBalance.mul(fee).div(100);

      // if address 0x0, rollover. 
      if(gamePlayers[roundN][randomnumber] != address(0x0)){
        TransferHelper.safeTransfer(address(RITZ), owner, toowner);
        TransferHelper.safeTransfer(address(RITZ), gamePlayers[roundN][randomnumber], theBalance.sub(theBalance));
      }

      roundN = roundN +1;
  }
  
  function anyStuckTokens(address _token, uint256 _amount) public {
      require(msg.sender == owner, 'not owner');
    TransferHelper.safeTransfer(_token, msg.sender, _amount); // in case of, will be airdropped
  }

  function changeFee(uint256 _fee) public {
      require(msg.sender == owner, 'not owner');
      require(_fee <= 10, 'fee too high');
      fee = _fee;
  }
 
  function changeOwner(address newOwner) public {
      require(msg.sender == owner, 'not owner');
      owner = newOwner;
  }
  
 function changePause(bool _pause) public {
      require(delegator[msg.sender] == true, 'Not a delegator');
      pause = _pause;
  }

}