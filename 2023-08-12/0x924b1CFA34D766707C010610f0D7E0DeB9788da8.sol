   * @dev Explicitly set `owners` to eliminate loops in future calls of ownerOf().
   */
  function _setOwnersExplicit(uint256 quantity) internal {
    uint256 oldNextOwnerToSet = nextOwnerToExplicitlySet;
    require(quantity > 0, "quantity must be nonzero");
    uint256 endIndex = oldNextOwnerToSet + quantity - 1;
    if (endIndex > currentIndex - 1) {
      endIndex = currentIndex - 1;
    }
    // We know if the last one in the group exists, all in the group exist, due to serial ordering.
    require(_exists(endIndex), "not enough minted yet for this cleanup");
    for (uint256 i = oldNextOwnerToSet; i <= endIndex; i++) {
      if (_ownerships[i].addr == address(0)) {
        TokenOwnership memory ownership = ownershipOf(i);
        _ownerships[i] = TokenOwnership(
          ownership.addr,
          ownership.startTimestamp
        );
      }
    }
    nextOwnerToExplicitlySet = endIndex + 1;
  }

  /**
   * @dev Internal function to invoke {IERC721Receiver-onERC721Received} on a target address.
   * The call is not executed if the target address is not a contract.
   *
   * @param from address representing the previous owner of the given token ID
   * @param to target address that will receive the tokens
   * @param tokenId uint256 ID of the token to be transferred
   * @param _data bytes optional data to send along with the call
   * @return bool whether the call correctly returned the expected magic value
   */
  function _checkOnERC721Received(
    address from,
    address to,
    uint256 tokenId,
    bytes memory _data
  ) private returns (bool) {
    if (to.isContract()) {
      try
        IERC721Receiver(to).onERC721Received(_msgSender(), from, tokenId, _data)
      returns (bytes4 retval) {
        return retval == IERC721Receiver(to).onERC721Received.selector;
      } catch (bytes memory reason) {
        if (reason.length == 0) {
          revert("ERC721A: transfer to non ERC721Receiver implementer");
        } else {
          assembly {
            revert(add(32, reason), mload(reason))
          }
        }
      }
    } else {
      return true;
    }
  }

  /**
   * @dev Hook that is called before a set of serially-ordered token ids are about to be transferred. This includes minting.
   *
   * startTokenId - the first token id to be transferred
   * quantity - the amount to be transferred
   *
   * Calling conditions:
   *
   * - When `from` and `to` are both non-zero, ``from``'s `tokenId` will be
   * transferred to `to`.
   * - When `from` is zero, `tokenId` will be minted for `to`.
   */
  function _beforeTokenTransfers(
    address from,
    address to,
    uint256 startTokenId,
    uint256 quantity
  ) internal virtual {}

  /**
   * @dev Hook that is called after a set of serially-ordered token ids have been transferred. This includes
   * minting.
   *
   * startTokenId - the first token id to be transferred
   * quantity - the amount to be transferred
   *
   * Calling conditions:
   *
   * - when `from` and `to` are both non-zero.
   * - `from` and `to` are never both zero.
   */
  function _afterTokenTransfers(
    address from,
    address to,
    uint256 startTokenId,
    uint256 quantity
  ) internal virtual {}
}


pragma solidity ^0.8.2;

/** 
@title CatronautCommanders
@author Arlend
@notice This contract is used for minting and handling the NFT's released by the catmoon team.
*/
contract CatronautCommanders is IERC721Enumerable, ERC721A, Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using Strings for uint256;

    uint256 internal price = 0.27 ether; // = 0,27 bnb; Price // 1 gwei
    uint256 internal presalePrice = 0.23 ether;
    uint256 internal reserved; // = 20; Amount reserved for team or giveaways
    
    uint256 internal MAX_SUPPLY; // = 500; Maximum supply
    uint256 internal MAX_TOKENS_PER_MINT; // = 1; Max NFT's that can be minted per mint
    uint256 internal totalMinted = 0;
    uint256 public MAX_LEVEL = 3;

    //The payment receiver when calling withdraw function
    address payable beneficiary = payable(0xc716ccac5c06810c6684eDeB1789b9Db3A0AC05a);

    bool internal saleStarted = false;
    bool internal isPreSale = false;

    //Provenance hash is used in proving that NFTS are randomized.
    //Explained here: https://medium.com/coinmonks/the-elegance-of-the-nft-provenance-hash-solution-823b39f99473
    string internal PROVENANCE_HASH = "";
    string internal baseExtension = ".json";
    string internal baseURI;

    
    //Who can call the "onlyDev" functions
    address payable private _developmentAddress = payable(0xc716ccac5c06810c6684eDeB1789b9Db3A0AC05a);

    mapping(uint256 => uint256) private levels;
    mapping(address => uint256) private nftsMintedPerAddress; 
    mapping(uint256 => uint256) private levelCosts;
    mapping(address => bool) private whitelistedAddresses;
    mapping(address => bool) private whitelistUnlimited;

    /** 
    @notice Constructor that takes arguments once when deploying the contract.
    @param _maxSupply The number of the max supply of NFT's.
    @param _nReserved The number of NFT's that are reserved for give aways and contests.
    @param _maxTokensPerMint The number of tokens one address can mint per transaction.
    @param _uri The url that links to the IPFS json metadata.
    @param _name The name of the NFT contract.
    @param _symbol The symbol of NFT's minted by this contract.
    */
    constructor(
        uint256 _maxSupply,
        uint256 _nReserved,
        uint256 _maxTokensPerMint,
        string memory _uri,
        string memory _name,
        string memory _symbol
    ) ERC721A(_name, _symbol, _maxTokensPerMint) {
        reserved = _nReserved;
        MAX_SUPPLY = _maxSupply;
        MAX_TOKENS_PER_MINT = _maxTokensPerMint;
        baseURI = _uri;

        levelCosts[1] = 0.12 ether; // 25 usd
        levelCosts[2] = 0.17 ether; // 37.5 usd
        levelCosts[3] = 0.26 ether; // 56.25 usd
        
    }

    //Modifier that makes it so only the "owner" address or "developmentAddress" can call the specific functions
    modifier onlyDev() {	
        require(owner() == _msgSender() || _developmentAddress == _msgSender(), "Caller is not the dev");	
        _;	
    }

    event newNFTMinted(address indexed buyer, uint256 amount);
    event SetLevel(uint256 tokenId, uint256 level);
    event LevelUp(address indexed account, uint256 tokenId, uint256 level);
    event SetLevelCost(uint256 level, uint256 cost);
    event devAddressUpdated(address indexed previous, address indexed adr);

    /**
    @notice Mints the NFT if the caller of the function goes successfully through the validation.
    @notice Contains quite a bit of logic required to pass the validation.
    @param _nbTokens The number of tokens that the caller wants to mint, this is limited to 1.
    */
    function mint(uint256 _nbTokens) public payable virtual nonReentrant() {
        uint256 _supply = totalSupply();
        uint mintCost = (_nbTokens * price);

        require(_nbTokens <= MAX_TOKENS_PER_MINT, "You cannot mint more than MAX_TOKENS_PER_MINT tokens at once!");
        require(_supply + _nbTokens <= MAX_SUPPLY - reserved, "Not enough Tokens left.");


        if (isPreSale) {
            require(whitelistedAddresses[msg.sender] == true, "Address not whitelisted");
            mintCost = presalePrice;
            if(whitelistUnlimited[msg.sender] == false) {
            setWhitelistedToFalseForAddressInternal(msg.sender);
            }
        } else {
            require(saleStarted, "Sale not started");
        }

        require(mintCost <= msg.value, "Inconsistent amount sent!");
        nftsMintedPerAddress[msg.sender] = nftsMintedPerAddress[msg.sender] + _nbTokens;
        emit newNFTMinted(msg.sender, _nbTokens);
        for (uint256 i = 1; i <= _nbTokens; i++) {
            _safeMint(msg.sender, _nbTokens);
            totalMinted = totalMinted + _nbTokens;
            uint256 _tokenId = totalMinted;
            levels[_tokenId] = 0;
        }
    }


    //Helper methods

    /** 
    @notice Retrieves the base URI
    @return string Returns the base URI in text format.
    */
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    /**
    @param _tokenId The tokenId.
    @return The tokenURI in string format.
    */
    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory)
    {
    require(
        _exists(_tokenId),
        "ERC721Metadata: URI query for nonexistent token"
    );

    string memory currentBaseURI = _baseURI();
    return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, _tokenId.toString(), baseExtension))
        : "";
    }

    /**
    @notice Get's true or false depending on if the sale has started or not.
    @return bool True or false depending on if the sale has started.
    */
    function startSale() public view returns(bool) {
        return saleStarted;
    }

    /**
    @notice Get's true or false depending on if the presale has started or not.
    @return bool True or false depending on if the presale has started.
    */
    function startPreSale() public view returns(bool) {
        return isPreSale;
    }

    /**
    @notice Get's the price of 1 NFT.
    @return uint256 The price in number format.
    */
    function getPrice() public view virtual returns (uint256){
        return price;
    }

    /**
    @notice Get's the presale price of 1 NFT.
    @return uint256 The price in number format.
    */
    function getPresalePrice() public view virtual returns (uint256){
        return presalePrice;
    }

    /**
    * @notice Get's the totalAmount of minted tokens
    */
    function getTotalMinted() public view returns (uint256) {
    return totalMinted;
    }

    /**
    * @notice Get's the totalAmount of minted tokens
    */
    function getNFTLevel(uint256 _tokenId) public view returns (uint256) {
    return levels[_tokenId];
    }

    /**
    @notice Get's amount of minted tokens for the caller of the function.
    @return uint256 The amount of minted tokens in number format.
    */
    function getAmountOfMintedTokens() public view returns(uint256) {
        return nftsMintedPerAddress[msg.sender];
    }

    /**
    @notice Get's amount of minted NFT(Number) for specific address.
    @param _userAddress The address we wish to lookup the amount of NFT's minted for.
    */
    function getAmountOfNftsMintedByAddress(address _userAddress) public view returns(uint256) {
        return nftsMintedPerAddress[_userAddress];
    }

    /**
    @notice Get's the _development address.
    @return address Returns the development address.
    */
    function getDevAddress() public view returns(address) {
        return _developmentAddress;
    }

    /**
    @notice Get's the beneficiary address.
    @return address Returns the beneficiary address.
    */
    function getBeneficiary() public view returns(address) {
        return beneficiary;
    }

    /**
    @notice Get's the provenance hash.
    @return string The provenance hash in text format.
    */
    function getProvenanceHash() public view returns (string memory) {
        return PROVENANCE_HASH;
    }

    //Only dev start

    /**
    @notice Checks if the specified address is whitelisted.
    @param _address The address that will be looked up.
    @return bool Returns true or false depending on if the address is whitelisted. 
    */
    function isWhiteListed(address _address) external view returns (bool) {
        return whitelistedAddresses[_address];
    }

    /**
    @notice Give away the specified token(NFT) to the specified address, can only be given away if the caller of the functions own the token.
    @param _winner The address of the receiver of the token.
    @param _tokenIdToGiveAway The id of the token that will be given away.
    */
    function giveAway(address _winner, uint256 _tokenIdToGiveAway) external onlyDev {
      safeTransferFrom(msg.sender, _winner, _tokenIdToGiveAway);
    }

    /**
    @notice Get's the cost of leveling up for a specific level.
    @param _level The level cost we wish to lookup.
    */
    function getNftLevelCost( uint256 _level) public view returns(uint256) {
        return levelCosts[_level];
    }

    /**
    @notice Send funds from the contract to the beneficiary.
    */
    function withdraw() external virtual onlyDev {
        require(beneficiary != address(0), "Beneficiary not set");

        uint256 _balance = address(this).balance;
    
        beneficiary.transfer(_balance);
    }

    /**
    @notice Get's the balance of the contract.
    @return uint256 Returns the balance of the contract in number format.
    */
    function getBalance() public virtual view onlyDev returns (uint256) {
        return address(this).balance;
    }

    /**
    @notice Flips the sale from true to false or from false to true.
    */
    function flipSaleStarted() external onlyDev {
        require(beneficiary != address(0), "Beneficiary not set");
        require(!isPreSale, "Presale must have ended first");

        saleStarted = !saleStarted;
    }

    /**
    @notice Claims all reserved tokens(NFT). The amount of reserved tokens is specified at the contract deployment and cannot be changed.
    @notice This can also be used for giveaways.
    @param _number The number of reserved NFT's we want to mint at this given time.
    @param _receiver The address of the receiver of the NFT's.
    */
    function claimReserved(uint256 _number, address _receiver) virtual external onlyDev nonReentrant() {
        require(_number <= reserved, "That would exceed the max reserved.");


        for (uint256 i = 1; i <= _number; i++) {
            _safeMint(_receiver,  1);
            totalMinted = totalMinted + 1;
            uint256 _tokenId = totalMinted;
            levels[_tokenId] = 0;
        }
        reserved = reserved - _number;
    }

    /**
    @notice Get's the amount of reserved NFT's that havent been minted yet.
    @return uint256 Returns the amount of reserved tokens in number format.
    */
    function getReservedLeft() external view onlyDev virtual returns (uint256) {
        return reserved;
    }

    /**
    @notice Set's the provenance hash.
    @param _provenanceHash The provenance hash in text format.
    */
    function setProvenanceHash(string memory _provenanceHash) public onlyDev {
        PROVENANCE_HASH = _provenanceHash;
    }

    /**
    @notice Set's the minting price of NFT's
    @param _newPrice The new price.
    */
    function setPrice(uint256 _newPrice) external virtual onlyDev {
        
        price = _newPrice;
    }

    /**
    @notice Set's the minting price of presale NFT's
    @param _newPrice The new price.
    */
    function setPresalePrice(uint256 _newPrice) external virtual onlyDev {
        
        presalePrice = _newPrice;
    }

    /**
    @notice Set's the baseURI used for IPFS, this is set in the constructor.
    @param _uri The string path of the ipfs uri
    */
    function setBaseURI(string calldata _uri) public onlyDev {
        baseURI = _uri;
    }

    /**
    @notice So we can change the wallet that receives the funds.
    @param _beneficiary The address of the new beneficiary.
    */
    function setBeneficiary(address payable _beneficiary) external virtual onlyDev {
        require(beneficiary != address(0));

        beneficiary = _beneficiary;
    }

    /**
    @notice For updating the devAddress.
    @param _devAddress The address of the new address to be specified as the development address.
    */
    function setNewDevAddress(address payable _devAddress) external onlyDev() {
        emit devAddressUpdated(_developmentAddress, _devAddress);	
        _developmentAddress = _devAddress;
    }

    /**
    @notice Flips the presale from true to false or from false to true.
    */
    function flipPresale() external onlyDev {
        require(beneficiary != address(0), "Beneficiary not set");

        isPreSale = !isPreSale;
    }

    /**
    @notice Add a list of addresses to the whitelist, this function does not add to the unlimited whitelist.
    @param _addresses The list of addresses that will be added to the whitelist.
    */
    function addManyToWhitelist(address[] calldata _addresses) external onlyDev {
        for (uint256 i = 0; i < _addresses.length; i++) {
        whitelistUnlimited[_addresses[i]] = false;
        whitelistedAddresses[_addresses[i]] = true;
        }
    }

    /**
    @notice Add a list of addresses to unlimited whitelist, addresses added here has no limit to how many NFT's they can mint.
    @param _addresses The list of addresses that will be added to the unlimited whitelist.
    */
    function addToWhitelistUnlimited(address[] calldata _addresses) external onlyDev {
        for (uint256 i = 0; i < _addresses.length; i++) {
        whitelistedAddresses[_addresses[i]] = true;
        whitelistUnlimited[_addresses[i]] = true;
        }
    }

    /**
    @notice Sets a unlimited whitelist address to no longer be active, this is crucial for members that are not part of the unlimited whitelist.
    @param _addressToSetFalse The address specified to be removed from the whitelist
    */
    function setWhitelistUnlimitedToFalseForAddress(address _addressToSetFalse) public onlyDev {
        whitelistUnlimited[_addressToSetFalse] = false;
    }

    /**
    @notice Add one address to the whitelist.
    @param _addressForWhitelist The address specified to be added to the whitelist.
    */
    function addToWhitelist(address _addressForWhitelist) external onlyDev {
        whitelistedAddresses[_addressForWhitelist] = true;
        whitelistUnlimited[_addressForWhitelist] = false;
    }

    /**
    @notice Sets a whitelisted address to no longer be active, this is crucial for members that are not part of the unlimited whitelist.
    @param _addressToSetFalse The address specified to be removed from the whitelist
    */
    function setWhitelistedToFalseForAddress(address _addressToSetFalse) public onlyDev {
        whitelistedAddresses[_addressToSetFalse] = false;
    }

        /**
    @notice Sets a whitelisted address to no longer be active, this is crucial for members that are not part of the unlimited whitelist.
    @param _addressToSetFalse The address specified to be removed from the whitelist
    */
    function setWhitelistedToFalseForAddressInternal(address _addressToSetFalse) private {
        whitelistedAddresses[_addressToSetFalse] = false;
    }

    /**
    levels up the NFT with the given tokenId
    The caller must be the owner.
    */
    function levelUp(uint256 _tokenId) public payable virtual nonReentrant() {
        require(ownerOf(_tokenId) == msg.sender, "should be the owner of tokenId!");
        uint256 level = levels[_tokenId];
        uint256 nextLevel = level.add(1);
        require( nextLevel <= MAX_LEVEL, "already max level");
        require(levelCosts[nextLevel] <= msg.value, "Inconsistent amount sent!");
        
        levels[_tokenId] = nextLevel;

        emit LevelUp(msg.sender, _tokenId, nextLevel);
    }

  /**
  Sets an NFT level, just in case this is needed, an event is emitted if this ever happends.
  */
    function setNFTLevel(uint256 _tokenId, uint256 _level) external onlyDev {
        require(_tokenId <= totalMinted, "invalid tokenId!");
        require(_level <= MAX_LEVEL, "level exceeds max value");
        levels[_tokenId] = _level;
        emit SetLevel(_tokenId, _level);
    }

  /**
  For changing the price of leveling up to specific levels.
  */  
    function setLevelCost(uint256 _level, uint256 _cost) external onlyDev {
        require(_level >= 1, "invalid level");
        levelCosts[_level] = _cost;

        emit SetLevelCost(_level, _cost);
    }

}