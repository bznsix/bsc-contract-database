// File: contracts/Maketplace/YPStoreExecutor.sol


pragma solidity ^0.8.4;




contract YPStoreExecutor is PlatformFee, IERC721Receiver {
    ILazyNFT LazyNFT;
    uint256 public MAX_MINT_BATCH = 20;

    constructor(address _lazyNFTAddress) {
        LazyNFT = ILazyNFT(_lazyNFTAddress);
    }

    function setMaxMintBatch(uint256 _batch) external onlyOwnerOrAdmin {
        MAX_MINT_BATCH = _batch;
    }

    function mint(
        uint256 _tokenId,
        string memory _uri
    ) external payable returns (uint256) {
        uint256 totalCost = calcTotalCost(1);
        require(msg.value >= totalCost, "Insufficient balance");

        if (!LazyNFT.exists(_tokenId)) {
            LazyNFT.mint(msg.sender, _tokenId, _uri);
        }
        chargeFee(totalCost);
        return 0;
    }

    function mintBatch(
        uint256[] memory _tokenIds,
        string[] memory _uris
    ) external payable returns (uint256) {
        require(_uris.length == _tokenIds.length, "Invalid arguments");
        require(_tokenIds.length <= MAX_MINT_BATCH, "Exceed max mint batch");
        uint256 totalCost = calcTotalCost(_tokenIds.length);
        require(msg.value >= totalCost, "Insufficient balance");
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            if (!LazyNFT.exists(_tokenIds[i])) {
                LazyNFT.mint(msg.sender, _tokenIds[i], _uris[i]);
            }
        }
        chargeFee(totalCost);
        return 0;
    }

    function calcTotalCost(uint256 _amount) public view returns (uint256) {
        return feeAmount * _amount;
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external returns (bytes4) {
        LazyNFT.safeTransferFrom(address(this), tx.origin, tokenId);
        return
            bytes4(
                keccak256("onERC721Received(address,address,uint256,bytes)")
            );
    }
}