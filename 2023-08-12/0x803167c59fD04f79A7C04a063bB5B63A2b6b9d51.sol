pragma solidity ^0.8.4;


contract Governance is Ownable {
    mapping(address => bool) adminUsers;

    function setAdminUsers(
        address[] memory addresses,
        bool isAdmin
    ) external onlyOwner {
        for (uint256 i = 0; i < addresses.length; i++) {
            adminUsers[addresses[i]] = isAdmin;
        }
    }

    modifier onlyOwnerOrAdmin() {
        require(
            adminUsers[msg.sender] || owner() == _msgSender(),
            "Caller is unauthorized"
        );
        _;
    }
}

// File: contracts/Maketplace/PlatformFee.sol


pragma solidity ^0.8.4;



contract PlatformFee is Governance {
    address public feeAccount;
    uint256 public feeAmount;

    function setFeeAccount(address _account) public onlyOwnerOrAdmin {
        feeAccount = _account;
    }

    function setFeeAmount(uint256 _amount) public onlyOwnerOrAdmin {
        feeAmount = _amount;
    }

    function chargeFee(uint256 totalCost) internal {
        if (totalCost > 0) {
            (bool sent, ) = payable(feeAccount).call{value: totalCost}("");

            require(sent, "Failed to send BRISE/CORE");
        }
    }
}

