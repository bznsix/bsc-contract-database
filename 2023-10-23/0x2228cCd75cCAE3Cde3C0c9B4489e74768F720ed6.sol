pragma solidity ^0.8.19;
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./interfaces/IHolder.sol";

contract Holder is IHolder, Ownable {
    using SafeMath for uint256;

    address[] public users;
    address[] public collections;

    uint32 public unlockTime;

    mapping(address => HolderInfo[]) public holderInfo;
    mapping(address => uint256) public userTokenCount;
    mapping(address => uint256) public startIds;

    event TokenDeposited(address indexed user, address indexed collection, uint256 tokenId);
    event TokensWithdrawn(address indexed user, address indexed collection, uint256 tokenId);
    event CollectionRescued(address indexed collection, uint256[] tokenIds);

    function deposit(address collection, uint112 id) external {
        require(block.timestamp < unlockTime, "Holder: expired");
        require(startIds[collection] != 0 && startIds[collection] <= id, "Holder: wrong id");

        uint112 tmpIndex = getCollectionIndex(collection);

        require(tmpIndex < collections.length, "Holder: collection doesn't exist");

        address user = msg.sender;        
        IERC721(collection).transferFrom(user, address(this), id);

        HolderInfo[] storage userTokens = holderInfo[user];

        userTokens.push(HolderInfo(msg.sender, tmpIndex, id, uint32(block.timestamp)));
        userTokenCount[user] = userTokenCount[user].add(1);
        if (userTokenCount[user] == 1) {
            users.push(user);
        }
        emit TokenDeposited(user, collection, id);
    }

    function withdraw(address[] calldata _users) external onlyOwner {
        require(block.timestamp >= unlockTime, "Holder: too early");
        for (uint256 i = 0; i < _users.length; i++) {
            address user = _users[i];
            HolderInfo[] storage userTokens = holderInfo[user];
            uint256 tokenCount = userTokenCount[user];
            for (uint256 j = tokenCount; j > 0; j--) {
                uint256 lastIndex = userTokens.length.sub(1);
                uint256 collectionId = userTokens[lastIndex].collectionId;
                uint256 tokenId = userTokens[lastIndex].tokenId;
                IERC721(collections[collectionId]).transferFrom(address(this), user, tokenId);
                emit TokensWithdrawn(user, collections[collectionId], tokenId);
                userTokens.pop();
                userTokenCount[user] = userTokenCount[user].sub(1);
            }
            if (userTokenCount[user] == 0) {
                removeUser(user);
            }
        }
    }

    function resque(address collection, uint256[] calldata tokenIds) external onlyOwner {
        require(collections.length > 0, "Holder: no collections exist");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721(collection).transferFrom(address(this), owner(), tokenIds[i]);
        }
        emit CollectionRescued(collection, tokenIds);
    }

    function setUnlockTime(uint32 _unlockTime) external onlyOwner {
        unlockTime = _unlockTime;
    }

    function setStartId(address collection, uint32 _startId) external onlyOwner {
        startIds[collection] = _startId;
    }

    function setNewCollection(address collection) external onlyOwner {
        collections.push(collection);
    }

    function getAllTokens() external view returns (HolderInfo[] memory) {
        uint256 totalTokens = 0;
        for (uint256 i = 0; i < users.length; i++) {
            totalTokens = totalTokens.add(userTokenCount[users[i]]);
        }
        HolderInfo[] memory allTokens = new HolderInfo[](totalTokens);
        uint256 index = 0;
        for (uint256 i = 0; i < users.length; i++) {
            HolderInfo[] storage userTokens = holderInfo[users[i]];
            for (uint256 j = 0; j < userTokens.length; j++) {
                allTokens[index] = userTokens[j];
                index++;
            }
        }
        return allTokens;
    }

    function getTokens(address user) external view returns (HolderInfo[] memory) {
        return holderInfo[user];
    }

    function getUserCount() external view returns (uint256) {
        return users.length;
    }

    function getCollectionCount() external view returns (uint256) {
        return collections.length;
    }

    function getCollectionIndex(address collection) internal view returns (uint112) {
        for (uint112 i = 0; i < collections.length; i++) {
            if (collections[i] == collection) {
                return i;
            }
        }
        return ~uint112(0);
    }

    function removeUser(address user) internal {
        for (uint256 i = 0; i < users.length; i++) {
            if (users[i] == user) {
                users[i] = users[users.length - 1];
                users.pop();
                return;
            }
        }
    }
}// SPDX-License-Identifier: Unlicensed
pragma solidity ^0.8.19;

interface IHolder {
        struct HolderInfo {
        address owner;
        uint112 collectionId;
        uint112 tokenId;
        uint32 timestamp;
    }
    function deposit(address collection, uint112 id) external;
    function withdraw(address[] calldata _users) external;
    function resque(address collection, uint256[] calldata tokenIds) external;

    function setUnlockTime(uint32 _unlockTime) external;
    function setStartId(address collection, uint32 _startId) external;
    function setNewCollection(address collection) external;

    function getAllTokens() external view returns (HolderInfo[] memory);
    function getTokens(address user) external view returns (HolderInfo[] memory);
    function getUserCount() external view returns (uint256);
    function getCollectionCount() external view returns (uint256);
}