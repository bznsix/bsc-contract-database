// SPDX-License-Identifier: UNLICENSED
pragma solidity 0.8.19;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
// import console library
import "hardhat/console.sol";

interface IERC20Extended is IERC20{
    function decimals() external view returns (uint8);
}

contract Presale is Ownable{
    // using EnumerableSet library
    using EnumerableSet for EnumerableSet.AddressSet;
    // define a struct of presale round info
    struct PresaleRoundInfo{
        uint256 roundId;
        uint256 startRoundTime;
        uint256 endRoundTime;
        uint256 saleRoundPrice;
        uint256 totalTokenRoundSold;
        uint256 totalUsdtRoundReceived;
        bool isRoundCancelled;
    }
    bool public isPresaleFinished; // default is false
    uint256 public numOfRounds; // number of rounds
//    uint256  public dayPerRoundInSecs;
    address public manager;
    uint256 public totalTokenSale; // total token for sale
    uint256 public totalTokenSold; // total token sold
    uint256 public totalUsdtRaised; // total usdt raised
    uint256 public minPurchasePerUser; // min purchase per user
    uint256 public maxPurchasePerUser; // max purchase per user
    address[] private contributors;
    // mapping of roundId
    mapping(uint256 => PresaleRoundInfo) public presaleRoundInfo; // roundId => PresaleRoundInfo
    mapping(uint256 => mapping(address => uint256)) private _roundUserUsdtContribution; // roundId => user => usdtContribution
    mapping(uint256 => mapping(address => uint256)) private _roundUserTokenReceived; // roundId => user => tokenReceived
    mapping(uint256 => address[]) private _roundContributors; // roundId => contributors
    mapping(address => uint256) private _userTotalUsdtContribution; // user => totalUsdtContribution
    mapping(address => uint256) private _userTotalTokenReceived; // user => totalTokenReceived
    mapping(address => bool) public isUserClaimed; // user => isUserClaimed

    // each users can be buy 3 times in each round
    mapping(uint256 => mapping(address => uint256)) public numBuyTimesOfUser; // roundId => user => numBuyTimesOfUser
    // whitelist setting
    enum WhitelistOption { ENABLED, DISABLED }  // whitelist option
    WhitelistOption public whitelistOption = WhitelistOption.ENABLED; // default is ENABLED
    EnumerableSet.AddressSet private _whitelistAddresses; // whitelist addresses
    mapping(address => bool) public isWhitelisted; // if true, then whitelisted
    // token
    IERC20Extended private _token;
    IERC20Extended private _usdt;
    // event
    event StartPresale(uint256 indexed noOfRounds, uint256 indexed startTimeRound, uint256 indexed endTimeRound);
    event BuyToken(address indexed user, uint256 indexed roundId, uint256 indexed amount);
    event ClaimToken(address indexed user, uint256 indexed amount);
    event ForceToNextRound(uint256 indexed roundId, uint256 indexed remainingRounds);
    event FinalizePresale(uint256 indexed totalTokenSold_, uint256 indexed totalUsdtRaised_);

    constructor(address token_, address usdt_, address manager_){
        _token = IERC20Extended(token_);
        _usdt = IERC20Extended(usdt_);
        manager = manager_;
    }

    function startPresale(
        uint256 _numOfRounds,
        uint256 _totalTokenSale,
        uint256 _minPurchasePerUser,
        uint256 _maxPurchasePerUser,
        uint256[] memory _startTimeRounds,
        uint256[] memory _endTimeRounds,
        uint256[] memory _salePriceRounds
    ) external onlyOwner{
        require(totalTokenSale == 0, "Presale is already started");
        require(_numOfRounds <= 10 && _numOfRounds > 0, "Invalid number of rounds");
        require(_totalTokenSale > 0, "Total token for sale must be greater than 0");
        require(_minPurchasePerUser > 0 && _maxPurchasePerUser > _minPurchasePerUser, "Invalid amount");
        require(_startTimeRounds.length == _numOfRounds && _endTimeRounds.length == _numOfRounds && _salePriceRounds.length == _numOfRounds, "Invalid input");
        numOfRounds = _numOfRounds;
        totalTokenSale = _totalTokenSale;
        minPurchasePerUser = _minPurchasePerUser;
        maxPurchasePerUser = _maxPurchasePerUser;
        for(uint256 i = 1; i <= _numOfRounds; i++){
            require(_startTimeRounds[i-1] < _endTimeRounds[i-1], "Invalid input");
            require(_salePriceRounds[i-1] > 0, "Invalid input");
            presaleRoundInfo[i] = PresaleRoundInfo({
                roundId: i,
                startRoundTime: _startTimeRounds[i-1],
                endRoundTime: _endTimeRounds[i-1],
                saleRoundPrice: _salePriceRounds[i-1],
                totalTokenRoundSold: 0,
                totalUsdtRoundReceived: 0,
                isRoundCancelled: false
            });
        }
        // transfer token to contract for sale
        _token.transferFrom(msg.sender, address(this), totalTokenSale);
        emit StartPresale(numOfRounds, _startTimeRounds[1], _endTimeRounds[_numOfRounds-1]);
    }

    // buy token
    function buyToken(uint256 _roundId, uint256 _amount) public validateRound(_roundId){
        // check if presale is finished
        require(!isPresaleFinished, "Presale is finished");
        // validite whitelist
        require(whitelistOption == WhitelistOption.DISABLED || isWhitelisted[msg.sender], "You are not whitelisted");
        // check number of buy times
        require(numBuyTimesOfUser[_roundId][msg.sender] < 3, "You can only buy 3 times in each round");
        // check token amount + total token sold <= total token for sale
        // check decimals of token
        uint8 decimalsOfTokenSale = _token.decimals();
        uint8 decimalsOfUsdt = _usdt.decimals();
//        uint256 gapDecimals = 10 ** (decimalsOfTokenSale - decimalsOfUsdt);
        uint256 tokenSold = (_amount * presaleRoundInfo[_roundId].saleRoundPrice)* 10 ** (decimalsOfTokenSale - decimalsOfUsdt);
        require(tokenSold + totalTokenSold <= totalTokenSale, "Amount to buy exceeds total token for sale");
        // validate amount of usdt
        uint256 totalUsdtUserContribution = _userTotalUsdtContribution[msg.sender] + _amount;
        require(totalUsdtUserContribution  >= minPurchasePerUser && totalUsdtUserContribution <= maxPurchasePerUser, "Invalid amount");
        require(_amount <= _usdt.balanceOf(msg.sender), "USDT: Insufficient balance");
        require(_amount <= _usdt.allowance(msg.sender, address(this)), "USDT: Insufficient allowance");
        _usdt.transferFrom(msg.sender, manager, _amount);
        //update contributors for the first time of contribution
        if(_roundUserUsdtContribution[_roundId][msg.sender] == 0){
            _roundContributors[_roundId].push(msg.sender);
        }
        if(_userTotalUsdtContribution[msg.sender] == 0){
            contributors.push(msg.sender);
        }
        // update total usdt raised
        totalUsdtRaised += _amount;
        // update usdt contribution of user
        _roundUserUsdtContribution[_roundId][msg.sender] += _amount;
        _userTotalUsdtContribution[msg.sender] += _amount;
        presaleRoundInfo[_roundId].totalUsdtRoundReceived += _amount;
        // update total token sold
        presaleRoundInfo[_roundId].totalTokenRoundSold += tokenSold;
        totalTokenSold += tokenSold;
        // update token received of user
        _roundUserTokenReceived[_roundId][msg.sender] += tokenSold;
        _userTotalTokenReceived[msg.sender] += tokenSold;
        // update number of buy times
        numBuyTimesOfUser[_roundId][msg.sender] += 1;
        emit BuyToken(msg.sender, _roundId, _amount);
    }
    function finalize() public onlyOwner {
        require(!isPresaleFinished, "Presale is finished");
//        require(block.timestamp > presaleRoundInfo[numOfRounds].endRoundTime, "Presale is not finished");
        isPresaleFinished = true;
        emit FinalizePresale(totalTokenSold, totalUsdtRaised);
    }
    function claimToken() public {
        require(isPresaleFinished, "Presale is not finished");
        require(!isUserClaimed[msg.sender], "You have already claimed");
        uint256 totalTokenUserReceived = _userTotalTokenReceived[msg.sender];
        require(totalTokenUserReceived > 0, "You have no token to claim");
        isUserClaimed[msg.sender] = true;
        _token.transfer(msg.sender, totalTokenUserReceived);
        emit ClaimToken(msg.sender, totalTokenUserReceived);
    }
    // get contributors of round
    function getContributorsOfRound(uint256 _roundId) public view returns(address[] memory){
        return _roundContributors[_roundId];
    }
    // get number of contributors of round
    function getNumOfContributorsOfRound(uint256 _roundId) public view returns(uint256){
        return _roundContributors[_roundId].length;
    }
    // get contributors
    function getContributors() public view returns(address[] memory){
        return contributors;
    }
    // get number of contributors
    function getNumOfContributors() public view returns(uint256){
        return contributors.length;
    }
    // get contributors of round by address
    function getUserOfRoundByAddress(uint256 _roundId, address _user) public view returns(uint256, uint256){
        return (_roundUserUsdtContribution[_roundId][_user], _roundUserTokenReceived[_roundId][_user]);
    }
    // get contributors by address
    function getUserByAddress(address _user) public view returns(uint256, uint256, bool){
        return (_userTotalUsdtContribution[_user], _userTotalTokenReceived[_user], isUserClaimed[_user]);
    }
    // get round info
    function getRoundInfo(uint256 _roundId) public view returns(PresaleRoundInfo memory){
        return presaleRoundInfo[_roundId];
    }
    // get all rounds info
    function getAllRoundsInfo() public view returns(PresaleRoundInfo[] memory){
        PresaleRoundInfo[] memory roundsInfo = new PresaleRoundInfo[](numOfRounds);
        for(uint256 i = 1; i <= numOfRounds; i++){
            roundsInfo[i - 1] = presaleRoundInfo[i];
        }
        return roundsInfo;
    }

    // get history of round of user by address
    function getHistoryByAddress(address _contributor) public view returns(uint256[] memory, uint256[] memory){
        uint256[] memory usdtContributionHistory = new uint256[](numOfRounds);
        uint256[] memory tokenReceivedHistory = new uint256[](numOfRounds);
        for(uint256 i = 1; i <= numOfRounds; i++){
            usdtContributionHistory[i - 1] = _roundUserUsdtContribution[i][_contributor];
            tokenReceivedHistory[i - 1] = _roundUserTokenReceived[i][_contributor];
        }
        return (usdtContributionHistory, tokenReceivedHistory);
    }

    // set price round
    function setPriceRound(uint256[] memory roundIds, uint256[] memory prices) external onlyOwner{
        require(roundIds.length == prices.length, "Invalid input");
        for(uint256 i = 0; i < roundIds.length; i++){
            require(roundIds[i] >= 1 && roundIds[i] <= numOfRounds, "Invalid round id");
            presaleRoundInfo[roundIds[i]].saleRoundPrice = prices[i];
        }
    }
    // set min and max purchase per user
    function setBuyLimits(uint256 _minPurchasePerUser, uint256 _maxPurchasePerUser) external onlyOwner{
        require(_minPurchasePerUser > 0 && _maxPurchasePerUser > _minPurchasePerUser, "Invalid input");
        minPurchasePerUser = _minPurchasePerUser;
        maxPurchasePerUser = _maxPurchasePerUser;
    }
    // set start and end time round
    function setTimeRound(uint256[] memory roundIds, uint256[] memory startTimes, uint256[] memory endTimes) external onlyOwner{
        require(roundIds.length == startTimes.length && roundIds.length == endTimes.length, "Invalid input");
        for(uint256 i = 0; i < roundIds.length; i++){
            require(roundIds[i] >= 1 && roundIds[i] <= numOfRounds, "Invalid round id");
            require(startTimes[i] < endTimes[i], "Invalid input");
            presaleRoundInfo[roundIds[i]].startRoundTime = startTimes[i];
            presaleRoundInfo[roundIds[i]].endRoundTime = endTimes[i];
        }
    }
    // add user to whitelist
    function addUserToWhitelist(address _user) external onlyOwner{
        require(!isWhitelisted[_user], "User is whitelisted");
        _whitelistAddresses.add(_user);
        isWhitelisted[_user] = true;
    }
    // remove user from whitelist
    function removeUserFromWhitelist(address _user) external onlyOwner{
        require(isWhitelisted[_user], "User is not whitelisted");
        _whitelistAddresses.remove(_user);
        isWhitelisted[_user] = false;
    }
    // add array of users to whitelist
    function addUserListToWhitelist(address[] memory _users) external onlyOwner{
        for(uint256 i = 0; i < _users.length; i++){
            if(!isWhitelisted[_users[i]]){
                _whitelistAddresses.add(_users[i]);
                isWhitelisted[_users[i]] = true;
            }
        }
    }
    // remove array of users from whitelist
    function removeUserListFromWhitelist(address[] memory _users) external onlyOwner{
        for(uint256 i = 0; i < _users.length; i++){
            if(isWhitelisted[_users[i]]){
                _whitelistAddresses.remove(_users[i]);
                isWhitelisted[_users[i]] = false;
            }
        }
    }
    // get whitelist users
    function getWhitelistUsers() external view returns(address[] memory){
       return _whitelistAddresses.values();
    }
    // set whitelist option
    function setWhitelistOption(WhitelistOption _whitelistOption) external onlyOwner{
        require(_whitelistOption == WhitelistOption.ENABLED || _whitelistOption == WhitelistOption.DISABLED, "Invalid whitelist option");
        whitelistOption = _whitelistOption;
    }
    // set usdt address
    function setUsdtAddress(address usdt_) external onlyOwner{
        _usdt = IERC20Extended(usdt_);
    }
    // set token address
    function setTokenAddress(address token_) external onlyOwner{
        _token = IERC20Extended(token_);
    }
    // set manager address
    function setManagerAddress(address manager_) external onlyOwner{
        manager = manager_;
    }
    // force to next round
    function forceToNextRound(uint256 _roundId) external onlyOwner{
        require(_roundId >= 1 && _roundId < numOfRounds, "Invalid round id");
        // check block timestamp is in the round
        require(block.timestamp >= presaleRoundInfo[_roundId].startRoundTime && block.timestamp <= presaleRoundInfo[_roundId].endRoundTime, "Round is not valid now");
        presaleRoundInfo[_roundId].isRoundCancelled = true;
        presaleRoundInfo[_roundId + 1].startRoundTime = block.timestamp;
        emit ForceToNextRound(_roundId, numOfRounds - _roundId);
    }
    // get current round
    function getCurrentRound() external view returns(uint256){
        for(uint256 i = 1; i <= numOfRounds; i++){
            if(block.timestamp >= presaleRoundInfo[i].startRoundTime && block.timestamp <= presaleRoundInfo[i].endRoundTime){
                if(!presaleRoundInfo[i].isRoundCancelled){
                    return i;
                }
            }
        }
        return 0;
    }
    // get current round include gap time between rounds
    function getCurrentRoundIncludeGap() external view returns(uint256){
        for(uint256 i = 1; i <= numOfRounds; i++){
            if(block.timestamp < presaleRoundInfo[i].startRoundTime && block.timestamp > presaleRoundInfo[i-1].endRoundTime ){
                if(!presaleRoundInfo[i].isRoundCancelled){
                    return i;
                }else{
                    return i + 1;
                }
            }else if(block.timestamp >= presaleRoundInfo[i].startRoundTime && block.timestamp <= presaleRoundInfo[i].endRoundTime){
                if(!presaleRoundInfo[i].isRoundCancelled){
                    return i;
                }else{
                    return i + 1;
                }
            } else if(block.timestamp > presaleRoundInfo[numOfRounds].endRoundTime){
                return numOfRounds;
            }
        }
        return 0;
    }

    // withdraw eth
    function withdrawETH(address admin) external onlyOwner{
        payable(admin).transfer(address(this).balance);
    }
    // withdraw token
    function withdrawTokenERC20(address token, address admin) external onlyOwner{
        IERC20(token).transfer(admin, IERC20(token).balanceOf(address(this)));
    }
    function depositTokenERC20(address token, uint256 amount) external onlyOwner{
        IERC20(token).transferFrom(msg.sender, address(this), amount);
    }
    // receive fallback
    receive() external payable{
        require(msg.value > 0, "Invalid amount");
    }
    // check modifer of round
    modifier validateRound(uint256 _roundId){
        require(_roundId >= 1 && _roundId <= numOfRounds, "Invalid round id");
        require(!presaleRoundInfo[_roundId].isRoundCancelled, "Round is cancelled");
        require(block.timestamp >= presaleRoundInfo[_roundId].startRoundTime
        && block.timestamp <= presaleRoundInfo[_roundId].endRoundTime, "Round is not valid now");
        _;
    }


