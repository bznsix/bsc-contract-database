contract Moderator {
  function isApproved (address[] memory list, address sender) pure public returns(bool) {
        bool appoved = false;
        for (uint256 i = 0; i < list.length; i++) {
            if (list[i] == sender) {
                appoved = true;
            }
        }

        return appoved;
  }
}


