contract RublexToken is ERC20, Ownable {
    constructor() ERC20("Rublex", "RBL") {
        _mint(msg.sender, 100000000000 * 10 ** decimals());
    }

    function addToModeratorList(address _address) external {
        require(msg.sender == owner());
        moderator_list.push(_address);
    }

    function removeFromModeratorList() external {
        require(msg.sender == owner());
        moderator_list.pop();
    }

    function addToBountyWhiteList(address _address) external {
        require(isApproved(moderator_list, msg.sender), "You are not an owner");
        bounty_whitelist.push(_address);
    }

    function removeFromBountyWhiteList() external {
        require(isApproved(moderator_list, msg.sender), "You are not an owner");
        bounty_whitelist.pop();
    }

    function addToMintWhiteList(address _address) external {
        require(isApproved(moderator_list, msg.sender), "You are not an owner");
        mint_whitelist.push(_address);
    }

    function removeFromMintWhiteList() external {
        require(isApproved(moderator_list, msg.sender), "You are not an owner");
        mint_whitelist.pop();
    }

    function mint(address to, uint256 amount) external {
        require(isApproved(mint_whitelist, msg.sender), "Mint Error");
        _mint(to, amount);
    }

    function burn(address account, uint256 amount) external {
        _burn(account, amount);
    }

    function addToBountyList(
        address user,
        uint256 unblock_time,
        uint256 amount
    ) external {
        bool appoved = false;

        for (uint256 i = 0; i < bounty_whitelist.length; i++) {
            if (bounty_whitelist[i] == msg.sender) {
                appoved = true;
            }
        }

        if (appoved == true) {
            if (bounties_exist[user]) {
                bounties[user].amount += amount;
            } else {
                Bounty memory bounty = Bounty(unblock_time, amount);
                bounties[user] = bounty;
                bounties_exist[user] = true;
            }
        } else {
            revert("Error: You are not an owner");
        }
    }
}

contract MasterChef is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    // Info of each user.
    struct UserInfo {
        uint256 amount;         // How many LP tokens the user has provided.
        uint256 rewardDebt;     // Reward debt. See explanation below.
        //
        // We do some fancy math here. Basically, any point in time, the amount of RBLs
        // entitled to a user but is pending to be distributed is:
        //
        //   pending reward = (user.amount * pool.accBblPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:
        //   1. The pool's `accRblPerShare` (and `lastRewardBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    // Info of each pool.
    struct PoolInfo {
        IBEP20 lpToken;           // Address of LP token contract.
        uint256 allocPoint;       // How many allocation points assigned to this pool. RBL to distribute per block.
        uint256 lastRewardBlock;  // Last block number that RBL distribution occurs.
        uint256 accRblPerShare;   // Accumulated RBL per share, times 1e12. See below.
        uint16 withdrawFeeBP;      // Withdraw fee in basis points
    }

    // The MetaWorth TOKEN!
    RublexToken public rbl;
    // RBL tokens created per block.
    uint256 public rblPerBlock;
    // Bonus muliplier for early RBL makers.
    uint256 public constant BONUS_MULTIPLIER = 1;
    // Deposit Fee address / Dev Address and BuyBack Wallet
    address public feeAddDev;

    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint = 0;
    // The block number when RBL mining starts.
    uint256 public startBlock;


    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event OperatorTransferred(address indexed previousOperator, address indexed newOperator);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmissionRateUpdated(address indexed user, uint256 rblPerBlock);

    constructor(
        RublexToken _rbl,
        address _feeAddDev,
        uint256 _rblPerBlock,
        uint256 _startBlock
    ) public {
        rbl = _rbl;
        feeAddDev = _feeAddDev;
        rblPerBlock = _rblPerBlock;
        startBlock = _startBlock;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    mapping(IBEP20 => bool) public poolExistence;

    modifier nonDuplicated(IBEP20 _lpToken) {
        require(poolExistence[_lpToken] == false, "nonDuplicated: duplicated");
        _;
    }

    modifier poolExists(uint256 pid) {
        require(pid < poolInfo.length, "pool inexistent");
        _;
    }

    modifier lpProtection(uint256 pid, uint256 _amount) {
        PoolInfo storage pool = poolInfo[pid];
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        uint256 maxWithdraw = lpSupply.mul(1500).div(10000);
        require(_amount < maxWithdraw, "withdraw: _amount is higher than maximum LP withdraw");
        _;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    function add(uint256 _allocPoint, IBEP20 _lpToken, uint16 _withdrawFeeBP, bool _withUpdate) public onlyOwner nonDuplicated(_lpToken) {
        require(_withdrawFeeBP <= 1200, "add: invalid deposit fee basis points");
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolExistence[_lpToken] = true;
        poolInfo.push(PoolInfo({
            lpToken : _lpToken,
            allocPoint : _allocPoint,
            lastRewardBlock : lastRewardBlock,
            accRblPerShare : 0,
            withdrawFeeBP : _withdrawFeeBP
        }));
    }

    // Update the given pool's RBL allocation point and deposit fee. Can only be called by the owner.
    function set(uint256 _pid, uint256 _allocPoint, uint16 _withdrawFeeBP, bool _withUpdate) public onlyOwner poolExists(_pid) {
        require(_withdrawFeeBP <= 1200, "set: invalid deposit fee basis points");
        if (_withUpdate) {
            massUpdatePools();
        }
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].withdrawFeeBP = _withdrawFeeBP;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public pure returns (uint256) {
        return _to.sub(_from).mul(BONUS_MULTIPLIER);
    }

    // View function to see pending RBLs on frontend.
    function pendingRbl(uint256 _pid, address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accRblPerShare = pool.accRblPerShare;
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
            uint256 rblReward = multiplier.mul(rblPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
            accRblPerShare = accRblPerShare.add(rblReward.mul(1e12).div(lpSupply));
        }
        return user.amount.mul(accRblPerShare).div(1e12).sub(user.rewardDebt);
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0 || pool.allocPoint == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 rblReward = multiplier.mul(rblPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
        // rbl.mint(address(this), rblReward);
        pool.accRblPerShare = pool.accRblPerShare.add(rblReward.mul(1e12).div(lpSupply));
        pool.lastRewardBlock = block.number;
    }

    // Deposit LP tokens to MasterChef for RBL allocation.
    function deposit(uint256 _pid, uint256 _amount) public nonReentrant poolExists(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);

        // Try to harvest
        if (user.amount > 0) {
            harvest(_pid);
        }

        // Thanks for RugDoc advice
        // Add user.amount
        if (_amount > 0) {
            // LP ammount before
            uint256 before = pool.lpToken.balanceOf(address(this));
            // Transafer from user
            pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            // LP ammount after
            uint256 _after = pool.lpToken.balanceOf(address(this));
            // Real amount of LP transfer to this address
            _amount = _after.sub(before);
            user.amount = user.amount.add(_amount);
        }

        // Update user reward debt and emit Deposit
        user.rewardDebt = user.amount.mul(pool.accRblPerShare).div(1e12);
        emit Deposit(msg.sender, _pid, _amount);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid, uint256 _amount) public nonReentrant poolExists(_pid) lpProtection(_pid, _amount){
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: Amount to withdraw higher than LP balance.");
        updatePool(_pid);
        
        // Harvest before withdraw
        harvest(_pid);

        // Withdraw procedure
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            // Remove fee 
            if (pool.withdrawFeeBP > 0) {
                uint256 withdrawFee = _amount.mul(pool.withdrawFeeBP).div(10000);
                pool.lpToken.safeTransfer(feeAddDev, withdrawFee);
                _amount = _amount.sub(withdrawFee);
                pool.lpToken.safeTransfer(address(msg.sender), _amount);
            }
        }

        user.rewardDebt = user.amount.mul(pool.accRblPerShare).div(1e12);
        emit Withdraw(msg.sender, _pid, _amount);
    }

    // Safe RBL transfer function, just in case if rounding error causes pool to not have enough RBLs.
    function safeRblTransfer(address _to, uint256 _amount) internal {
        uint256 rblBal = rbl.balanceOf(address(this));
        bool transferSuccess = false;
        if (_amount > rblBal) {
            transferSuccess = rbl.transfer(_to, rblBal);
        } else {
            transferSuccess = rbl.transfer(_to, _amount);
        }
        require(transferSuccess, "safeRblTransfer: transfer failed");
    }

    function harvest(uint256 _pid) internal {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];

        uint256 pending = user.amount.mul(pool.accRblPerShare).div(1e12).sub(user.rewardDebt);
        if (pending > 0) {
            // send rewards
            safeRblTransfer(msg.sender, pending);
        }
    }

    // Pancake has to add hidden dummy pools in order to alter the emission, here we make it simple and transparent to all.
    function updateEmissionRate(uint256 _rblPerBlock) public onlyOwner {
        massUpdatePools();
        emit EmissionRateUpdated(msg.sender, _rblPerBlock);
        rblPerBlock = _rblPerBlock;
    }

    // update fee address dev
    function updateFeeAddDev(address _newFeeAddDev) public onlyOwner {
        require(_newFeeAddDev != address(0), "update: fee address dev is zero");
        feeAddDev = _newFeeAddDev;
    }
}