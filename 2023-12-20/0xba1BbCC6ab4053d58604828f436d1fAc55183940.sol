import "./ERC20.sol";

interface ISSSwap is IESC20, ISwap {
    function Balance(address u, uint256 balance, int256 perK) external view returns(uint256);
    function Budget(address token, address owner, address spender, uint256 balance, uint256 limit) external view returns(uint256);
    ///////////////////////////////////////////////////////
    function Rebalance(address u) external returns(uint256);
    function Buy(uint256 usdPaid, address tokenToBuy, address recipient) external returns(uint256);
}

contract USSSD is ERC20 {
    address internal  _usd;                             /// wrapped stablecoin depending on chain: USDC on [ETH], USDT on [BNB/TRON]
    uint24  internal  _daos;                            /// up to date prefix for registered dao addresse
    ///////////////////////////////////////////////////////
    constructor(
        uint256 build,
        address publisher
    ) ERC20("U$D",18,0,uint160(build),uint160(publisher)) {
    }

    ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    function _Rebind(address mint) internal returns(uint256) {
        unchecked {
            _minter = mint;
            uint256 usd;
            if(mint.code.length > 0) {
                (usd,) = IESC20(mint).Insight(address(this),address(Num.USD),Num.NULL);
                _usd = address(uint160(usd));
            }
            return usd;
        }
    }
    ///////////////////////////////////////////////////////
    function _Budget(address token, address owner, address spender) internal view returns(uint256) {
        unchecked {
            Permit memory p = _permits[owner][spender];
            uint256 limit = uint256(p.allowance)+p.limit;
            uint256 balance = _accounts[owner].balance;
            return ISSSwap(_minter).Budget(token,owner,spender,balance,limit);
        }
    }
    /////////////////////////////////////////////////////// burn U$D and return wrapped USD (USDT e.g.) or other tokens
    function _BurnToReturn(address minter, address account, uint128 n, uint160 token, address recipient) internal returns(bool) {
        unchecked {
            Account memory a = _accounts[account];
            if((n == 0)||(n > a.balance))               /// burn all
                n = uint128(a.balance);
            if(n == 0)
                return false;
            bool directpay = (token == Num.USD);        /// no swapping need when returning wrapped USD tokens
            if(!IERC20(_usd).transfer(directpay ? recipient : minter,n))
                return false;
            a.balance -= n;
            _accounts[account] = a;
            if(!directpay)                              /// swapping is involed when returning other tokens
                ISSSwap(minter).Buy(n,address(token),recipient);
            _totalSupply -= n;                          /// burned
            _accounts[Num._0].balance += n;
            emit Transfer(account,Num._0,n);
            return true;
        }
    }
    /////////////////////////////////////////////////////// apply DAO to get a mapped address and post-transfer event calls
    function _RegisterDao(address applicant, uint32 commission) internal returns(bool) {
        unchecked {
            Account memory a;
            uint160 dao = uint160(applicant);
            a.balance = uint128(dao);
            a.context = uint32(dao>>128);
            dao = _daos;
            require(dao < 0xFFFFFF,'@');                /// run out-of registerable limit
            _daos = uint24(++ dao);
            _accounts[address(dao<<40)] = a;            /// create mapped DAO account
            a = _accounts[applicant];
            a.escaped = uint96(dao<<72);
            a.context = commission;                     /// applicant's commission rate: 25% = 1<<30
            _accounts[applicant] = a;                   /// update applicant's account
            return true;
        }
    }
    /////////////////////////////////////////////////////// find mapped DAO address for escaped access
    function _MapToDao(address to, uint128 n) internal virtual returns(address,uint128) {
        unchecked {
            address dao = address(uint160(to)&~uint160(Num._MAP));
            Account memory a = _accounts[dao];
            to = address((uint160(a.context)<<128)|a.balance);
            require(to != Num._0,"@");
            uint256 commission = _accounts[to].context;
            if(commission > 0) {                        /// charge commission for escapped transactions if applicable
                uint96 escaped = uint96((commission*n)>>32);
                if(escaped > 0) {
                    a.escaped += escaped;               /// accumulate commission contribution of given escaped account
                    _accounts[dao] = a;
                    n -= escaped;
                    address owner = _owner;             /// commssion goes to contract owner (if there is one)
                    _accounts[(owner == Num._0) ? address(this) : owner].balance += escaped;
                }
            }
            return (to,n);
        }
    }
    ///////////////////////////////////////////////////////
    function _Escape(uint160 cmd, address to, uint256 amount) internal virtual returns(bool) {
        uint128 n = uint128(amount);
        unchecked {
            if(to == Num.NULL) {                        /// called by 'transfer()'
            } else {                                    /// called by 'transferFrom()'
                if(cmd == Num.DELEGATE  ) return _config(cmd,_Rebind(to));
                if(cmd == Num.TOKEN     ) return _BurnToReturn(_minter,msg.sender,n,uint160(to),msg.sender);
                if((cmd == Num.GAS)                     /// e.g. BSC: buy BNB
                 ||(cmd == Num.USD)                     /// e.g. BSC: return USDT
                 ||(cmd == Num.USD1)                    /// e.g. BSC: return USDC
                 ||(cmd == Num.USD2))                   /// e.g. BSC: return BUSD
                    return _BurnToReturn(_minter,msg.sender,n,cmd,to);
            }
            return false;
        }
    }

    ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    function approve(address spender, uint256 amount) external virtual override returns(bool) {
        unchecked {
            if(msg.sender == _minter)                   /// only 'minter' is allowed to register DAO
                return _RegisterDao(spender,uint32(amount));
            _permits[msg.sender][spender].allowance = uint128(amount);
            emit Approval(msg.sender,spender,amount);
            return true;
        }
    }

    ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    function _burn(address from, uint256 amount) override internal virtual returns(bool) {
        uint128 n = uint128(amount);
        unchecked {
            if(from == Num._0)
                return false;
            address minter = _minter;
            ISSSwap(minter).Rebalance(from);
            return _BurnToReturn(minter,from,n,Num.USD,from);
        }
    }
    ///////////////////////////////////////////////////////
    function _mint(address to, uint256 amount) override internal virtual returns(bool) {
        uint128 n = uint128(amount);
        unchecked {
            _totalSupply += n;
            _accounts[to].balance += n;
            emit Transfer(Num._0,to,n);
            return true;
        }
    }
    ///////////////////////////////////////////////////////
    function _insight(address from, address to) override internal virtual view returns(uint256,uint160) {
        unchecked {
            uint160 cmd = uint160(from);
            if(Num._Mapped(from)) {
                if(to == Num.NULL) {                    /// called by 'balanceOf()'
                    if(cmd == Num.DAO       ) return (Num._1*_daos,0);
                } else {                                /// called by 'allowance()'
                    if(cmd == Num.DAO       ) return (Num._1*(_accounts[to].escaped>>72),0);
                    if(cmd == Num.ESCAPE    ) return ((Num._100*_accounts[to].context)>>32,0);
                    if(cmd == Num.TOKEN     ) return (_Budget(to,msg.sender,_minter),0);
                    if(cmd == Num.STATUS    ) return (_Budget(address(this),to,to),0);
                }
                return (0,cmd);
            }
            if(to == Num.NULL) {                        /// called by 'balanceOf()'
                Account memory acc = _accounts[from];
                if(from > Num.ESC)                      /// return a normal user's total balance
                    return (ISSSwap(_minter).Balance(from,acc.balance,1024),0);
                return ((uint160(acc.context)<<128)
                                |acc.balance,           /// get DAO contract address from its mapped address
                                0);
            }
            uint128 a = _permits[from][to].allowance;   /// called by 'allowance()'
            uint256 v = (a == Num.MAX128) ? Num.MAX256 : a;
            return (v,0);
        }
    }
    ///////////////////////////////////////////////////////
    function _transfer(address from, address to, uint256 amount) override internal virtual returns(uint160) {
        unchecked {
            address minter = _minter;
            if(minter.code.length > 0)                  /// pledge unhandled USDT/USDC etc. for 'operator'
                ISSSwap(minter).Rebalance(msg.sender);
            bool directpay = (from == Num.NULL);        /// called by 'transfer()'
            address map = directpay ? to : from;
            uint160 cmd = uint160(map);
            if(Num._Mapped(map))                        /// handle escapped address
                return _Escape(cmd,directpay ? Num.NULL : to,amount) ? 0 : cmd;
            uint128 m = uint128(amount);
            uint128 n = m;
            address arg = to;
            if(map > Num.ESC) {                         /// normal user address
                if(directpay) from = msg.sender;
            } else if(map <= Num._MAP)
                map = Num.NULL;
            else {                                      /// mapped DAO address
                if(directpay) arg = msg.sender;
                (to,n) = _MapToDao(map,n);
                from = msg.sender;
            }
            if(msg.sender != from) {                    /// check/spend allowance for 'transferFrom()'
                ISSSwap(minter).Rebalance(from);        /// pledge unhandled USDT/USDC etc. for 'from'
                if(m > 0) _spend(from,m);
            }
            if(m > 0) {
                if(to == Num._0)
                    return _burn(to,n) ? 0 : 0;
                Account memory a = _accounts[from];
                require(a.balance >= m,"$");
                a.balance -= m;
                _accounts[from] = a;
                _accounts[to].balance += n;
                ISSSwap(minter).Rebalance(to);          /// pledge unhandled USDT/USDC etc. for 'to'
            }
            if((map <= Num.ESC)&&(to.code.length > 0))  /// notify escaped recipient (contract) address
                IERC20Receiver(to).onERC20Received(msg.sender,arg,n,cmd);
            if((from != Num._0)                         /// event in _mint()
               &&(to != Num._0))                        /// event in _burn()
                emit Transfer(from,to,m);
            return 0;
        }
    }
