// SPDX-License-Identifier: MIT
pragma solidity ^0.8.14;

import "./Ownable.sol";
import "./TransferHelper.sol";
import "./SafeMath.sol";
import "./ECDSA.sol";
import "./IERC20.sol";
import "./ReentrancyGuard.sol";
import "./Address.sol";
import "./IPancakeRouter.sol";
import "./IPancakePair.sol";
import "./IPancakeFactory.sol";


contract Market is Ownable,ReentrancyGuard {
	using SafeMath for uint256;
	address public route;
	address public factory;
	address public weth;
	
	struct Token {
		uint kind;			//1:ETH 2:WETH 3:ERC20
		address token;		//代币合约地址
	}

	
	struct GroupItem {
        address wallet;
        uint rate;
    }

	
	// 滑点最大比率 * 2
	uint private pointMax = 10 ** 8;




	constructor (address _route) {
		_setting(_route);

		// 测试用例
		// address cake = 0xFa60D973F7642B748046464e165A65B7323b0DEE;
		// address tbnb = address(0);
		// address wbnb = 0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd;
		// poolCreat(1,cake,tbnb,4,500000);
		// poolCreat(2,tbnb,cake,4,500000);
		// poolCreat(3,cake,wbnb,4,500000);
		// poolCreat(4,wbnb,cake,4,500000);
	}

    receive() external payable {}
    fallback() external payable {}

	function _tokenSend(
        address _token,
		address _to,
        uint _amount
    ) private returns (bool result) {
        if(_token == address(0)){
            Address.sendValue(payable(_to),_amount);
            result = true;
        }else{
            IERC20 token = IERC20(_token);
            result = token.transfer(_to,_amount);
        }
    }

    function tokenExtract(
        address _token,
		address _to,
        uint _amount
    ) public onlyOwner {
        require(_tokenSend(_token,_to,_amount),"Market::extract fail");
    }

	function outCompute(
		uint inAmount,
		address[] memory path
	) internal view returns (uint result) {
		uint[] memory amounts = IPancakeRouter(route).getAmountsOut(inAmount,path);
		result = amounts[amounts.length.sub(1)];
		require(result > 0,"Market::outAmount should be greater than 0");
	}

	function inCompute(
		uint outAmount,
		address[] memory path
	) internal view returns (uint result) {
		uint[] memory amounts = IPancakeRouter(route).getAmountsIn(outAmount,path);
		result = amounts[0];
		require(result > 0,"Market::inAmount should be greater than 0");
	}

	function pointHandle(
		uint point,
		uint amount,
		bool isplus
	) internal view returns (uint result) {
		uint rate = 1;
		if(isplus) {
			rate = pointMax.add(point);
		}else {
			rate = pointMax.sub(point);
		}
		result = amount.mul(rate).div(pointMax);
	}

	

	

	function Convert(
		address inToken,
		address outToken,
		uint inAmount,
		uint outAmount,
		address[] memory path,
		GroupItem[] memory list,
		uint side,
		uint deadline,
		uint point
	) public onlyOwner {
		uint total = 0;
        for(uint i = 0; i < list.length; i++) {
            total = total.add(list[i].rate);
        }
        require(total <= pointMax,"Market::Over 100%");
		
		if(side == 0) {
			outAmount = outCompute(inAmount,path);
			outAmount = pointHandle(point,outAmount,false);
		}else if(side == 1) {
			inAmount = inCompute(outAmount,path);
			inAmount = pointHandle(point,inAmount,true);
		}
		Token memory tokenIn = Token({
			kind:tokenType(inToken),
			token:inToken
		});
		Token memory tokenOut = Token({
			kind:tokenType(outToken),
			token:outToken
		});


		if(tokenIn.kind == 3) {
			require(inAmount <= balanceOf(inToken),"Market::insufficient funds reserves");
		}else {
			require(inAmount <= balanceOf(address(0)),"Market::insufficient funds reserves");
		}

		_swap(tokenIn,tokenOut,inAmount,outAmount,path,side,deadline);
		
		
		for(uint i = 0;i < list.length; i++) {
			if(tokenOut.kind == 3) {
				require(_tokenSend(outToken,list[i].wallet,list[i].rate.mul(outAmount).div(pointMax)),"Market::asset conversion fail");
			}else {
				require(_tokenSend(address(0),list[i].wallet,list[i].rate.mul(outAmount).div(pointMax)),"Market::asset conversion fail");
			}
		
		}
		
		
	}

	function balanceOf(
		address _token
	) public view returns (uint balance) {
		if(_token == address(0)) {
			balance = address(this).balance;
		}else {
			IERC20 token = IERC20(_token);
            balance = token.balanceOf(address(this));
		}
	}

	function _swap(
		Token memory tokenIn,
		Token memory tokenOut,
		uint inAmount,
		uint outAmount,
		address[] memory path,
		uint side,
		uint deadline
	) private {
		if(tokenIn.kind == 3) {
			TransferHelper.safeApprove(tokenIn.token,route,inAmount);
		}
		if(tokenIn.kind == 3 && tokenOut.kind == 3 && side == 0) {
			IPancakeRouter(route).swapExactTokensForTokensSupportingFeeOnTransferTokens(
				inAmount,
				outAmount,
				path,
				address(this),
				deadline
			);
		}else if(tokenIn.kind == 3 && tokenOut.kind == 3 && side == 1) {
			IPancakeRouter(route).swapTokensForExactTokens(
				outAmount,
				inAmount,
				path,
				address(this),
				deadline
			);
		}else if((tokenIn.kind == 2 || tokenIn.kind == 1) && tokenOut.kind == 3 && side == 0) {
			IPancakeRouter(route).swapExactETHForTokensSupportingFeeOnTransferTokens{value: inAmount}(
				outAmount,
				path,
				address(this),
				deadline
			);
		}else if(tokenIn.kind == 3 && (tokenOut.kind == 2 || tokenOut.kind == 1) && side == 1) {
			IPancakeRouter(route).swapTokensForExactETH(
				outAmount,
				inAmount,
				path,
				address(this),
				deadline
			);
		}else if(tokenIn.kind == 3 && (tokenOut.kind == 2 || tokenOut.kind == 1) && side == 0) {
			IPancakeRouter(route).swapExactTokensForETHSupportingFeeOnTransferTokens(
				inAmount,
				outAmount,
				path,
				address(this),
				deadline
			);
		}else if((tokenIn.kind == 2 || tokenIn.kind == 1) && tokenOut.kind == 3 && side == 1) {
			IPancakeRouter(route).swapETHForExactTokens{value: inAmount}(
				outAmount,
				path,
				address(this),
				deadline
			);
		}
	}


	


	




	

	function quote(
		address inToken,
		address outToken,
		uint inAmount
	) public view returns (uint amountIn,uint amountOut,uint reserveIn,uint reserveOut) {
		Token memory tokenIn = Token({
			kind:tokenType(inToken),
			token:inToken
		});
		Token memory tokenOut = Token({
			kind:tokenType(outToken),
			token:outToken
		});
		
		
		address _pair = IPancakeFactory(factory).getPair(tokenIn.token,tokenOut.token);
		require(_pair != address(0),"Market::liquidit pool non-existent");
		
		(uint reserve0,uint reserve1,) = IPancakePair(_pair).getReserves();
		address token0 = IPancakePair(_pair).token0();
		amountIn = inAmount;
		if(tokenIn.token == token0) {
			amountOut = IPancakeRouter(route).quote(inAmount,reserve0,reserve1);
			reserveIn = reserve0;
			reserveOut = reserve1;
		}else {
			amountOut = IPancakeRouter(route).quote(inAmount,reserve1,reserve0);
			reserveIn = reserve1;
			reserveOut = reserve0;
		}
	}

	
	function tokenType(
		address _token
	) internal view returns (uint) {
		uint typeval = 3;
		if(_token == address(0)) {
			typeval = 1;
		}else if(_token == weth) {
			typeval = 2;
		}
		return typeval;
	}

	

	function setting(
		address _route
	) public onlyOwner {
		_setting(_route);
	}

	function _setting(
		address _route
	) private {
		require(_route != address(0),"Market::invalid route address");
		route = _route;
		factory = IPancakeRouter(_route).factory();
		weth = IPancakeRouter(_route).WETH();
	}

	

}
