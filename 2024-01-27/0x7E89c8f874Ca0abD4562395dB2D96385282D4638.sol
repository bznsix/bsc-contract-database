interface IUniswapV3Factory {
    /// @notice Emitted when the owner of the factory is changed
    /// @param oldOwner The owner before the owner was changed
    /// @param newOwner The owner after the owner was changed
    event OwnerChanged(address indexed oldOwner, address indexed newOwner);

    /// @notice Emitted when a pool is created
    /// @param token0 The first token of the pool by address sort order
    /// @param token1 The second token of the pool by address sort order
    /// @param fee The fee collected upon every swap in the pool, denominated in hundredths of a bip
    /// @param tickSpacing The minimum number of ticks between initialized ticks
    /// @param pool The address of the created pool
    event PoolCreated(
        address indexed token0,
        address indexed token1,
        uint24 indexed fee,
        int24 tickSpacing,
        address pool
    );

    /// @notice Emitted when a new fee amount is enabled for pool creation via the factory
    /// @param fee The enabled fee, denominated in hundredths of a bip
    /// @param tickSpacing The minimum number of ticks between initialized ticks for pools created with the given fee
    event FeeAmountEnabled(uint24 indexed fee, int24 indexed tickSpacing);

    /// @notice Returns the current owner of the factory
    /// @dev Can be changed by the current owner via setOwner
    /// @return The address of the factory owner
    function owner() external view returns (address);

    /// @notice Returns the tick spacing for a given fee amount, if enabled, or 0 if not enabled
    /// @dev A fee amount can never be removed, so this value should be hard coded or cached in the calling context
    /// @param fee The enabled fee, denominated in hundredths of a bip. Returns 0 in case of unenabled fee
    /// @return The tick spacing
    function feeAmountTickSpacing(uint24 fee) external view returns (int24);

    /// @notice Returns the pool address for a given pair of tokens and a fee, or address 0 if it does not exist
    /// @dev tokenA and tokenB may be passed in either token0/token1 or token1/token0 order
    /// @param tokenA The contract address of either token0 or token1
    /// @param tokenB The contract address of the other token
    /// @param fee The fee collected upon every swap in the pool, denominated in hundredths of a bip
    /// @return pool The pool address
    function getPool(
        address tokenA,
        address tokenB,
        uint24 fee
    ) external view returns (address pool);

    /// @notice Creates a pool for the given two tokens and fee
    /// @param tokenA One of the two tokens in the desired pool
    /// @param tokenB The other of the two tokens in the desired pool
    /// @param fee The desired fee for the pool
    /// @dev tokenA and tokenB may be passed in either order: token0/token1 or token1/token0. tickSpacing is retrieved
    /// from the fee. The call will revert if the pool already exists, the fee is invalid, or the token arguments
    /// are invalid.
    /// @return pool The address of the newly created pool
    function createPool(
        address tokenA,
        address tokenB,
        uint24 fee
    ) external returns (address pool);

    /// @notice Updates the owner of the factory
    /// @dev Must be called by the current owner
    /// @param _owner The new owner of the factory
    function setOwner(address _owner) external;

    /// @notice Enables a fee amount with the given tickSpacing
    /// @dev Fee amounts may never be removed once enabled
    /// @param fee The fee amount to enable, denominated in hundredths of a bip (i.e. 1e-6)
    /// @param tickSpacing The spacing between ticks to be enforced for all pools created with the given fee amount
    function enableFeeAmount(uint24 fee, int24 tickSpacing) external;
}

interface IUniswapV3SwapCallback {
    /// @notice Called to `msg.sender` after executing a swap via IUniswapV3Pool#swap.
    /// @dev In the implementation you must pay the pool tokens owed for the swap.
    /// The caller of this method must be checked to be a UniswapV3Pool deployed by the canonical UniswapV3Factory.
    /// amount0Delta and amount1Delta can both be 0 if no tokens were swapped.
    /// @param amount0Delta The amount of token0 that was sent (negative) or must be received (positive) by the pool by
    /// the end of the swap. If positive, the callback must send that amount of token0 to the pool.
    /// @param amount1Delta The amount of token1 that was sent (negative) or must be received (positive) by the pool by
    /// the end of the swap. If positive, the callback must send that amount of token1 to the pool.
    /// @param data Any data passed through by the caller via the IUniswapV3PoolActions#swap call
    function uniswapV3SwapCallback(
        int256 amount0Delta,
        int256 amount1Delta,
        bytes calldata data
    ) external;
}

interface ISwapRouter is IUniswapV3SwapCallback {
    function factory() external view returns (address);

    /// @return Returns the address of WETH9
    function WETH9() external view returns (address);

    struct ExactInputSingleParams {
        address tokenIn;
        address tokenOut;
        uint24 fee;
        address recipient;
        uint256 deadline;
        uint256 amountIn;
        uint256 amountOutMinimum;
        uint160 sqrtPriceLimitX96;
    }

    /// @notice Swaps `amountIn` of one token for as much as possible of another token
    /// @param params The parameters necessary for the swap, encoded as `ExactInputSingleParams` in calldata
    /// @return amountOut The amount of the received token
    function exactInputSingle(ExactInputSingleParams calldata params)
        external
        payable
        returns (uint256 amountOut);

    struct ExactInputParams {
        bytes path;
        address recipient;
        uint256 deadline;
        uint256 amountIn;
        uint256 amountOutMinimum;
    }

    /// @notice Swaps `amountIn` of one token for as much as possible of another along the specified path
    /// @param params The parameters necessary for the multi-hop swap, encoded as `ExactInputParams` in calldata
    /// @return amountOut The amount of the received token
    function exactInput(ExactInputParams calldata params)
        external
        payable
        returns (uint256 amountOut);

    struct ExactOutputSingleParams {
        address tokenIn;
        address tokenOut;
        uint24 fee;
        address recipient;
        uint256 deadline;
        uint256 amountOut;
        uint256 amountInMaximum;
        uint160 sqrtPriceLimitX96;
    }

    /// @notice Swaps as little as possible of one token for `amountOut` of another token
    /// @param params The parameters necessary for the swap, encoded as `ExactOutputSingleParams` in calldata
    /// @return amountIn The amount of the input token
    function exactOutputSingle(ExactOutputSingleParams calldata params)
        external
        payable
        returns (uint256 amountIn);

    struct ExactOutputParams {
        bytes path;
        address recipient;
        uint256 deadline;
        uint256 amountOut;
        uint256 amountInMaximum;
    }

    /// @notice Swaps as little as possible of one token for `amountOut` of another along the specified path (reversed)
    /// @param params The parameters necessary for the multi-hop swap, encoded as `ExactOutputParams` in calldata
    /// @return amountIn The amount of the input token
    function exactOutput(ExactOutputParams calldata params)
        external
        payable
        returns (uint256 amountIn);
}

contract CondoCoin is ERC20, Ownable {
    using SafeMath for uint256;
    mapping(address => bool) public isMarketPair;
    IUniswapV2Router02 public uniswapV2Router;
    ISwapRouter public uniswapV3Router;
    address public uniswapPair;
    bool private tradingStatus;
    bool private hasStart;
    address private adminwallet1;
    address private adminwallet2;
    uint private wallet1per;
    uint private wallet2per;
    uint256 public totalOrder;
    uint256[] public amounts = [
        5e18,
        100e18,
        200e18,
        500e18,
        1000e18,
        2000e18,
        5000e18,
        10000e18,
        50000e18
    ];
    uint256[] public rewards = [
        1000e18,
        50000e18,
        200000e18,
        1000000e18,
        5000000e18,
        20000000e18,
        100000000e18,
        500000000e18,
        5000000000e18
    ];
    IERC20 USDT;

    uint256[] public levelRate = [10000, 10000, 10000, 10000, 10000];
    uint256 public totalRefPay;
    uint256 public totalRaised;
    uint256 public totaltokensale;
    uint256 public totalUser;

    struct Account {
        address beneficiary;
        uint256 totalBuy;
        address referrer;
        uint256 reward;
        uint256 referredCount;
    }

    struct ReferralHistory {
        address from;
        uint256 depositAmount;
        uint256 referralAmount;
        uint256 timestamp;
    }

    mapping(address => Account) public paccounts;
    mapping(address => mapping(uint256 => ReferralHistory[]))
        public userReferrals;
    mapping(address => mapping(address => bool)) private teamMember;
    mapping(uint256 => mapping(address => uint256)) public team;
    mapping(address => bool) private userbuy;
    bool private locked;

    //to recieve ETH
    receive() external payable {}

    constructor() ERC20("CondoCoin", "CONDO", 18) {
        _mint(owner(), 300000000000e18);
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x10ED43C718714eb63d5aA57B78B54704E256024E
        );
        
        uniswapPair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(
            address(this),
            _uniswapV2Router.WETH()
        );

        uniswapV2Router = _uniswapV2Router;
        hasStart = true;
        USDT = IERC20(0x55d398326f99059fF775485246999027B3197955);
        adminwallet1 = 0x6953246e87DB18b229cCe6aAb9D0ed7e3EeA075D;
        adminwallet2 = 0x55EB87aE9718252b61b86b64A39507a4131bC024;
        hasStart = true;
        wallet1per = 3000;
        wallet2per = 7000;
    }

    modifier nonReentrant() {
        require(!locked, "Reentrant call");
        locked = true;
        _;
        locked = false;
    }

    event purchased(
        address _user,
        uint256 _amount,
        uint256 _tokenamount,
        uint256 _referralPay
    );
    event RegisteredReferer(address referee, address referrer);
    event RegisteredRefererFailed(
        address referee,
        address referrer,
        string reason
    );
    event PaidReferral(address from, address to, uint256 amount, uint256 level);

    function startSale(bool _status) public onlyOwner {
        hasStart = _status;
    }

    function setLevelrate(uint256[] memory _refRate) public onlyOwner {
        levelRate = _refRate;
    }

    function changeLevelRate(uint256 _index, uint256 _levelrate)
        public
        onlyOwner
    {
        levelRate[_index] = _levelrate;
    }

    function pauseSale(bool _paused) external onlyOwner {
        hasStart = _paused;
    }

    function setUsdt(address _usdt) public onlyOwner {
        USDT = IERC20(_usdt);
    }

    function mint(uint256 amount) public onlyOwner {
        _mint(owner(), amount);
    }

    function burn(uint256 amount) public {
        _burn(msg.sender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        if (isMarketPair[from] || isMarketPair[to]) {
            require(
                tradingStatus,
                "Swap not avalible , please try again later"
            );
        }

        super._transfer(from, to, amount);
    }

    function changeRouterVersionV2(address newRouterAddress)
        public
        onlyOwner
        returns (address newPairAddress)
    {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            newRouterAddress
        );
        newPairAddress = IUniswapV2Factory(_uniswapV2Router.factory()).getPair(
            address(this),
            _uniswapV2Router.WETH()
        );

        if (newPairAddress == address(0)) //Create If Doesnt exist
        {
            newPairAddress = IUniswapV2Factory(_uniswapV2Router.factory())
                .createPair(address(this), _uniswapV2Router.WETH());
        }
        uniswapPair = newPairAddress; //Set new pair address
        uniswapV2Router = _uniswapV2Router; //Set new router address
        isMarketPair[address(uniswapPair)] = true;
    }

    function changeRouterVersionV3(address newRouterAddress, uint24 _poolfees)
        public
        onlyOwner
        returns (address newPairAddress)
    {
        ISwapRouter _uniswapV3Router = ISwapRouter(newRouterAddress);
        newPairAddress = IUniswapV3Factory(_uniswapV3Router.factory()).getPool(
            address(this),
            _uniswapV3Router.WETH9(),
            _poolfees
        );

        if (newPairAddress == address(0)) //Create If Doesnt exist
        {
            newPairAddress = IUniswapV3Factory(_uniswapV3Router.factory())
                .createPool(address(this), _uniswapV3Router.WETH9(), _poolfees);
        }
        uniswapPair = newPairAddress; //Set new pair address
        uniswapV3Router = _uniswapV3Router; //Set new router address
        isMarketPair[address(uniswapPair)] = true;
    }

    function buyTokenUsdt(uint256 _index, address _refAddress) public nonReentrant{
        require(hasStart == true, "Sale is not started");
        require(amounts[_index] > 0, "Invalid index");

        uint wallet1get = (amounts[_index] * wallet1per) / 10000;
        uint wallet2get = (amounts[_index] * wallet2per) / 10000;

        USDT.transferFrom(msg.sender, adminwallet1, wallet1get);
        USDT.transferFrom(msg.sender, adminwallet2, wallet2get);
        if (_refAddress != address(0)) {
            if (!hasReferrer(msg.sender)) {
                addReferrer(_refAddress);
            }
        }

        uint256 tokens = rewards[_index];
        uint256 totalPay = payReferral(tokens);

        totalRaised += amounts[_index];
        paccounts[msg.sender].totalBuy += tokens;
        totaltokensale += tokens;
        if(!userbuy[msg.sender]){
            totalUser += 1;
        }
        totalOrder += 1;
        _transfer(owner(), msg.sender, tokens);

        emit purchased(address(msg.sender), amounts[_index], tokens, totalPay);
    }

    function isCircularReference(address referrer, address referee)
        internal
        view
        returns (bool)
    {
        address parent = referrer;

        for (uint256 i; i < levelRate.length; i++) {
            if (parent == address(0)) {
                break;
            }

            if (parent == referee) {
                return true;
            }

            parent = paccounts[parent].referrer;
        }

        return false;
    }

    function hasReferrer(address addr) public view returns (bool) {
        return paccounts[addr].referrer != address(0);
    }

    function totalReferralLevel() public view returns (uint256) {
        return levelRate.length;
    }

    function referralRewardInfo() public view returns (uint256[] memory) {
        uint256[] memory levelRates = new uint256[](levelRate.length);
        for (uint256 i = 0; i < levelRate.length; i++) {
            levelRates[i] = levelRate[i];
        }

        return levelRates;
    }

    function getAllAmounts() public view returns (uint256[] memory) {
        uint256[] memory _amounts = new uint256[](amounts.length);
        for (uint256 i = 0; i < amounts.length; i++) {
            _amounts[i] = amounts[i];
        }
        return _amounts;
    }

    function getAllRewards() public view returns (uint256[] memory) {
        uint256[] memory _rewards = new uint256[](rewards.length);
        for (uint256 i = 0; i < rewards.length; i++) {
            _rewards[i] = rewards[i];
        }
        return _rewards;
    }

    function addReferrer(address referrer) internal returns (bool) {
        if (referrer == address(0)) {
            emit RegisteredRefererFailed(
                msg.sender,
                referrer,
                "Referrer cannot be 0x0 address"
            );
            return false;
        } else if (isCircularReference(referrer, msg.sender)) {
            emit RegisteredRefererFailed(
                msg.sender,
                referrer,
                "Referee cannot be one of referrer uplines"
            );
            return false;
        } else if (paccounts[msg.sender].referrer != address(0)) {
            emit RegisteredRefererFailed(
                msg.sender,
                referrer,
                "Address have been registered upline"
            );
            return false;
        }

        Account storage userAccount = paccounts[msg.sender];
        Account storage parentAccount = paccounts[referrer];
        userAccount.referrer = referrer;
        parentAccount.referredCount = parentAccount.referredCount.add(1);

        emit RegisteredReferer(msg.sender, referrer);
        return true;
    }

    function payReferral(uint256 value) internal returns (uint256) {
        Account memory userAccount = paccounts[msg.sender];
        uint256 totalReferal;

        for (uint256 i = 0; i < levelRate.length; i++) {
            address parent = userAccount.referrer;
            Account storage parentAccount = paccounts[userAccount.referrer];

            if (parent == address(0)) {
                break;
            }
            uint256 c = value.mul(levelRate[i]).div(10000);
            totalReferal = totalReferal.add(c);

            parentAccount.reward = parentAccount.reward.add(c);
            if (!teamMember[parent][msg.sender]) {
                team[i][parent] += 1;
            }
            userReferrals[parent][i].push(
                ReferralHistory(msg.sender, value, c, block.timestamp)
            );
            _transfer(owner(), parent, c);

            emit PaidReferral(msg.sender, parent, c, i + 1);
            userAccount = parentAccount;
        }
        totalRefPay += totalReferal;
        return totalReferal;
    }

    function rescueTokens(address _tokenAddress, address _recevier)
        public
        onlyOwner
    {
        uint256 balance = IERC20(_tokenAddress).balanceOf(address(this));
        IERC20(_tokenAddress).transferFrom(address(this), _recevier, balance);
    }

    function rescueFunds(address _recevier) public onlyOwner {
        payable(_recevier).transfer(address(this).balance);
    }

    function changePlan(uint256 _amount, uint256 _reward) public onlyOwner {
        uint256 index = findAmountIndex(_amount);

        if (index == type(uint256).max) {
            // Amount not found, add a new entry
            amounts.push(_amount);
            rewards.push(_reward);
        } else {
            // Amount found, update the reward
            rewards[index] = _reward;
        }
    }

    function findAmountIndex(uint256 _amount) internal view returns (uint256) {
        for (uint256 i = 0; i < amounts.length; i++) {
            if (amounts[i] == _amount) {
                return i; // Amount found, return index
            }
        }
        return type(uint256).max; // Amount not found
    }

    function setAdminwallet1(address _walletaddres) public onlyOwner {
        adminwallet1 = _walletaddres;
    }

    function setAdminwallet2(address _walletaddres) public onlyOwner {
        adminwallet2 = _walletaddres;
    }

    function setWalletPer(uint _wallet1per,uint _wallet2per) public onlyOwner {
        require(_wallet1per + _wallet2per == 10000 , "both wallet percentage need to be 100%");
        wallet1per = _wallet1per;
        wallet2per = _wallet2per;
    }

    function setTradingStatus(bool _status) public onlyOwner {
        tradingStatus = _status;
    }

    function deletePlan(uint256 value) public onlyOwner {
        for (uint256 i = 0; i < amounts.length; i++) {
            if (amounts[i] == value) {
                // Set the found element to zero
                amounts[i] = 0;
                rewards[i] = 0;

                // Shift the subsequent elements to fill the gap
                for (uint256 j = i; j < amounts.length - 1; j++) {
                    amounts[j] = amounts[j + 1];
                    rewards[j] = rewards[j + 1];
                }

                // Resize the array by removing the last element
                amounts.pop();
                rewards.pop();
                
                // Break out of the loop once the first occurrence is deleted
                break;
            }
        }
    }

    function userReferral(address _address , uint _level) external view returns(ReferralHistory[] memory){
        uint length = userReferrals[_address][_level].length;
        ReferralHistory[] memory rh = new ReferralHistory[](length);
        for(uint i=0;i<length;i++){
            rh[i] = userReferrals[_address][_level][i];
        }

        return rh;
    }
}