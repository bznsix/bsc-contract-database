    // mainnet
     IERC1155 public pixoNftV1 = IERC1155(0x7B700806Fd58df88F71b87eA68a80c49f8E3f296);

    //IERC1155 public pixoNftV1 = IERC1155(0x404C821e773528b776E2AfB22a86184F0f73A7b0);
    IERC1155 public pixoNftV2 = IERC1155(0x04777a6840E60848C333C36D01ca5917441b9dd1);


    function pause() public onlyOwner {
      _pause();
    }
    
    function unpause() public onlyOwner {
      _unpause();
    }

    function recoverLeftOverEth(uint256 amount) external onlyOwner {
        payable(owner()).transfer(amount);
    }

    function recoverLeftOverToken(address token,address to,uint256 amount) external onlyOwner {
        IERC20(token).transfer(to,amount);
    }

    function setPixoNftV1(address nftContract) external onlyOwner {
        pixoNftV1 = IERC1155(nftContract);
    }

    function setPixoNftV2(address nftContract) external onlyOwner {
        pixoNftV2 = IERC1155(nftContract);
    }

    function getSwapData(address account,uint256 id,uint256 supply) internal  pure returns (bytes memory) {
        return  abi.encodeWithSignature("swap(address,uint256,uint256)",account, id, supply);
    }

    function swap(uint256 id,uint256 supply) external whenNotPaused {
        address account = _msgSender();
        pixoNftV1.safeTransferFrom(account,address(this),id,supply, "");
        pixoNftV2.swap(account,id,supply);
    }

}