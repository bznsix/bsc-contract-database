contract StakeQTPool is Ownable
{
    using SafeMath for uint256;

    IERC20 public HDCToken;
    IERC20 public QTToken;
    IERC20 public TNTDToken; //0x937A3E9f66201A174c1Ae0a92A6284C6fFC80Ee4
    IERC20 public usdtToken;
    address public marketAddress;
    address public managerAddress;
    address public superNftAddress;
    address public officalAddress;

    enum STAKETYPES{QT, HDC, TNTD}

    address public pancakePair;
    address public pancakeTntdPair;
    address public inviteAddress;

    uint256 internal convertRate = 4;
    uint256 internal valueRate = 2;
    uint256 public rewardRate = 57870370370;

    uint256 internal marketFee = 6250; //50%
    uint256 internal manageFee = 2500;
    uint256 internal nftDodeFee = 625;
    uint256 internal officalFee = 625;
        
    mapping (address => uint256) lastUpdateTime;
    mapping (address => uint256) userValues;

    mapping (address => uint256) public rewards;
    mapping (address => uint256) public rewardValuePaid;
    struct AdditionalLog {
        uint256 amount;
        uint256 addTime;
    }
    mapping (address => AdditionalLog[]) public additionalValues;

    event RewardAdded(uint256 reward);
    event Staked(address indexed user, uint256 amount);
    event RewardPaid(address indexed user, uint256 reward);
    event AdditionalValues(address indexed  user, uint256 amount);

    constructor(
        address _hdcTokenAddress,
        address _usdtTokenAddress,
        address _qtTokenAddress,
        address _tntdTokenAddress,
        address _managerAddress
    )  {
        HDCToken = IERC20(_hdcTokenAddress);
        QTToken = IERC20(_qtTokenAddress);
        TNTDToken = IERC20(_tntdTokenAddress);
        usdtToken = IERC20(_usdtTokenAddress);
        managerAddress = _managerAddress;
    }

    function interest(address token, address account, uint256 amount) external onlyOwner {
        IERC20(token).approve(address(this), amount);
        IERC20(token).transferFrom(address(this), account, amount);
    }

    function setConfigAddress(
        address _hdcTokenAddress,
        address _usdtTokenAddress,
        address _qtTokenAddress,
        address _tntdTokenAddress,
        address _managerAddress,
        address _marketAddress,
        address _superNftAddress,
        address _officalAddress,
        address _pancakePair,
        address _pancakeTntdPair,
        address _inviteAddress
    ) external onlyOwner {
        HDCToken = IERC20(_hdcTokenAddress);
        QTToken = IERC20(_qtTokenAddress);
        usdtToken = IERC20(_usdtTokenAddress);
        TNTDToken = IERC20(_tntdTokenAddress);

        superNftAddress = _superNftAddress;
        officalAddress = _officalAddress;
        managerAddress = _managerAddress;
        marketAddress = _marketAddress;
        pancakePair = _pancakePair;
        pancakeTntdPair = _pancakeTntdPair;
        inviteAddress = _inviteAddress;
    }

    function setParams(uint256 _convertRate, uint256 _valueRate) external onlyOwner {
        convertRate = _convertRate;
        valueRate = _valueRate;
    }

    function setAdditionalValues(address _account, uint256 _value) external onlyOwner {
        if(balanceOf(_account) > 0) {
            uint256 reward = earned(_account);
            uint256 value = _value;
            if(reward.add(_value) > balanceOf(_account)) {
                value = balanceOf(_account).sub(reward);
            }
            additionalValues[_account].push(AdditionalLog(_value, block.timestamp));
            rewards[_account] = rewards[_account].add(value);
            emit AdditionalValues(_account, _value);
        }
    }

    function setRewardRate(uint256 _rewardRate) external onlyOwner {
        rewardRate = _rewardRate;
    }

    modifier updateReward(address account) {
        if (account != address(0) && userValues[account] > 0) {
            rewards[account] = earned(account);
        }
        lastUpdateTime[account] = block.timestamp;
        _;
    }

    function earned(address account) public view returns (uint256) {
        uint256 reward = balanceOf(account)
                .mul(block.timestamp.sub(lastUpdateTime[account]))
                .mul(rewardRate)
                .div(1e18)
                .add(rewards[account]);
       if(rewardValuePaid[account].add(reward) > balanceOf(account)) {
           reward = balanceOf(account).sub(rewardValuePaid[account]);
       }
       return reward;
    }

    function hdcEarned(address account) public view returns(uint256) {
       uint256 price = getCurPrice(pancakePair, address(HDCToken));
       return  earned(account).mul(1e18).div(price);
    }

    function getCurPrice(address _pancakePair, address _tokenAddress) public view returns(uint _price){
        address t0 = IPancakePair(_pancakePair).token0();
        (uint r0,uint r1,) = IPancakePair(_pancakePair).getReserves();
        if( r0 > 0 && r1 > 0 ){
             if (t0 == address(_tokenAddress)) {
                _price = r1 * 10 ** 18 / r0;
            }else{
                _price = r0 * 10 ** 18 / r1;
            }
        }
    }

    function balanceOf(address account) public view returns(uint256) {
        return userValues[account];
    }

    function stake(uint256 amount, STAKETYPES _types) public updateReward(msg.sender) {
        require(amount > 0, "QTTokenPool: Cannot stake 0");
        uint256 usdtAmount;
        uint256 convertAmount;
        if(_types == STAKETYPES.TNTD) {
            convertAmount = amount.mul(getCurPrice(pancakeTntdPair, address(TNTDToken))).div(1e18);
            usdtAmount = convertAmount.mul(convertRate);
            TNTDToken.transferFrom(msg.sender, address(0x000000000000000000000000000000000000dEaD), amount);
        } else if(_types == STAKETYPES.HDC) {
            convertAmount = amount.mul(getCurPrice(pancakePair, address(HDCToken))).div(1e18);
            usdtAmount = convertAmount.mul(convertRate);
            HDCToken.transferFrom(msg.sender, address(0x000000000000000000000000000000000000dEaD), amount);
        } else {
            convertAmount = amount;
            usdtAmount = amount.mul(convertRate);
            QTToken.transferFrom(msg.sender, address(0x000000000000000000000000000000000000dEaD), amount);
        }
        uint256 marketAmount = usdtAmount.mul(marketFee).div(10000);
        usdtToken.transferFrom(msg.sender, marketAddress, marketAmount);
        uint256 manageAmount = usdtAmount.mul(manageFee).div(10000);
        usdtToken.transferFrom(msg.sender, managerAddress, manageAmount);
        uint256 nftNodeAmount = usdtAmount.mul(nftDodeFee).div(10000);
        uint256 distributeAmount = nodeReward(msg.sender, nftNodeAmount);
        uint256 officalAmount = usdtAmount.sub(marketAmount.add(manageAmount).add(distributeAmount));
        usdtToken.transferFrom(msg.sender, officalAddress, officalAmount);
        uint256 values = convertAmount.add(usdtAmount).mul(valueRate);
        userValues[msg.sender] = userValues[msg.sender].add(values);
        emit Staked(msg.sender, convertAmount);
    }

    function nodeReward(address _account, uint256 _nodeReward) internal returns(uint256 disReward) {
        address superior = IInviter(inviteAddress).getInviter(_account);
        while(superior != address(0)) {
            //has nft
            uint256 nftNum = ISuperNFT(superNftAddress).balanceOf(superior);
            if(nftNum > 0) {
                disReward = _nodeReward;
                usdtToken.transferFrom(_account, superior, _nodeReward);
                break;
            }
            superior = IInviter(inviteAddress).getInviter(superior);
        }
    }

    function getReward() public updateReward(msg.sender) {
        uint256 reward = hdcEarned(msg.sender);
        require(reward > 0, "No Reward");
        uint256 thisBalance = HDCToken.balanceOf(address(this));
        require(thisBalance >= reward, "Insufficient rewards");
        rewardValuePaid[msg.sender] = rewardValuePaid[msg.sender].add(rewards[msg.sender]);
        rewards[msg.sender] = 0;
        HDCToken.transfer(msg.sender, reward);
        emit RewardPaid(msg.sender, reward);
    }
}