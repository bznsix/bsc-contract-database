 function calculateSwapAmount(  uint256 amount
    ) internal view returns (uint256) {
    // Calculate the swap charge
    uint256 swapChargeAmount = (amount * SwapCharge) / 100;
    // Calculate the net amount after deducting the swap charge
    uint256 netAmount = amount - swapChargeAmount;
        return netAmount;
    }
