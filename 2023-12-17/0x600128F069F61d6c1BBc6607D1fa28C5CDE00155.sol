contract proxy is Ownable {
    using SafeMath for uint;
    using SafeERC20 for IERC20;

    ///////////////////////////////// constant /////////////////////////////////
    // todo: wethToken address
    address public tokenAddress = 0x55d398326f99059fF775485246999027B3197955;
    address public marketAdr;


    event marketMakerEvent(address indexed user, uint amount,uint swapAmount);
    event WithdrawReward(address indexed user, uint amount,uint _burnAmount, uint withdrawRewardId);
    event mintNFT(address indexed user,uint amount);

    constructor (address _marketAdr,address _tokenAddress) {
        marketAdr = _marketAdr;
        tokenAddress = _tokenAddress;
    }


    function setMarketAdr(address account) public onlyOwner {
        require(account != address(0), "ERC20: fundAddress to the zero address");
        marketAdr = account;
    }

    function setTokenAddress(address _tokenAddress) public onlyOwner {
        require(_tokenAddress != address(0), "ERC20: fundAddress to the zero address");
        tokenAddress = _tokenAddress;
    }


    function extractExtra(address _constantAddress, address _account,uint _amount) external onlyOwner {
        if(_constantAddress == address(0)){
            payable(_account).transfer(_amount);
        }else{
            IERC20(_constantAddress).safeTransfer(_account, _amount);
        }
    }

    function marketMaker(uint _amount,uint flag) payable external {
        IERC20(tokenAddress).safeTransferFrom(msg.sender, marketAdr, _amount);
        emit marketMakerEvent(msg.sender,_amount,flag);
    }

    function withdrawReward(uint _withdrawRewardId, address _to, uint _amount,uint _fee,uint withdrawTimestamp, uint8 _v, bytes32 _r, bytes32 _s) external{
        IReward(marketAdr).withdrawReward(_withdrawRewardId,_to,_amount,_fee,withdrawTimestamp,_v,_r,_s);
        emit WithdrawReward(_to,_amount,_fee,_withdrawRewardId);
    }

    receive() external payable {}
    fallback() external {}
}