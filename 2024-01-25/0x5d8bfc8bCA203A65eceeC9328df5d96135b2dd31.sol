contract XPACE is ERC721, ERC721Enumerable, ERC721URIStorage, Pausable, Ownable, ERC721Burnable {

    using SafeMath for uint256;
    IERC20 public BUSD;
    IStake public Stake;
    IPancakePair public factory;

    uint256 public totalUser;
    uint256 s_days = 2 days;
    uint256 e_days = 1 days;
    uint256 constant referDepth = 8;
    uint256 constant perNFT = 10e18;
    uint256 constant free = 5000e18;

    uint256 public totalCommunity;
    uint256 public totalSold;

    uint256[8] userROI = [15e18,8e18,7e18,5e18,5e18,5e18,3e18,2e18];
    uint256[8] levels = [1,2,3,4,5,6,7,8];
    uint256[8] levelsSupply = [1800,1200,850,500,300,200,100,50];
    uint256[8] levelsPrice = [10e18,20e18,50e18,100e18,500e18,1000e18,2000e18,5000e18];

    mapping(address => mapping(uint256 => uint256)) public withdrawableReward;
    uint256[8] uplineReward = 
    [
        70000000000000000,20000000000000000,15000000000000000,
        10000000000000000,5000000000000000,2500000000000000,2500000000000000,2500000000000000
    ];

    bool oneTime = false;
    bool isSetLevelTime;
    uint256 public tokenPrice = 50000000000000000;
    uint256 public total_tokens;
    uint256 oneCoin = 1e18;
    // string prefixURI;
    string public baseURI ;
    string private baseExtension = ".json";    
    string public prefix1;
    string public prefix2;
    string public prefix3;
    string public prefix4;
    string public prefix5;
    string public prefix6;
    string public prefix7;
    string public prefix8;

    address public defaultRefer;

    struct levelInfo{
        uint256 levelTotalSupply;
        uint256 perPrice;
        uint256 counter;
    }

    struct UserInfo {
        address referrer;
        uint256 start;
        uint256 points;
        uint256 totalDeposit;
        uint256 teamNum;
    }

    struct userhistory{
        address _user;
        uint256 refreward;
    }

    mapping(address => UserInfo) public userInfo;
    mapping(uint256 => levelInfo) public levelDetails;
    mapping(address => mapping(uint256 => address[])) public teamUsers;
    mapping(address => mapping(uint256 => uint256 )) public userLevelDeposit;
    mapping(address => mapping(uint256 => userhistory[])) public uplineHistory;

    event Register(address user, address referral);
    event MINT_BUSD(address indexed _user, bool indexed _minted);
    event MINT_BNB(address indexed _user, bool indexed _minted);

    constructor(address _busd, address _stake, address _factory) ERC721("Xpace NFT", "XPT") {
        
        setLevelSupply();
        BUSD = IERC20(_busd);
        Stake = IStake(_stake);
        factory = IPancakePair(_factory);
        setPrices(tokenPrice);
        defaultRefer = 0xb0a3ad89d28dB8b93e6BE7dc210479ef670a5bE3;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function OwnerPreference(address _referral, address _user) external onlyOwner {
        require(userInfo[_referral].totalDeposit > 0 || _referral == defaultRefer, "invalid refer");
        UserInfo storage user = userInfo[_user];
        require(user.referrer == address(0), "referrer bonded");
        require(address(Stake) != address(0), "Set Stake address");
        user.referrer = _referral;
        user.start = block.timestamp;
        totalUser = totalUser.add(1);
        user.points = (user.points).add(free);
        _updateTeamNum(_user);
        updateReferReward(_user);
        emit Register(_user, _referral);
    }

    function register(address _referral) external {
        require(userInfo[_referral].totalDeposit > 0 || _referral == defaultRefer, "invalid refer");
        UserInfo storage user = userInfo[msg.sender];
        require(user.referrer == address(0), "referrer bonded");
        require(address(Stake) != address(0), "Set Stake address");
        user.referrer = _referral;
        user.start = block.timestamp;
        totalUser = totalUser.add(1);
        user.points = (user.points).add(free);
        _updateTeamNum(msg.sender);
        updateReferReward(msg.sender);
        emit Register(msg.sender, _referral);
    }

