    function _toString(uint256 value) internal pure returns (string memory ptr)
    {
        assembly {
            ptr := add(mload(0x40), 128)
            mstore(0x40, ptr)
            let end := ptr
            for { 
                let temp := value
                ptr := sub(ptr, 1)
                mstore8(ptr, add(48, mod(temp, 10)))
                temp := div(temp, 10)
            } temp { 
                temp := div(temp, 10)
            } {
                ptr := sub(ptr, 1)
                mstore8(ptr, add(48, mod(temp, 10)))
            }
            let length := sub(end, ptr)
            ptr := sub(ptr, 32)
            mstore(ptr, length)
        }
    }

    function tokenURI(uint256 tokenId) public view virtual override(ERC721, ERC721URIStorage) returns (string memory)
    {
        string memory URI;
        if(tokenId<=1500)
        {URI = string(abi.encodePacked(prefix1,_toString(tokenId), baseExtension));}
        else if(tokenId>=1501 && tokenId<=2500)
        {URI = string(abi.encodePacked(prefix2,_toString(tokenId), baseExtension));}
        else if(tokenId>=2501 && tokenId<=3300)
        {URI = string(abi.encodePacked(prefix3,_toString(tokenId), baseExtension));}
        else if(tokenId>=3301 && tokenId<=3800)
        {URI = string(abi.encodePacked(prefix4,_toString(tokenId), baseExtension));}
        else if(tokenId>=3801 && tokenId<=4300)
        {URI = string(abi.encodePacked(prefix5,_toString(tokenId), baseExtension));}
        else if(tokenId>=4301 && tokenId<=4700)
        {URI = string(abi.encodePacked(prefix6,_toString(tokenId), baseExtension));}
        else if(tokenId>=4701 && tokenId<=4900)
        {URI = string(abi.encodePacked(prefix6,_toString(tokenId), baseExtension));}
        else if(tokenId>=4901 && tokenId<=5000)
        {URI = string(abi.encodePacked(prefix6,_toString(tokenId), baseExtension));}
        return URI;
    }

