            _mint(address(Stake), tokenId);
            Stake.lockNFT(_user, _level, tokenId, _coins, _time);
        }

        totalSold = totalSold.add(_count);

        user.points = (user.points).sub(_totalPoints);
        user.totalDeposit = user.totalDeposit.add(_totalPrice);
        userLevelDeposit[_user][_level] = userLevelDeposit[_user][_level].add(_totalPrice);
        updateWithdraw(_user, _dollers);
        
        levelDetails[_level].counter = levelDetails[_level].counter.add(_count);
        emit MINT_BNB(_user, true);
    }
    
    function Mint_BNB(uint256 _level, uint256 _count) 
    public payable
    {
        UserInfo storage user = userInfo[msg.sender];
        require(user.referrer != address(0), "register first");
        require(_level >= 1 && _level <= 8, "Invalid Level");
        require((levelDetails[_level].levelTotalSupply) >= (levelDetails[_level].counter) + _count, "Low Supply");

        (uint256 bnbPrice, uint256 _perBNB) = getBNBPrice(_level, _count);
