    function priceCheck(uint256 _level, uint256 _count) public view returns(uint256,uint256) {
        uint256 _levelPrice = levelDetails[_level].perPrice;
        uint256 _totalPrice = _levelPrice.mul(_count);
        uint256 _coinPrice = (_totalPrice.mul(perNFT)).div(1e18);
        return (_totalPrice,_coinPrice);
    }

    function getPoints(uint256 _level) public view returns(uint256){
        uint256 _price = levelsPrice[_level - 1];
        uint256 _coins;
        _coins = (_price.mul(perNFT)).div(1e18);
        return _coins;
    }

    function getBNBTokens(uint256 _total, uint256 _per) public view returns(uint256, uint256){
        uint256 _dollers = (_total.mul(1e18)).div(_per);
        uint256 _tokens = (_dollers.mul(total_tokens)).div(1e18);
        return (_tokens, _dollers);
    }
    
    function Mint_BUSD(uint256 _level, uint256 _count, uint256 amount_) 
    public
    {
        UserInfo storage user = userInfo[msg.sender];
        require(user.referrer != address(0), "register first");
        require(_level >= 1 && _level <= 8, "Invalid Level");

        (uint256 _totalPrice, uint256 _amount) = priceCheck(_level, _count);

        require(user.points >= _amount, "low balance");
        require(_totalPrice <= amount_, "amount mis_matched");
        require((levelDetails[_level].levelTotalSupply) >= (levelDetails[_level].counter) + _count, "Low Supply");
        BUSD.transferFrom(msg.sender, address(this), amount_);
        userLevelDeposit[msg.sender][_level] = userLevelDeposit[msg.sender][_level].add(_totalPrice);
        uint256 _coins = getPoints(_level);
        address _user = msg.sender;
        
        if(user.totalDeposit != 0){
            totalCommunity = totalCommunity.add(1);
        }

        user.totalDeposit = user.totalDeposit.add(amount_);
        uint256 _tokenId;
        _tokenId = levelDetails[_level].counter;
        for(uint256 i; i< _count; i++){
            _tokenId +=1;
            _mint(address(Stake), _tokenId);
            Stake.lockNFT(_user, _level, _tokenId, _coins,block.timestamp);
        }

        totalSold = totalSold.add(_count);

        user.points = (user.points).sub(_amount);
        updateWithdraw(msg.sender, _totalPrice);
        
        levelDetails[_level].counter = levelDetails[_level].counter.add(_count);
        emit MINT_BUSD(msg.sender, true);
    }

    function getBNBPrice(uint256 _level, uint256 _count) public view returns(uint256, uint256){
        uint256 _Price = levelDetails[_level].perPrice;
        (uint256 reserve1, uint256 reserve2, ) = factory.getReserves();
        uint256 _perBNB = (reserve1.mul(1e18)).div(reserve2);
        uint256 _levelPrice = _Price.mul(_count);
        uint256 _totalPrice = (_levelPrice.mul(_perBNB)).div(1e18);
        // uint256 _totalPoints = (_totalPrice.mul(perNFT)).div(1e18);
        return (_totalPrice, _perBNB);
    }

