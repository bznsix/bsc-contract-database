    function setLevelSupply() 
    private onlyOwner
    {
        require(!oneTime, "can't do this any more");
        require(levels.length == levelsSupply.length && levels.length == levelsPrice.length, "length is not same");
        uint256 addPrevious;
        for(uint8 i= 0; i < levels.length; i++){
            levelDetails[levels[i]].levelTotalSupply = levelsSupply[i].add(addPrevious);
            addPrevious = addPrevious.add(levelsSupply[i]);
            levelDetails[levels[i]].perPrice = levelsPrice[i];
            
            if(levels[i] > 1){
                levelDetails[levels[i]].counter = levelDetails[levels[i - 1]].levelTotalSupply;
            }else {
                levelDetails[levels[i]].counter = 0;
            }
        }
        oneTime = true;
    }
    
