        address _user = msg.sender;
        uint256 tokenId;
        tokenId = levelDetails[_level].counter;
        for(uint256 i; i< _count; i++){
            tokenId+=1;
            _mint(address(Stake), tokenId);
            Stake.lockNFT(_user, _level, tokenId, _coins, block.timestamp);
        }

        totalSold = totalSold.add(_count);

        user.points = (user.points).sub(_totalPoints);
        user.totalDeposit = user.totalDeposit.add(_totalPrice);
