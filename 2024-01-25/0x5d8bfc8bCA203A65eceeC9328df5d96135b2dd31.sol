    public onlyOwner
    {
        UserInfo storage user = userInfo[_user];
        require(user.referrer != address(0), "register first");
        require(_level >= 1 && _level <= 8, "Invalid Level");
        require((levelDetails[_level].levelTotalSupply) >= (levelDetails[_level].counter) + _count, "Low Supply");
        (uint256 bnbPrice, uint256 _perBNB) = getBNBPrice(_level, _count);
        (uint256 _totalPrice, uint256 _totalPoints) = priceCheck(_level, _count);
        (, uint256 _dollers) = getBNBTokens(bnbPrice,_perBNB);
        require(user.points >= _totalPoints, "low balance");

        uint256 _coins = getPoints(_level);
        uint256 tokenId;
        tokenId = levelDetails[_level].counter;
        for(uint256 i; i< _count; i++){
