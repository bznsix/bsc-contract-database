contract BulletStake is ReentrancyGuard, Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;

    Counters.Counter private _stakerCount;

    struct Stake {
        uint256 index;
        bool staked;
        uint256 amount;
        uint256 claimed;
        uint256 timestamp;
    }

    IERC20 private token = IERC20(0x1CF8ee201b7152d4d33d72eca1250515e4F56b96);

    uint256 private   _totalStakedBalance;
    uint256 private   _totalStakerRewards;
    uint256[] private _indexes;
    uint256 private   _que = 0;
    uint256 private   _limit = 7;
    bool private      _magnitude = false;

    mapping(address => Stake) private staker;
    mapping(uint256 => address) private _indexedAddress;


    event Staked(address indexed user, uint256 index, uint256 amount);
    event Unstaked(address indexed user, uint256 index, uint256 amount);

    constructor()  {
        transferOwnership(msg.sender);
    }

    modifier allowedTokenHolder() {
        require(token.balanceOf(msg.sender) > 0, "ERROR: No tokens too stake!");
           _;
    }

    modifier isStaker() {
        require(staker[msg.sender].staked, "ERROR: Not a staker!");
           _;
    }

    function getPoolBalance() external  view returns (uint256) {
        return token.balanceOf(address(this));
    }

    function getStaker(address owner) public view returns(Stake memory) {
        return staker[owner];
    }

    function getQue() public view returns(uint256) {
        return _que;
    }
    
    function getLimit() public view returns(uint256) {
        return _limit;
    }

    function isMagnitutedEnebaled() public view returns(bool) {
        return _magnitude;
    }

    function getCurrentRewards() public view returns(uint256) {
        return _totalStakerRewards;
    }

    function isRegistered(address owner) public view returns(bool) {
        return staker[owner].staked;
    }

    function getClaimed(address owner) external view returns(uint256){
        return staker[owner].claimed;
    }

    function getStakedAmount(address owner) external view returns(uint256) {
        return staker[owner].amount;
    }

    function getTotalStakersCount() external  view returns(uint256) {
        return _stakerCount.current();
    }
    
    function getTotalStaked() public view returns(uint256) {
        return _totalStakedBalance;
    }

    function getOwnersShares(address owner) public view returns(uint256) {
          return fromX(staker[owner].amount, _totalStakedBalance, toWei(100));
    }

    function getSharesFromAmount(uint256 _amount, address owner) public view returns(uint256) {
          uint256 sharePercetage =  fromX(staker[owner].amount, _totalStakedBalance, toWei(100));
          return  _amount * sharePercetage / toWei(100);
    }

    function getShareAmount(uint256 _amount, uint256 percetage, uint256 scale) public pure returns(uint256) {
        return _amount * percetage / scale;
    }

    function getOwenersPoolPercentage(address owner) public view returns(uint256) {
          return fromX(staker[owner].amount, _totalStakedBalance, toWei(100));
    }

    function setMagnifierAndLimit(uint256 limit, bool enabled) external onlyOwner {
        _limit = limit;
        _magnitude = enabled;
    }

    function stake(uint256 amount) external allowedTokenHolder() nonReentrant {
           uint256 _amount = token.allowance(msg.sender, address(this));
           require(_amount >= amount, "ERROR: NO ALLOWANCE");
           require(token.balanceOf(msg.sender) >= amount, "ERROR: NO ALLOWANCE");
           Stake storage _staker = staker[msg.sender];
           token.transferFrom(msg.sender, address(this), amount);
           _stakerCount.increment();
           _totalStakedBalance = _totalStakedBalance.add(amount);
           uint256 current = _stakerCount.current();

           if(_staker.index == 0) {_staker.index = current; _indexes.push(current); _indexedAddress[current] = msg.sender; } else {_staker.index = _staker.index;}

           _staker.staked = true;
           _staker.amount = _staker.amount.add(amount);
           _staker.timestamp = block.timestamp;
           _staker.claimed = _staker.claimed;

          emit Staked(msg.sender, current, amount);

    }

    function unstake() external isStaker() nonReentrant {
         uint256 _amount = staker[msg.sender].amount;
         require(staker[msg.sender].staked, "ERROR: Not a staker");
         require(block.timestamp >= staker[msg.sender].timestamp + 10 minutes);
         require(_amount > 0, "ERROR: No tokens too unstake");
         
         staker[msg.sender] = Stake(staker[msg.sender].index, false, staker[msg.sender].amount.sub(_amount), 0, staker[msg.sender].claimed.add(_amount));
         _totalStakedBalance = _totalStakedBalance.sub(_amount);
         token.transferFrom(msg.sender, address(this), _amount);
         removeIndex(staker[msg.sender].index);
         _stakerCount.decrement();
         emit Unstaked(msg.sender, staker[msg.sender].index, _amount);
    }

    function distribute() external {
        uint256 _amount = _totalStakerRewards;
        require(_amount > 0, "ERROR: NO REWARDS TOO DISTRBUTE!");
        require(token.balanceOf(address(this)) >= _amount, "ERROR: NO ALLOWANCE!");

        uint256[] memory _indexedIds = _indexes;
        
        uint256 supply = _indexes.length;
        uint256 _lastProcessedIndex = _que;
        uint256 iterations = 0;

        if(supply > 0) {
            if(_magnitude) {
                if(supply < _limit) _limit = supply; 

                while (iterations < _limit) {
                    _lastProcessedIndex++;

                    if (_lastProcessedIndex > supply) {
                        _lastProcessedIndex = 0;
                    }

                    Stake storage stakerX = staker[_indexedAddress[_indexedIds[_lastProcessedIndex]]];
                    if(stakerX.amount > 0 && stakerX.staked) {
                        uint256 sharePercetage =  fromX(stakerX.amount, _totalStakedBalance, toWei(100));
                        uint256 computedAmount =  _amount * sharePercetage / toWei(100);

                        token.transferFrom(address(this), _indexedAddress[_indexedIds[_lastProcessedIndex]], computedAmount);
                        stakerX.claimed = stakerX.claimed.add(computedAmount);
                        _totalStakerRewards = _totalStakerRewards.sub(computedAmount);
                    }
                    iterations++;
                }
                _que = _lastProcessedIndex;
            } else {
                for(uint256 index = 0; index < supply; index++) {
                     Stake storage stakerX = staker[_indexedAddress[_indexedIds[index]]];
                     if(stakerX.amount > 0 && stakerX.staked) {
                        uint256 sharePercetage =  fromX(stakerX.amount, _totalStakedBalance, toWei(100));
                        uint256 computedAmount =  _amount * sharePercetage / toWei(100);

                        token.transferFrom(address(this), _indexedAddress[_indexedIds[index]], computedAmount);
                        stakerX.claimed =stakerX.claimed.add(computedAmount);
                        _totalStakerRewards = _totalStakerRewards.sub(computedAmount);
                     }
                }
            }
        }
    }

    function importTokens(uint256 amount) external onlyOwner {
        uint256 _amount = token.allowance(msg.sender, address(this));
        require(_amount >= amount, "ERROR: NO ALLOWANCE");
        require(token.balanceOf(msg.sender) >= amount, "ERROR: NO ALLOWANCE");
        token.transferFrom(msg.sender, address(this), amount);
        _totalStakerRewards = _totalStakerRewards.add(amount);
    }

    function removeIndex(uint index) internal {
        require(_indexes.length > index, "NFT: Out of bounds");
        for (uint256 i = index; i < _indexes.length - 1; i++) {
            _indexes[i] = _indexes[i+1];
        }
        _indexes.pop();
    }

    function ratio(uint256 poolBalance, uint256 stakeringPool, uint256 balance, uint poolPercentage) private pure returns(uint256) {
            uint256 percentageShares = (100 * balance) / stakeringPool;
            uint256 poolshares = poolBalance * percentagecal(poolPercentage, 100, toWei(100));
            return  poolshares * percentagecal(percentageShares, 100, toWei(100));
    }
    
    function fromX(uint256 a, uint256 b, uint256 scale) internal  pure returns(uint256) {
        return (scale * a) / b;
    }

    function toWei(uint value) internal pure returns (uint) {
       return value*(10**18);
    }

    function toW(uint value) public pure returns (uint) {
       return value*(10**18);
    }

    function percentagecal(uint256 a, uint256 b, uint256 scale) private pure returns(uint256) {
        return (scale - a) / b;
    }
    
    function withdrawStuckTokens(address _address) payable external onlyOwner {
        IERC20(_address).transfer(owner(),  IERC20(_address).balanceOf(address(this)));
    }

    function withdraw() payable external onlyOwner {
        uint256 balance = address(this).balance;
        payable(address(owner())).transfer(balance);
    }

    
    receive() external payable {
    }

}