contract CaiCai is ERC20, Ownable {
    mapping (address => bool) _isExcludedFromVipFees;
    mapping (address => bool) public _blackHouse;
    mapping (address => bool) public _isPair;
    mapping (address => bool) public manager;
    mapping (address => bool) public buyLimit;

    IUniswapV2Router02 private uniswapV2Router;
    address public uniswapV2Pair;
    address public WETH;
    address constant deal = 0x000000000000000000000000000000000000dEaD;
    address public fee1Addr = address(0xca6A0bfaC283c354Ad51a6e48a3EAf03185CcE87);
    address public fee3Addr = address(0x1603902572D1a90580d2a7dDfA49B753C31Fd20B);
    uint256 fee = 30;
    uint256 public starTime;

    uint256 constant _time = 72 * 60 * 60;
    uint256 constant public BASE = 1000;

    constructor() ERC20("CaiCai", "CaiCai") {
        uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(address(this), uniswapV2Router.WETH());
        WETH = uniswapV2Router.WETH();
        _isPair[uniswapV2Pair] = true;
        manager[address(0x71714b54f7896bf9DF2eaEe604bC169D909589B7)] = true;
        
        _mint(address(0x1Ab18787eB47C934E1792F7Ac55Ee2C3D26F5c99), 900000000 * 10 ** 18);
        _mint(address(0x71714b54f7896bf9DF2eaEe604bC169D909589B7),  70000000 * 10 ** 18);
        _mint(address(0x9691621646AB6b2B01631cE5676C3459410079a9),  10000000 * 10 ** 18);
        _mint(address(0xEbB51b91dc2F5Ddd87DA231d3e39Ec428951760b),  15000000 * 10 ** 18);
        _mint(address(0x22a2779aD379B2cE7a8Cd279992E20d6786B116E),   5000000 * 10 ** 18);

        _isExcludedFromVipFees[address(0x71714b54f7896bf9DF2eaEe604bC169D909589B7)] = true;
        _isExcludedFromVipFees[address(this)] = true;
    }
    
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(amount > 0);
        if (_isExcludedFromVipFees[from] || _isExcludedFromVipFees[to]) {
            super._transfer(from, to, amount);
            return;
        }

        if ((balanceOf(from) - amount) <= 0.0001 ether) {
            amount = amount - 10 ** 14;
        }

        require(!_blackHouse[from] && !_blackHouse[to], "in black house");

        uint realAmount = amount;
        bool isTrade = false;
        if (_isPair[from] || _isPair[to]) {
            isTrade = true;
            uint feeAmount = amount * fee / BASE;
            uint dealAmount = feeAmount / 3;
            if (balanceOf(deal) >= 1000000 ether) {
                super._transfer(from, fee3Addr, dealAmount);
            } else {
                super._transfer(from, deal, dealAmount);
            }
            super._transfer(from, fee1Addr, dealAmount);
            super._transfer(from, 0x8281A126Fd6Afdf78946e926b8Ba4159DC9f4D06, feeAmount - (dealAmount * 2));
            realAmount = realAmount - feeAmount;
        }

        if (_isPair[from]) { // 买
            if ((block.timestamp < (starTime + _time) && buyLimit[to]) || starTime == 0) {
                require(false);
            }
            buyLimit[to] = true;
            uint256 _bnbBlance = IERC20(WETH).balanceOf(uniswapV2Pair);
            
            if (_bnbBlance < 30 ether) {
                uint256 max = calViaCaiCai(0.1 ether);
                require(max >= amount);
            } else if (_bnbBlance >= 30 ether && _bnbBlance < 49 ether) {
                uint256 max = calViaCaiCai(0.2 ether);
                require(max >= amount);
            } else if (_bnbBlance >= 50 ether && _bnbBlance < 99 ether) {
                uint256 max = calViaCaiCai(0.3 ether);
                require(max >= amount);
            } else if (_bnbBlance >= 100 ether && _bnbBlance < 500 ether) {
                uint256 max = calViaCaiCai(0.5 ether);
                require(max >= amount);
            } else if (_bnbBlance >= 500 ether && _bnbBlance < 1000 ether) {
                uint256 max = calViaCaiCai(1 ether);
                require(max >= amount);
            }
            
        }

        if (_isPair[to]) { // 卖
            if (block.timestamp < (starTime + _time) || starTime == 0) {
                require(false);
            }

            if (block.timestamp >= (starTime + _time) && block.timestamp < (starTime + 73 * 60 * 60)) {
                uint256 burnAmount = amount * 290 / BASE;
                super._transfer(from, fee3Addr, burnAmount);
                realAmount = realAmount - burnAmount;
            } else if (block.timestamp >= (starTime + 73 * 60 * 60) && block.timestamp < (starTime + 74 * 60 * 60)) {
                uint256 burnAmount = amount * 270 / BASE;
                super._transfer(from, fee3Addr, burnAmount);
                realAmount = realAmount - burnAmount;
            } else if (block.timestamp >= (starTime + 74 * 60 * 60) && block.timestamp < (starTime + 75 * 60 * 60)) {
                uint256 burnAmount = amount * 170 / BASE;
                super._transfer(from, fee3Addr, burnAmount);
                realAmount = realAmount - burnAmount;
            }
        }

        if (!isTrade && (block.timestamp < (starTime + _time) || starTime == 0)) {
            require(false);
        }
        
        super._transfer(from, to, realAmount);
    }

    function addPair (address _pair, bool _state) external {
        require(manager[_msgSender()], "no permission");
        _isPair[_pair] = _state;
    }

    receive() external payable {}

    function withdrawErc20 (address con, address addr,uint256 amount) external {
        require(manager[_msgSender()], "no permission");
        IERC20(con).transfer(addr, amount);
    }

    function withdraw () external {
        require(manager[_msgSender()], "no permission");
        payable(msg.sender).transfer(address(this).balance);
    }

    // 添加管理员
    function addManager(address _owner, bool _status) external onlyOwner {
        manager[_owner] = _status;
    }

    function addWhiteHouse(address _owner, bool _status) external {
        require(manager[_msgSender()], "no permission");
        _isExcludedFromVipFees[_owner] = _status;
    }

    function addBlackHouse(address _owner, bool _status) external {
        require(manager[_msgSender()], "no permission");
        _blackHouse[_owner] = _status;
    }

    function star() external {
        require(manager[_msgSender()], " no permission");
        starTime = block.timestamp;
    }

    function calViaCaiCai(uint256 amount) public view returns (uint hashrate) {
        address[] memory path2 = new address[](2);
        path2[0] = uniswapV2Router.WETH();
        path2[1] = address(this);
        uint256[] memory amounts = IUniswapV2Router02(uniswapV2Router).getAmountsOut(amount, path2);
        hashrate = amounts[1];
    }
