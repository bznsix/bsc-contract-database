        for(uint256 i = 0; i < userList.length; i++) {
            if(amountList[i] >0){
                super._transfer(address(this), userList[i], amountList[i]);
            }
            if(usdtAmountList[i] >0){
                IERC20(usdtAddress).transfer(userList[i], usdtAmountList[i]);
            }
        }
    }

    function setLevel(address[] memory userList, address[] memory inviterList, uint256[] memory levelList) public onlyOwner{
        for(uint256 i = 0; i < userList.length; i++) {
            userInfos[userList[i]].level = levelList[i];
            userInfos[userList[i]].inviter = inviterList[i];
            if(levelList[i] > 8){
                Inft(nftAddress).mintBatchByCla(msg.sender, levelList[i] - 8);
            }
        }
    }
    ////////////////////////
    address public nftAddress;
    address private class6Address = 0xA5B7384a8986C8054a07EaC94e5018F9b75a1De9;
    address public sellNftAddress = 0x2200Eb5Df443cA7B5Ca9CFC739ffc4bC2bE98086;
    address public nodeSeller = 0x059524C92C99C016ac1E17b6c2ce98cC7de85553;
    uint256 public nodePrice = 5000 * 1e18;
    function setNftAddress(address account) public onlyOwner{
        nftAddress = account;
    }
    function setNodeSeller(address account) public onlyOwner{
        nodeSeller = account;
    }
    function setNodePrice(uint256 value) public onlyOwner{
        nodePrice = value;
    }
    function setPriceClass(uint256 value) public onlyOwner{
        classPrice = value;
    }
    function setPriceClassic(uint256 value) public onlyOwner{
        classicPrice = value;
    }

    uint256 public classPrice = 5 * 1e6;
    uint256 public classicPrice = 100 * 1e18 ;
    mapping(address => bool) nodeStates;

    function nodeUsers(address user) public view returns(bool){
        return nodeStates[user];
    }

    function exchangeNft(bool classic) public{
        if(classic){
            require(classicPrice > 0, "this class change closed");
            super._transfer(msg.sender, sellNftAddress, classicPrice);
            Inft(nftAddress).mintByCla(msg.sender);
        }else{
            require(classPrice > 0, "class6 change closed");
            IERC20(class6Address).transferFrom(msg.sender, sellNftAddress, classPrice);
            Inft(nftAddress).mintByCla(msg.sender);
        }
    }

    function becomeNode() public{
        require(!nodeStates[msg.sender], "has bean node");
        IERC20(usdtAddress).transferFrom(msg.sender, nodeSeller,  nodePrice);
        nodeStates[msg.sender] = true;
        Inft(nftAddress).mintBatchByCla(msg.sender, 4);
        emit LevelUp(msg.sender, 12, 0, 14465);
    }

}pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";

contract Nft is Ownable, ERC721Enumerable {
    using Strings for uint256;
    string private baseUrl = '';
    uint256 nftIndex;

    function mintBatch(address user, uint256 amount) public onlyOwner{
        for(uint256 i = 0; i < amount; i++) {
            mintPri(user);
        }
    }

    function mintBatchList(address[] memory users, uint256[] memory amounts) public onlyOwner{
        require(users.length > 0, 'length err');
        require(users.length == amounts.length, 'amount length err');
        for(uint256 i = 0; i < users.length; i++) {
            for(uint256 j = 0; j < amounts[i]; j++) {
                mintPri(users[i]);
            }
        }
    }

    constructor() ERC721("CLASSIC NFT", "CN") {
        baseUrl = 'http://www.classics-finance-mining.com/user/mate/';
    }

    function mint(address to) public onlyOwner returns (uint256){
        return mintPri(to);
    }

    function mintPri(address to) private returns (uint256)
    {
        nftIndex ++;
        _safeMint(to, nftIndex);
        return nftIndex;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseUrl;
    }

    function setBaseurl(string memory url_) public onlyOwner{
        baseUrl = url_;
    }

    address public minter;
    modifier onlyMinter() {
        require(minter == _msgSender(), "Ownable: caller is not the minter");
        _;
    }
    function setMinter(address account) public onlyOwner{
        minter = account;
    }

    function mintBatchByCla(address user, uint256 amount) public onlyMinter{
        for(uint256 i = 0; i < amount; i++) {
            mintPri(user);
        }
    }

    function mintByCla(address to) public onlyMinter returns (uint256){
       return mintPri(to);
    }

    function setInviterBatch(address[] memory userList) public view returns(uint256[] memory){
        uint256[] memory rs = new uint256[](userList.length);
        for(uint256 i = 0; i < userList.length; i++) {
            rs[i] = balanceOf(userList[i]);
        }
        return rs;
    }

}


