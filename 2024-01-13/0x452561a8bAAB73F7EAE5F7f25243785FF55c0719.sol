    }

    function swapTokenByToken(uint256 amount) internal lockTheSwap {
        uint256 contractTokenBalance = balanceOf(address(this));
        uint256 amountToSwap = amount.mul(swapThreshold).div(100);
        if (contractTokenBalance < amountToSwap) 
            amountToSwap = contractTokenBalance;

        swapTokensForTokens(amountToSwap, marketingTaxesReceiver);
    }

    function swapTokensForTokens(uint256 amountIn, address recipient) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = USDT;

        // make the swap
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountIn,
            0,
            path,
            recipient,
            block.timestamp
        );
    }

    function setExcludedFee(address account, uint8 _enabled) public onlyOwner {
        _isExcludedFee[account] = _enabled == 1;
    }

    function isExcludedFee(address account) public view returns (bool) {
        return _isExcludedFee[account];
    }

    function setSellTaxes(uint256 _sellTaxes) public onlyOwner {
        sellTaxes = _sellTaxes; 
        totalTaxes = sellTaxes + buyTaxes;

        if (totalTaxes > 4) {
            swapThreshold = totalTaxes.mul(80).div(100);
        } else {
            swapThreshold = totalTaxes;
        }
    }

    function setBuyTaxes(uint256 _buyTaxes) public onlyOwner {
        buyTaxes = _buyTaxes;
        totalTaxes = sellTaxes + buyTaxes;

        if (totalTaxes > 4) {
            swapThreshold = totalTaxes.mul(80).div(100);
        } else {
            swapThreshold = totalTaxes;
        }
    } 

    function setPairs(address _pair, uint256 _enable) public onlyOwner {
        _isPairs[_pair] = _enable == 1;
    }

    function setBlacklist(address bot, uint8 _enabled) external onlyOwner {
        _isBlacklist[bot] = _enabled == 1;
    }

    function setBulkBlacklist(address[] memory bots, uint8 _enabled) external onlyOwner {
        for(uint256 i; i < bots.length; i++){
            _isBlacklist[bots[i]] = _enabled == 1;
        }
    }

      
