  function getMinBalanceForUpkeep(uint256 id) external view returns (uint96 minimumBalance);

  function addFunds(uint256 id, uint96 amount) external;
}

contract RandomNumberGenerator is AccessControl, VRFConsumerBaseV2, AutomationCompatibleInterface {
  event RequestSent(uint256 requestId, uint32 numWords);
  event RequestFulfilled(uint256 requestId, uint256[] randomWords);
  event NotEnoughAssetsToFundSubscription(uint256 linkNeeded);
  event UpKeepCreated(uint256 id);
  error TransferFailed();

  struct RequestStatus {
    bool requested;
    bool fulfilled;
    uint256[] randomWords;
  }

  mapping(uint256 => RequestStatus) public s_requests; /* requestId --> requestStatus */

  bytes32 public constant DDD_ROLE = keccak256("DDD_ROLE");

  uint256[] public requestIds;
  uint256 public lastRequestId;

  IAutomationRegistry chainLinkRegistry;
  address public chainLinkRegistrar;
  bytes32 public gasLaneKeyHash;
  uint32 public gasLaneMaxPrice;
  uint64 public chainlinkVrfId;
  uint256 public chainlinkUpkeepId;
  IVRFCoordinatorV2 public vrfCoordinator;
  AggregatorV3Interface internal linkPriceAggregator;
  LinkTokenInterface public link;

  bytes4 registerSig = KeeperRegistrarInterface.register.selector;

  uint32 public callbackGasLimit = 2500000; // bump this up if callback is failing
  uint32 public maxVerificationGas = 200000; // not actually used in request random words
  uint16 public requestConfirmations = 3;
  uint32 public numWords = 5;

  uint256 public constant eth = 1 ether;

  IUniswapV2Router02 private constant router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
  ILinkSwap private constant linkSwapper = ILinkSwap(0x1FCc3B22955e76Ca48bF025f1A6993685975Bb9e);
  IERC20Upgradeable private constant pegLink = IERC20Upgradeable(0xF8A0BF9cF54Bb92F17374d9e9A321E6a111a51bD);
  IPriceReader public PriceReader = IPriceReader(0x64A4C814311bd6AcDc589aA907b19920404B9A0D);
  IERC20Upgradeable private constant BUSD = IERC20Upgradeable(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
  IERC20Upgradeable private constant DRIP = IERC20Upgradeable(0x20f663CEa80FaCE82ACDFA3aAE6862d246cE0333);
  AggregatorV3Interface internal busdBnbPriceFeed = AggregatorV3Interface(0x87Ea38c9F24264Ec1Fff41B04ec94a97Caf99941);

  IDripDropDraw public dripDropDraw;

  constructor(
    address _LinkAddress,
    address _VrfCoordinatorAddress,
    uint64 _SubscriptionId,
    address _AggregatorAddress,
    address _chainLinkRegistry,
    address _chainLinkRegistrar,
    bytes32 _gasLaneKeyHash,
    uint32 _gasLaneMaxPrice
  ) VRFConsumerBaseV2(_VrfCoordinatorAddress) {
    _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
    _grantRole(DDD_ROLE, _msgSender());
    chainlinkVrfId = _SubscriptionId;
    link = LinkTokenInterface(_LinkAddress);
    vrfCoordinator = IVRFCoordinatorV2(_VrfCoordinatorAddress);
    linkPriceAggregator = AggregatorV3Interface(_AggregatorAddress);
    chainLinkRegistry = IAutomationRegistry(_chainLinkRegistry);
    chainLinkRegistrar = _chainLinkRegistrar;
    gasLaneKeyHash = _gasLaneKeyHash;
    gasLaneMaxPrice = _gasLaneMaxPrice;
  }

  modifier onlyAdmin() {
    _checkRole(DEFAULT_ADMIN_ROLE);
    _;
  }

  modifier onlyDripDropDraw() {
    _checkRole(DDD_ROLE);
    _;
  }

  // note to receive BNB from DDD marketing tax mechanic
  receive() external payable {}

  function getVRFRequestFee() internal view returns (uint256 fee) {
    uint256 gasCost = tx.gasprice * maxVerificationGas;
    (, int256 linkPriceInBnb, , , ) = linkPriceAggregator.latestRoundData();
    (uint32 linkPremiumInMillions, , , , , , , , ) = vrfCoordinator.getFeeConfig();
    uint256 linkPremium = uint256(linkPremiumInMillions) * 1e12;
    uint256 premium = (linkPremium * uint256(linkPriceInBnb)) / eth;

    fee = gasCost + premium;
  }

  function requestRandomWords() external onlyDripDropDraw returns (uint256 requestId) {
    // TODO toggle for testnets
    _checkVRFSubscriptionBalance();
    _checkUpkeepSubscriptionBalance();

    requestId = VRFCoordinatorV2Interface(address(vrfCoordinator)).requestRandomWords(
      gasLaneKeyHash,
      chainlinkVrfId,
      requestConfirmations,
      callbackGasLimit,
      numWords
    );
    s_requests[requestId] = RequestStatus({ requested: true, randomWords: new uint256[](0), fulfilled: false });
    requestIds.push(requestId);
    lastRequestId = requestId;
    emit RequestSent(requestId, numWords);
    return requestId;
  }

  function adminFulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) external onlyAdmin {
    dripDropDraw.payoutWinners(_randomWords);
    emit RequestFulfilled(_requestId, _randomWords);
  }

  function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) internal override {
    require(s_requests[_requestId].requested, "request not found");
    s_requests[_requestId].fulfilled = true;
    s_requests[_requestId].randomWords = _randomWords;
    dripDropDraw.payoutWinners(_randomWords);
    emit RequestFulfilled(_requestId, _randomWords);
  }

  // *** ADMIN FUNCTIONS ***
  function setVRFId(uint64 _vrfId) public onlyAdmin {
    chainlinkVrfId = _vrfId;
  }

  function setUpkeepId(uint256 _upkeepId) public onlyAdmin {
    chainlinkUpkeepId = _upkeepId;
  }

  function setCallbackGasLimit(uint32 _callbackGasLimit) external onlyAdmin {
    callbackGasLimit = _callbackGasLimit;
  }

  function setMaxVerificationGas(uint32 _maxVerificationGas) external onlyAdmin {
    maxVerificationGas = _maxVerificationGas;
  }

  function setGasLaneMaxPrice(uint32 _gasLaneMaxPrice) external onlyAdmin {
    gasLaneMaxPrice = _gasLaneMaxPrice;
  }

  function setGasLaneKeyHash(bytes32 _gasLaneKeyHash) external onlyAdmin {
    gasLaneKeyHash = _gasLaneKeyHash;
  }

  function setRequestConfirmations(uint16 _requestConfirmations) external onlyAdmin {
    requestConfirmations = _requestConfirmations;
  }

  function setNumWords(uint32 _numWords) external onlyAdmin {
    numWords = _numWords;
  }

  function setDripDropDraw(address _dripDropDraw) external onlyAdmin {
    _grantRole(DDD_ROLE, _dripDropDraw);
    dripDropDraw = IDripDropDraw(_dripDropDraw);
  }

  // *** GETTERS ***
  function getRequestStatus(uint256 _requestId) external view returns (bool fulfilled, uint256[] memory randomWords) {
    require(s_requests[_requestId].requested, "request not found");
    RequestStatus memory request = s_requests[_requestId];
    return (request.fulfilled, request.randomWords);
  }

  function checkUpkeep(bytes calldata) external view override returns (bool upkeepNeeded, bytes memory) {
    upkeepNeeded = dripDropDraw.getRoundTimeRemaining() == 0 && dripDropDraw.currentRound() > 0;
  }

  function performUpkeep(bytes calldata) external override {
    if (dripDropDraw.getRoundTimeRemaining() == 0 && dripDropDraw.currentRound() > 0) {
      dripDropDraw.requestRandomWords();
    }
  }

  function _estimateMinimumLinkNeededForVRFSubscription() internal view returns (uint256 linkNeeded) {
    uint256 estimatedMaxGas = gasLaneMaxPrice * (maxVerificationGas + callbackGasLimit);
    uint256 estimatedMax = estimatedMaxGas * 1e9; // ~0.15BNB
    (, int256 linkPriceInBnb, , , ) = linkPriceAggregator.latestRoundData();
    linkNeeded = (estimatedMax * eth) / (uint256(linkPriceInBnb));
    (uint32 linkPremiumInMillions, , , , , , , , ) = vrfCoordinator.getFeeConfig();
    uint256 linkPremium = uint256(linkPremiumInMillions) * 1e12;
    linkNeeded += linkPremium;
  }

  /**
   * @dev Estimates Link required for VRF subscription and funds it if it's low
   */
  // note, recommended at 2 times the minimum buffer
  function _checkVRFSubscriptionBalance() internal {
    if (chainlinkVrfId != 0) {
      uint256 minimumSubscriptionBalance = _estimateMinimumLinkNeededForVRFSubscription();
      // console.log("VRF SUBCRIPTION MIN: ", minimumSubscriptionBalance / 1 ether);
      (uint256 subscriptionBalance, , , ) = vrfCoordinator.getSubscription(chainlinkVrfId);
      uint256 minimumSubscriptionBalanceWithBuffer = minimumSubscriptionBalance * 2;
      // console.log("VRF SUBSCRIPTION MIN WITH BUFFER: ", minimumSubscriptionBalanceWithBuffer / 1 ether);
      if (subscriptionBalance < minimumSubscriptionBalanceWithBuffer) {
        uint256 balanceNeededLink = minimumSubscriptionBalanceWithBuffer - subscriptionBalance;
        _getPegLinkForSubscription(balanceNeededLink, true);
      }
    }
  }

  // note, recommended at 5 times the minimum buffer
  function _checkUpkeepSubscriptionBalance() internal {
    if (chainlinkUpkeepId != 0) {
      (, , , uint96 subscriptionBalance, , , , ) = chainLinkRegistry.getUpkeep(chainlinkUpkeepId);
      uint96 minimumSubscriptionBalance = chainLinkRegistry.getMinBalanceForUpkeep(chainlinkUpkeepId);
      // console.log("UPKEEP SUBCRIPTION MIN: ", minimumSubscriptionBalance / 1 ether);
      uint96 minimumSubscriptionBalanceWithBuffer = minimumSubscriptionBalance * 5;
      // console.log("UPKEEP SUBSCRIPTION MIN WITH BUFFER: ", minimumSubscriptionBalanceWithBuffer / 1 ether);
      if (subscriptionBalance < minimumSubscriptionBalanceWithBuffer) {
        uint256 balanceNeededLink = minimumSubscriptionBalanceWithBuffer - subscriptionBalance;
        _getPegLinkForSubscription(balanceNeededLink, false);
      }
    }
  }

  function _getPegLinkForSubscription(uint256 balanceNeededLink, bool isVRF) internal {
    if (balanceNeededLink / 1 ether > 0) {
      (, int256 linkPriceInBnb, , , ) = linkPriceAggregator.latestRoundData();
      uint256 bnbNeeded = (((balanceNeededLink * uint256(linkPriceInBnb)) / eth));
      (, int256 busdPriceInBnb, , , ) = busdBnbPriceFeed.latestRoundData();
      uint256 busdNeeded = ((bnbNeeded * eth) / uint256(busdPriceInBnb));
      uint256 dripPriceInBusd = PriceReader.getDripPcsPrice();
      uint256 dripNeeded = (busdNeeded * eth) / dripPriceInBusd;
      uint256 dripBalance = DRIP.balanceOf(address(this));
      if (dripBalance > dripNeeded) {
        uint256 pegLinkAmount = _getPegLinkWithDrip(dripNeeded);
        _swapPegLinkForLinkAndFundSubscription(isVRF, pegLinkAmount);
      } else if ((address(this).balance) > bnbNeeded) {
        uint256 pegLinkAmount = _getPegLinkWithBnb(bnbNeeded);
        _swapPegLinkForLinkAndFundSubscription(isVRF, pegLinkAmount);
      } else if (BUSD.balanceOf(address(this)) > busdNeeded) {
        uint256 pegLinkAmount = _getPegLinkWithBusd(busdNeeded);
        _swapPegLinkForLinkAndFundSubscription(isVRF, pegLinkAmount);
      } else if (link.balanceOf(address(this)) > balanceNeededLink) {
        _fundSubscriptionWithLink(balanceNeededLink, isVRF);
      } else {
        emit NotEnoughAssetsToFundSubscription(balanceNeededLink);
      }
    }
  }

  function _getPegLinkWithDrip(uint256 amount) internal returns (uint256 amountOut) {
    // console.log("swapping drip for peglink: ", amount / 1 ether);
    uint8 pathLength = 4;
    uint256 deadline = block.timestamp + 2 minutes;
    address[] memory path = new address[](pathLength);
    path[0] = address(DRIP);
    path[1] = address(BUSD);
    path[2] = router.WETH();
    path[3] = address(pegLink);
    uint256[] memory amountsOut = router.getAmountsOut(amount, path);
    uint256 amountOutMin = amountsOut[pathLength - 1];
    DRIP.approve(address(router), amount);
    uint256 pegLinkBalanceBefore = pegLink.balanceOf(address(this));
    router.swapExactTokensForTokensSupportingFeeOnTransferTokens(amount, amountOutMin, path, address(this), deadline);
    delete path;
    uint256 pegLinkBalanceAfter = pegLink.balanceOf(address(this));
    amountOut = pegLinkBalanceAfter - pegLinkBalanceBefore;
  }

  function _getPegLinkWithBnb(uint256 amount) internal returns (uint256 amountOut) {
    // console.log("swapping bnb for peglink: ", amount);
    uint8 pathLength = 2;
    uint256 deadline = block.timestamp + 2 minutes;
    address[] memory path = new address[](pathLength);
    path[0] = router.WETH();
    path[1] = address(pegLink);
    uint256[] memory amountsOut = router.getAmountsOut(amount, path);
    uint256 amountOutMin = (amountsOut[pathLength - 1]);
    uint256[] memory amountsOutTransfer = router.swapExactETHForTokens{ value: amount }(
      amountOutMin,
      path,
      address(this),
      deadline
    );
    delete path;
    amountOut = amountsOutTransfer[pathLength - 1];
  }

  function _getPegLinkWithBusd(uint256 amount) internal returns (uint256 amountOut) {
    // console.log("swapping busd for peglink: ", amount / 1 ether);
    uint8 pathLength = 3;
    uint256 deadline = block.timestamp + 2 minutes;
    address[] memory path = new address[](pathLength);
    path[0] = address(BUSD);
    path[1] = router.WETH();
    path[2] = address(pegLink);
    uint256[] memory amountsOut = router.getAmountsOut(amount, path);
    uint256 amountOutMin = (amountsOut[pathLength - 1]);
    BUSD.approve(address(router), amount);
    uint256[] memory amountsOutTransfer = router.swapExactTokensForTokens(
      amount,
      amountOutMin,
      path,
      address(this),
      deadline
    );
    delete path;
    amountOut = amountsOutTransfer[pathLength - 1];
  }

  function _fundSubscriptionWithLink(uint256 amount, bool vrf) internal {
    require(link.balanceOf(address(this)) >= amount, "not enough LINK to fund subscription!");
    // console.log("funding %s with %s link", vrf ? "VRF" : "UPKEEP", Strings.toString(amount / 1 ether));
    uint256 id;
    address manager;
    if (vrf) {
      manager = address(vrfCoordinator);
      id = chainlinkVrfId;
    } else {
      manager = address(chainLinkRegistry);
      id = chainlinkUpkeepId;
    }
    link.transferAndCall(manager, amount, abi.encode(id));
  }

  function _swapPegLinkForLinkAndFundSubscription(bool vrf, uint256 amount) internal {
    uint256 linkBalanceBefore = link.balanceOf(address(this));
    pegLink.approve(address(linkSwapper), amount);
    linkSwapper.swap(amount, address(pegLink), address(link));
    uint256 linkBalanceAfter = link.balanceOf(address(this));
    _fundSubscriptionWithLink(linkBalanceAfter - linkBalanceBefore, vrf);
  }

  function _createUpkeep(string memory name, bytes calldata encryptedEmail, uint256 amount) internal {
    require(link.balanceOf(address(this)) >= amount, "not enough LINK to create upkeep!");

    bytes memory payload = abi.encode(
      name,
      encryptedEmail,
      address(this),
      2500000, // https://docs.chain.link/chainlink-automation/supported-networks#bnb-chain
      address(msg.sender),
      "",
      amount, // amount of link
      55, // random identifier
      address(this)
    );

    (State memory state, , ) = chainLinkRegistry.getState();
    uint256 oldNonce = state.nonce;
    link.transferAndCall(chainLinkRegistrar, amount, bytes.concat(registerSig, payload));
    (state, , ) = chainLinkRegistry.getState();
    uint256 newNonce = state.nonce;
    if (newNonce == oldNonce + 1) {
      uint256 newUpkeepId = uint256(
        keccak256(abi.encodePacked(blockhash(block.number - 1), address(chainLinkRegistry), uint32(oldNonce)))
      );
      setUpkeepId(newUpkeepId);
      emit UpKeepCreated(newUpkeepId);
    }
  }

  function createUpkeepFromLink(string memory name, bytes calldata encryptedEmail, uint256 amount) external onlyAdmin {
    _createUpkeep(name, encryptedEmail, amount);
  }

  function createUpkeepFromBnb(string memory name, bytes calldata encryptedEmail, uint256 _amount) external onlyAdmin {
    require(address(this).balance >= _amount, "not enough BNB to create upkeep!");
    uint256 pegLinkAmount = _getPegLinkWithBnb(_amount);
    pegLink.approve(address(linkSwapper), pegLinkAmount);
    uint256 linkBalanceBefore = link.balanceOf(address(this));
    linkSwapper.swap(pegLinkAmount, address(pegLink), address(link));
    uint256 linkBalanceAfter = link.balanceOf(address(this));
    _createUpkeep(name, encryptedEmail, linkBalanceAfter - linkBalanceBefore);
  }

  function withdraw(address _tokenAddress, uint256 _amount) external onlyAdmin {
    uint256 contractBalance;
    if (_tokenAddress == address(0)) {
      contractBalance = address(this).balance;
