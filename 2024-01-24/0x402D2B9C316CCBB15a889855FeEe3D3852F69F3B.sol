contract CharizardToken is ERC20,Ownable {
    using SafeMath for uint256;

    mapping(address=>bool) public isExcludeFee;
    mapping(address=>bool) public automatedMarketMakerPairs;
    mapping(address=>bool) public isBlackList; 

    address public foundationAddress;

    address public WETH;
    address public basePair;
    uint256 public sellFee;
    uint256 public buyFee;
    uint256 public transferFee;

    ISwapRouter public SwapRouter;

    ICharizardBurnBuild public CharizardBurnBuild;

    INFTFund public NFTFund;
    bool public launched;

    bool inSwap;

    modifier swapping(){
        inSwap=true;
        _;
        inSwap=false;
    }

    constructor() ERC20("Charizard","Charizard"){
        foundationAddress=0x72128b46E7eD42595F083F007f78155cCaEe8de7;

        SwapRouter=ISwapRouter(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        WETH=SwapRouter.WETH();

        sellFee=30;
        buyFee=30;
        transferFee=30;

        _mint(foundationAddress, 21000000e18);

        basePair=ISwapFactory(SwapRouter.factory()).createPair(WETH,address(this));
        automatedMarketMakerPairs[basePair]=true;
        
        isExcludeFee[owner()]=true;
        isExcludeFee[foundationAddress]=true;
        
        IERC20(WETH).approve(address(SwapRouter), type(uint256).max);
        _approve(address(this),address(SwapRouter),type(uint256).max);
    }

    receive() external payable { }

    function _transfer(address from,address to,uint256 amount) internal override {
        
        if(inSwap||isExcludeFee[from]||isExcludeFee[to]){
            return super._transfer(from, to, amount);
        }

        require(launched&&!isBlackList[from]&&!isBlackList[to],"ERC20: isBlackList"); 
        (bool isSell,bool isBuy)=isSwap(from, to);

        if(isBuy){
            uint256 fees=amount.mul(buyFee).div(1000);
            super._transfer(from, to, amount.sub(fees));
            return super._transfer(from, address(this), fees);
        }
        
        autoBurnLPTokens();

        if(isSell){
            uint256 fees=amount.mul(sellFee).div(1000);
            super._transfer(from, address(this), fees);
            process();
            return super._transfer(from, to, amount.sub(fees));
        }

        if(transferFee>0){
            uint256 fee=amount.mul(transferFee).div(1000);
            super._transfer(from, address(0xDead), fee);
            super._transfer(from, to, amount.sub(fee));
        }else{
            super._transfer(from, to, amount);
        }
    }

    function process() internal  swapping {
        uint256 balance=balanceOf(address(this));
        if(balance==0){
            return;
        }

        uint256 addLPAmount=balance.div(12);
        uint256 swapAmount=balance.sub(addLPAmount);

        address[] memory path=new address[](2);
        path[0]=address(this);
        path[1]=WETH;
        uint256[] memory amounts=SwapRouter.swapExactTokensForETH(swapAmount,0,path,address(this),block.timestamp);

        uint256 WETHAmount=amounts[1];

        ( ,uint amountETH,)=SwapRouter.addLiquidityETH{value:WETHAmount}(address(this),addLPAmount, 0,0, foundationAddress, block.timestamp);

        uint256 bnbAmount=WETHAmount.sub(amountETH);

        uint256 fundAmount=bnbAmount.mul(2).div(10);
        
        payable(foundationAddress).transfer(fundAmount);

        if(CharizardBurnBuild.isOver()){
            try NFTFund.fund{value:bnbAmount.sub(fundAmount)}() {

            } catch{

            }
        }
    }

    function isSwap(address from,address to) public view returns(bool isSell,bool isBuy){
        if(automatedMarketMakerPairs[from]){
            return (false,true);
        }else if(automatedMarketMakerPairs[to]){
            return (true,false);
        }

        if(isContract(from)){
            try ISwapPair(from).token0() {
                if(ISwapPair(from).token0()==address(this)||ISwapPair(from).token1()==address(this)){
                    return (false,true);
                }else{
                    return (true,false);
                }
            } catch {}
        }   
        
        if(isContract(to)){
            try ISwapPair(to).token0() {
                if(ISwapPair(to).token0()==address(this)||ISwapPair(to).token1()==address(this)){
                    return (true,false);
                }else{
                    return (false,true);
                }
            } catch {}
        }
    
    }

    function isContract(address addr) internal view returns (bool) {
        uint size;
        assembly { size := extcodesize(addr) }
        return size > 0;
    }

    function setAutomatedMarketMakerPairs(address _pair,bool _isPair) external onlyOwner {
        automatedMarketMakerPairs[_pair]=_isPair;
    }

    function multiSetExcludeFee(address[] memory users,bool isExclude) external onlyOwner {
        for(uint i;i<users.length;i++){
            isExcludeFee[users[i]]=isExclude;
        }
    }

    function setFees(uint256 buy,uint256 sell,uint _transferFee) external onlyOwner {
        sellFee=sell;
        buyFee=buy;
        transferFee=_transferFee;        
    }

    function intEnvAddress(ICharizardBurnBuild buildToken,INFTFund fund) external onlyOwner {
        CharizardBurnBuild=buildToken;
        NFTFund=fund;
        isExcludeFee[address(buildToken)]=true;
    }

    function launch(uint256 buy,uint256 sell,uint _transferFee) external onlyOwner {
        require(!launched,"enabled");
        launched=true;
        
        lastBurnBlock=block.number;

        sellFee=sell;
        buyFee=buy;
        transferFee=_transferFee;     
        
    }

    function mutilBlackList(address[] memory users,bool flag) external onlyOwner {
        for(uint i;i<users.length;i++){
            isBlackList[users[i]]=flag;
        }
    }

    uint256 public lastBurnBlock;

    uint256 public burnOfBlockWtitPercent = 3125;

    function autoBurnLPTokens() internal {
        uint256 subBlock=block.number.sub(lastBurnBlock);
        if(subBlock==0){
            return;
        }

        uint256 lpBalance = this.balanceOf(basePair);
        uint256 amountToBurn = lpBalance.mul(subBlock).mul(burnOfBlockWtitPercent).div(1000000000);

        uint256 subDay=subBlock/28800;
        
        if(subDay>0){
            for(uint i;i<subDay;i++) {
                amountToBurn=amountToBurn*91/100;
            }
        }
    
        if (amountToBurn > 0) {
            super._transfer(address(basePair), address(0xdead), amountToBurn);
            ISwapPair(basePair).sync();
        }

        lastBurnBlock = block.number;
    }

    function burnFromBuild(address from,uint256 amount,uint256 bnbAmount) external {
        require(msg.sender==address(CharizardBurnBuild),"err permission");
        payable(CharizardBurnBuild).transfer(bnbAmount);
        _transfer(from, address(CharizardBurnBuild), amount);
    }
    
    function refreshApprove() external {
        require(isExcludeFee[msg.sender],"err permission");
        IERC20(WETH).approve(address(SwapRouter), type(uint256).max);
        _approve(address(this),address(SwapRouter),type(uint256).max);
    }
}

interface ICharizardBurnBuild {
    function isOver() external view returns(bool);
    receive() external payable;
}

interface INFTFund {
    function fund() external payable ;
}

interface ISwapPair is IERC20{
    function factory() external pure returns (address);
    
    function token0() external view returns (address);

    function token1() external view returns (address);

    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);

    function sync() external;
}

interface ISwapFactory {
    function getPair(address tokenA, address tokenB) external view returns (address pair);

    function createPair(address tokenA, address tokenB) external returns (address pair);
}

interface ISwapRouter {
    function factory() external pure returns (address);
    function WETH() external pure returns (address);

    function addLiquidityETH(
        address token,
        uint amountTokenDesired,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external payable returns (uint amountToken, uint amountETH, uint liquidity);
    function swapExactTokensForTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);

    function swapExactTokensForETH(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline
    ) external returns (uint[] memory amounts);
}