contract PixoNft is ERC1155, Ownable, ERC1155Supply, MintingStore, ERC1155URIStorage {
    constructor() {
        _setBaseURI("www.pixo.com");
    }

    function getNftPriceInCurrentRound() external view returns (uint256) {
        return getPixoPrice(fractionalPrice[currentSaleId]);
    }

    function getFractionalPrice(uint256 saleId) external view returns (uint256) {
        return fractionalPrice[saleId];
    }

    function getCurrentSaleCount(uint256 saleId) external view returns (uint256) {
        return saleCount[saleId];
    }

    function getSaleFractionCount() public view returns(uint256) {
        if(currentSaleId >= 1 && currentSaleId <= 10) {
            return 1000;
        } else if(currentSaleId >= 11 && currentSaleId <= 30) {
            return 2000;
        } else if(currentSaleId >= 31 && currentSaleId <= 55) {
            return 4000;
        } else {
            return 0;
        }
    }

    function setBaseURI(string memory newuri) public onlyOwner {
        _setBaseURI(newuri);
    }

    function setTokenURI(uint256 tokenId, string memory tokenURI) public onlyOwner {
        _setURI(tokenId,tokenURI);
    }

    function setFractionPrice(uint256[55] calldata newFractionPrice) public onlyOwner {
        for(uint8 i=0;i<55;i++) {
            fractionalPrice[i+1] = newFractionPrice[i];
        }
    }

    function setTokenURI(uint256[] memory tokenId, string[] memory tokenURI) public onlyOwner {
        for(uint256 id=0;id<tokenId.length;id++) {
            _setURI(tokenId[id],tokenURI[id]);
        }
    }

    function mint(address referral,uint256 id, uint256 supply)
        public
    {
        address account = _msgSender();
        uint256 currentSaleCount = saleCount[currentSaleId] + supply;
        uint256 getTotalFractionalCount = getSaleFractionCount();
        require(referral != account, "PixoNft: Referral address not be a sender");
        require(((totalSupply(id) - _swapedTokenCount[id]) + supply) <= _nftMaxSupply[id],  "PixoNft: Supply is already minted");
        require(currentSaleCount <= getTotalFractionalCount, "PixoNft: Exceed Sale Rount Limit");

        uint256 nftPriceInUsd = supply * fractionalPrice[currentSaleId];
        uint256 nftPriceInPixo = getPixoPrice(nftPriceInUsd);
        
        saleCount[currentSaleId] = currentSaleCount;

        if((referral != address(0)) && (referralShare != 0)) {
            uint256 refFee = nftPriceInPixo * referralShare / 1e4;
            nftPriceInPixo = nftPriceInPixo - refFee;
            pixo.transferFrom(account, referral, refFee);
        }

        if(burnFee != 0) {
            uint256 burnPixo = nftPriceInPixo * burnFee / 1e4;
            nftPriceInPixo = nftPriceInPixo - burnPixo;
            pixo.transferFrom(account, burnAddress, burnPixo);
        }

        pixo.transferFrom(account, owner(), nftPriceInPixo);
        _mint(account, id, supply, "0x");

        if(currentSaleCount == getTotalFractionalCount) {
            currentSaleId++;
        }
        _referral[account] = referral;
    }

    function swap(address account,uint256 id,uint256 supply) external  {
        require(msg.sender == pixoNftSwap);
        _mint(account, id, supply, "0x");
        _swapedTokenCount[id] += supply;
    }

    function uri(uint256 tokenId) 
        public 
        view 
        override(ERC1155,ERC1155URIStorage) returns (string memory) {
        return super.uri(tokenId);
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
        internal
        override(ERC1155, ERC1155Supply)
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
}